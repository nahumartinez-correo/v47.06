/****o* Pendientes de agrupación/OFB:op:adt_op_refresh
* NOMBRE
*   OFB:op:adt_op_refresh
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "adtutlmisc.h"

SUB op:adt_op_refresh
    REFRESH op:adt_login_id
    REFRESH op:adt_branch_id
    REFRESH op:adt_operator_id
    REFRESH op:adt_operator_siaf_id
    REFRESH op:adt_password
    REFRESH op:adt_node_id
    REFRESH op:adt_aux_prt_id
    REFRESH op:adt_product
    REFRESH op:adt_title
    REFRESH op:adt_fecha_com_op
    REFRESH op:adt_fecha_fin_op
    REFRESH op:adt_nro_legajo
    REFRESH op:adt_tipo_doc
    REFRESH op:adt_nro_doc
    REFRESH op:adt_inhibido
    //REFRESH op:adt_per_password
    //REFRESH op:adt_fecha_password
    REFRESH op:adt_sup_auth
    REFRESH op:adt_users_auth
    REFRESH op:adt_tables_auth
    REFRESH op:adt_tlr_auth
    REFRESH op:adt_cl_auth
    REFRESH op:adt_dds_auth
    REFRESH op:adt_na_auth
    REFRESH op:adt_sup_auth2
    REFRESH op:adt_con_auth
    REFRESH op:adt_tes_auth
    REFRESH op:adt_tesforz_auth
    REFRESH op:adt_ret_auth
    REFRESH op:adt_sist_auth
    REFRESH op:adt_hotline_auth
    REFRESH op:adt_host_mode
    REFRESH op:adt_cash_min
    REFRESH op:adt_cash_max
    REFRESH op:adt_cash_min_dol
    REFRESH op:adt_cash_max_dol
    REFRESH op:adt_check_cash_max
    REFRESH op:adt_start_sk_menu
    REFRESH op:adt_session_num
    REFRESH op:adt_reentry_aut /*REENTRY AUTOMATICO */
    REFRESH op:adt_tesoro
    REFRESH op:adt_tesoro_id
    refresh op:adt_llave_tesoro
    refresh op:adt_cargo
    refresh op:adt_audit_auth
	/*Se agrega perfil de autoimpresion - Gabriel Falciola*/
    REFRESH op:adt_autoimpre_auth
END SUB

