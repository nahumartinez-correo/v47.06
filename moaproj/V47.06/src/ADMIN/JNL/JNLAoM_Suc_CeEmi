//mercedes - journalizacion Alta o Modif. sucursales

SUB admin:JNLAoM_Suc_CeEmi
    MOVE "Suc_CeEmi" TO tabla
	CDS APPEND DSC:tlr:CONjnl_write op:op_login_id as op:adt_login_id admin:tabla
	
	CALL csr:proc_list(F_APPEND,DSC:tlr:CONjnl_write,LST:LISTsuc_CeEmi_flds,0)
	CALL csr:proc_list(F_APPEND,DSC:tlr:CONjnl_write,LST:LISTbck_Suc_CeEmi,0)
END SUB

