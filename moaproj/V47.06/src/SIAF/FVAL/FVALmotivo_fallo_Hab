#include "siafFVAL.h"

SUB siaf:FVALmotivo_fallo_Hab
    IF (siaf:motivo.numchars > 0) THEN
    	call siaf:MISCMotivos_6850_Hab
    	RETURN (RETVAL)
    ELSE
    	RETURN (-2)
    END IF
END SUB

