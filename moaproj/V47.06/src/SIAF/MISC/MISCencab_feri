/****o* Pendientes de agrupación/OFB:siaf:MISCencab_feri
* NOMBRE
*   OFB:siaf:MISCencab_feri
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "siafMISC.h"

SUB siaf:MISCencab_feri
    local loc_spaces	like siaf:i

    /* sba 20031222 - No es mas Correo Argentino
    FPRINT common:csr_string,"                             CORREO ARGENTINO"
    */
    // sba 20031222 - Ahora el nombre de la empresa esta en la variable common:emp_desc
    let loc_spaces = 40 - (common:emp_desc.NUMCHARS / 2) 
    fprint common:csr_string,"%*.*s%s" loc_spaces,loc_spaces," ",common:emp_desc

    CALL siaf:MISCwrite_lis
    common:csr_string = "   "
    CALL siaf:MISCwrite_lis
    FPRINT common:csr_string,"                                  FERIADOS"
    CALL siaf:MISCwrite_lis
    common:csr_string = "   "
    CALL siaf:MISCwrite_lis
    common:csr_string = "   "
    CALL siaf:MISCwrite_lis
    FPRINT common:csr_string, \
"_______________________________________________________________________________________________________ "    
    CALL siaf:MISCwrite_lis
    FPRINT common:csr_string, \
"TIPO DE FERIADO  PROVINCIA  OP/BANC  SUCURSAL     FECHA      DESCRIPCION"
    CALL siaf:MISCwrite_lis
    FPRINT common:csr_string, \
"_______________________________________________________________________________________________________ "      
    CALL siaf:MISCwrite_lis
    common:csr_string = "   "
    CALL siaf:MISCwrite_lis
END SUB

/*******/
