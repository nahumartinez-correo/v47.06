#include "siafMISC.h"

SUB siaf:MISCdisable_chkMot
/*MFM - Deshabilita los checkbox de tipos de clientes*/
	DISABLECONTROL op:CHK9
	DISABLECONTROL op:CHK10
    DISABLECONTROL op:CHK11
    DISABLECONTROL op:CHK12
    DISABLECONTROL op:CHK13
    DISABLECONTROL op:CHK14
    PROTFIELD adt_mot_bonifCM     
END SUB

