#include "siafSVAL.h"

SUB siaf:SVALAfip_General
    IF (siaf:form_grupo == 40) THEN
        CALL siaf:MISCvalida_Afip_Gral //validaciones
        IF (RETVAL < 0) THEN
    	    RETURN (-2)
    	END IF    
    END IF
END SUB

