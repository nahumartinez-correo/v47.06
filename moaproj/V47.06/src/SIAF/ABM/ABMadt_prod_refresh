    REFRESH adt_prod_moneda
    REFRESH adt_prod_pla_min
    REFRESH adt_prod_pla_max
    REFRESH adt_prod_tem_min
    REFRESH adt_prod_tem_max
    REFRESH adt_prod_tna_min
    REFRESH adt_prod_tna_max
    REFRESH adt_prod_monto_min
    REFRESH adt_prod_monto_max
    REFRESH adt_prod_dias_atras
    REFRESH adt_prod_capitaliza
    REFRESH adt_prod_renueva
    REFRESH adt_prod_transfer
    END SUB

