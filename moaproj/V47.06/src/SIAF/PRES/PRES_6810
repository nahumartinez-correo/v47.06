/****o* Pendientes de agrupación/OFB:siaf:PRES_6810
* NOMBRE
*   OFB:siaf:PRES_6810
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "siafPRES.h"

SUB siaf:PRES_6810
    nombre_transac.HIGHLITE = 0
    codigo_siaf = 6810
    restr_otra_casa = "N"
    linea_validacion = "S"
    exhibe_pantalla = "S"
    imprime_comprobante = "D"
    
    /*Reingenieria.MFM - borro el campo de descuento a cliente Medio por si quedò sucio*/
    if (post:estado_presu == 2) then
    	clearfield post:Dcto_CliMed
    	clearfield post:Imp_DctoCliMed
    endif
    /*MFM - Borro de memoria los datos del picklist*/
    CALL csr:f_sadroppl "MOTIVOS HAB"
    
    /*MFM - Genero picklist dinámico con motivos habilitados*/
    ;CALL MISCMotivos_Hab(6810)

    INDhost_snd_fv = drv:INDspc_rtn[6]
     /* El importe se va a ingresar en el campo impoerte2, luego se calculará
        la sobretasa de iva y se incrementará en importe.
	Por eso el campo queda protegido */
    protfield post:SobreTasaIva
    protfield importe
    /* */
    CALL PRESfecha_hora
    IF (RETVAL < 0) THEN
      RETURN (-1)
    END IF

    /*MLD: Armo el picklist acá porque ya tengo el tipo de cliente*/
   if (!testflag(tlr:tran_flags,correction))
     CALL MISCMotivos_Hab(6810)
   end if

   ;MOVE "INGRESOS VARIOS" TO DAUtitulo1
    MOVE siaf:nombre_transac TO DAUtitulo1

    post:INDPresuD_Dat = OFB:CDSTRAN6600_append
    CALL post:MISCLeo_PresDet

   	/*Limpio el campo que contiene el Grupo de IVA del cliente, ya que se saca de la PresuAct
	y si no hay una Sesión debe estar vacío - Gabriel Falciola*/
	if (post:estado_presu == 1)
		clearfield post:PresGrupoIva
	end if

	;if (post:cli_tipo == 4) then
		;ENABLECONTROL drv:PBcb3 
	;else
	DISABLECONTROL drv:PBcb3 
	;end if	

    CALL post:MISCprotege(LST:fld6600protege,LST:fld6600desprotege)
    call post:MISCTranCap_MP


    RETURN (RETVAL)
END SUB

/*******/
