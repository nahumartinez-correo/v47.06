/****o* Pendientes de agrupación/OFB:post:PRET6860
* NOMBRE
*   OFB:post:PRET6860
* DESCRIPCION
*   Pre screen de la transacción de descuento de contrareembolso
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   post:TRAN6860
* DOCUMENTACION
* SOURCE
*/
#include "girospres.h"
SUB post:PRET6860
   	siaf:codigo_siaf =  6860
   	post:INDPresuD_Dat = OFB:CDS6860Append
   	call siaf:PRESfecha_hora
   	if ( retval < 0 )
   	   return -1
   	endif
   	
   	call post:MISCLeo_PresDet
   	if ( retval < 0 )
   	   return -1
   	endif
	
	if (siaf:adt_tran_med_pago != "S") then
		post:captura_mp = siaf:adt_tran_med_pago
		siaf:moneda = 99
		clearfield post:medio_pago_desc
	end if
	protfield siaf:moneda
	protfield FalloCrem_imp
END SUB

