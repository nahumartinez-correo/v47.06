#include "postmod.h"

SUB post:MODCM_Contado
	IF ((((!TESTFLAG(common:rt_flags,reentry_active) && (hcomm:tran_status < 100)) || \
	(TESTFLAG(common:rt_flags,reentry_active) && (hcomm:tran_status >= 100))))) then
		/*¨Cliente Medio en Modalidad Contado */
		if (post:cli_tipo == 5 && ModalidadCC != 1 && ModalidadCD != 1) then
        	RETURN (0)
        end if
   	END IF
    RETURN (-3)
END SUB

