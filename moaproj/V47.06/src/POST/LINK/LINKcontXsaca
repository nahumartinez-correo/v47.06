/****o* Pendientes de agrupación/OFB:post:LINKcontXsaca
* NOMBRE
*   OFB:post:LINKcontXsaca
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postLINK.h"

SUB post:LINKcontXsaca
	/* Cte. Op.: LLenado de Saca		*/
	/* May. 1998 - B. Petrella - 			*/
      if (testflag(tlr:tran_flags,correction))
             return -1
      end if
	/* Verifico que entre los prox. reg. de DetTmp, exista	*/
	/* un registro que no tenga la marca de borrado, porque sino*/
	/* la transaccion debe finalizar aqui.-				*/
	if (orden < orden_max)
	   let orden = orden + 1
	   DO
	   	call CDSleoDetTmp
	   	if (retval != 0)
         		let orden = orden + 1
			if (orden > orden_max)
				break
			end if
		else
			let orden = orden - 1
			break
		end if
	   LOOP
	end if
	if (orden >= orden_max)	/* Termino de procesar */
		/* Hay que modificar el registro del journal de los 	*/
		/* cont. seleccionados para la saca.-		*/
		call CDSmodifJNLsaca

		/* Según la saca, son distintas las impresiones */
		call MISCimpSaca

		return -1
	else
		return 0
	end if
END SUB

/*******/
