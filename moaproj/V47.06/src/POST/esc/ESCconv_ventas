*/
#include "postesc.h"

SUB post:ESCconv_ventas
	unprotfield post:convenio
	unprotfield post:convenio_desc
	unprotfield post:cliente_rsocial
	keydef KEY_UP_AT,RT_UP
	keydef KEY_DOWN_AT,RT_DOWN
	drv:button = TFB0
	call drv:but_up_enable
END SUB

