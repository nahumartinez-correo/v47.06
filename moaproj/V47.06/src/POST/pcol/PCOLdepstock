/****o* Pendientes de agrupación/OFB:post:PCOLdepstock
* NOMBRE
*   OFB:post:PCOLdepstock
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postpcol.h"

SUB post:PCOLdepstock
  /*Desactiva el Highlit de los campos antes de mostrarlos en pantalla*/
   CALL CSR:proc_list(F_NOHIGHLITE,LST:LISTdepstock_flds)
   CALL CSR:proc_list(F_REFRESH,LST:LISTdepstock_flds)
  
  IF (op:Flag_temp == 1)//la transaccion se esta ejecutando desde modif. pendientes
	CALL CDSsel_temporal
	    IF (RETVAL == 0) THEN
			IF (TESTFLAG(op:adt_temp_flag,update)) THEN
		        CALL CSR:proc_list(190,LST:LISTdepstock_flds2)
		        CALL post:MISCset_sidebar(1,0,0,0)
		        /*Carina- no ejecutan las Fval cuando los datos se extraen de la tabla temporal*/
		         CALL CSR:proc_list(F_SETVALDONE,LST:LISTdepstock_flds)
		         /*Carina- llama a una rutina que resalta los campos modificados*/
				CALL post:MISCResaltarCampos ("DepStock")
		        CALL CSR:proc_list(F_REFRESH,LST:LISTdepstock_flds)
			END IF
			IF (TESTFLAG(op:adt_temp_flag,update)) THEN
		        CALL CSR:proc_list(190,LST:LISTdepstock_flds2)
		        CALL post:MISCset_sidebar(0,1,0,0)
		        /*Carina- no ejecutan las Fval cuando los datos se extraen de la tabla temporal*/
		         CALL CSR:proc_list(F_SETVALDONE,LST:LISTdepstock_flds)
		         /*Carina- llama a una rutina que resalta los campos modificados*/
				CALL post:MISCResaltarCampos ("DepStock")
		        CALL CSR:proc_list(F_REFRESH,LST:LISTdepstock_flds)
			END IF
		END IF
  ELSE  
	 	IF 	(adt_depstock_cod.NUMCHARS == 0 ) THEN
        	CALL post:MISCset_sidebar(0,0,0,1)
		END IF
  END IF
END SUB

/*******/
