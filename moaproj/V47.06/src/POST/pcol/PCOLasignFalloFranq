#include "postpcol.h"

SUB post:PCOLasignFalloFranq
	CLEARFIELD asign_fallo_franq 
	CLEARFIELD asign_fallo_imp
	CLEARFIELD asign_fallo_op
	clearfield franq_seq
	REFRESH asign_fallo_franq
	REFRESH asign_fallo_imp
	REFRESH asign_fallo_op
	refresh franq_seq
	PROTFIELD asign_fallo_imp
	PROTFIELD franq_seq 
	CLEARFIELD tes:rend_marca_fallo
END SUB

