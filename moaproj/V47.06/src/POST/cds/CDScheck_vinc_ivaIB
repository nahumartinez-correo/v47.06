/****o* Pendientes de agrupación/OFB:post:CDScheck_vinc_ivaIB
* NOMBRE
*   OFB:post:CDScheck_vinc_ivaIB
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postcds.h"

SUB post:CDScheck_vinc_ivaIB
/*Esta rutina chequea si existen inscripciones en Ingresos Brutos que quedarán inconsistentes
si se debe validar la Tasa de Ingresos Brutos con la Categoría de IVA del Cliente - Gabriel Faciola*/
	if (adt_prvta_vinc_ivaIB == "S")
		CDS JOIN post:IvaCod,post:IngBCod FROM DSC:clioca_tbl as "a" DSC:CliIngBr_tbl as "b" \
		BECOMES "Vinc_ivaIB_ctx" \
		WHERE "a".post:CloTipDoc == "b".post:CloTipDoc && \
		"a".post:CloCuit == "b".post:CloCuit && "a".post:CloCatCodIB == fld:adt_prvta_tasa_cod && \
		"b".post:CloProvin == fld:adt_prvta_pcia_cod
		cds findfirst "Vinc_ivaIB_ctx" nolock
		while (cdsretval == 0)
			cds extract "Vinc_ivaIB_ctx" post:IvaCod post:IngBCod
			if (IvaCod != IngBCod)
				msgbox "No puede modificar este atributo, existen Ins. de Clientes que quedarán inconsist. (Ocasional)",OFB_OK,"ERROR"
				cds endselect "Vinc_ivaIB_ctx"
				return (-2)
			end if
			cds findnext "Vinc_ivaIB_ctx" nolock
		end while
		cds endselect "Vinc_ivaIB_ctx"

		/*Agrego el mismo chequeo para UP*/
		CDS JOIN post:IvaCod,post:IngBCod FROM DSC:cliocaUP_tbl as "a" DSC:CliIBUP_tbl as "b" \
		BECOMES "Vinc_ivaIB_ctx" \
		WHERE "a".post:CloTipDoc == "b".post:CloTipDoc && \
		"a".post:CloCuit == "b".post:CloCuit && "a".post:CloCatCodIB == fld:adt_prvta_tasa_cod && \
		"b".post:CloProvin == fld:adt_prvta_pcia_cod
		cds findfirst "Vinc_ivaIB_ctx" nolock
		while (cdsretval == 0)
			cds extract "Vinc_ivaIB_ctx" post:IvaCod post:IngBCod
			if (IvaCod != IngBCod)
				msgbox "No puede modificar este atributo, existen Insc. de Clientes que quedarán inconsist. (U.P.)",OFB_OK,"ERROR"
				cds endselect "Vinc_ivaIB_ctx"
				return (-2)
			end if
			cds findnext "Vinc_ivaIB_ctx" nolock
		end while
		cds endselect "Vinc_ivaIB_ctx"

		CDS JOIN post:IvaCod,post:IngBCod FROM DSC:TABCliMed as "a" \
		DSC:CliIBCliMed_tbl as "b" BECOMES "Vinc_ivaIB_ctx" \
		WHERE "a".post:CloTipDoc == "b".post:CloTipDoc && \
		"a".post:CloCuit == "b".post:CloCuit && \
		"a".post:CloCatCodIB == fld:adt_prvta_tasa_cod && \
		"b".post:CloProvin == fld:adt_prvta_pcia_cod

		cds findfirst "Vinc_ivaIB_ctx" nolock
		while (cdsretval == 0)
			cds extract "Vinc_ivaIB_ctx" post:IvaCod post:IngBCod
			if (IvaCod != IngBCod)
				msgbox "No puede modificar este atributo, existen Insc. de Clientes que quedarán inconsist. (CLIENTE MEDIO)",OFB_OK,"ERROR"
				cds endselect "Vinc_ivaIB_ctx"
				return (-2)
			end if
			cds findnext "Vinc_ivaIB_ctx" nolock
		end while
		cds endselect "Vinc_ivaIB_ctx"
	end if
END SUB

/*******/
