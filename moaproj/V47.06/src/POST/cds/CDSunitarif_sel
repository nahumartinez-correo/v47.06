/****o* Pendientes de agrupación/OFB:post:CDSunitarif_sel
* NOMBRE
*   OFB:post:CDSunitarif_sel
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postcds.h"

SUB post:CDSunitarif_sel
offset_0000:
    MOVE adt_unineg_cod TO tmp_unineg_cod
    CDS SELECT FROM DSC:adt_unineg_tbl BECOMES DSC:adt_unineg_ctx
    let siaf:last_key = LASTKEY
    CDS MULTIDISPLAY DSC:adt_unineg_ctx LISTunitarif
	IF (CDSRETVAL == 0x01FD) THEN
        GOTO offset_0000
    END IF
    LET lib:err_retval = CDSRETVAL
    IF (lib:err_retval == 0) THEN
        MOVE tmp_unineg_cod TO adt_unineg_cod
        CALL CDSunitarif_load
        RETURN (-2)
    END IF
    LET lib:err_retval = CDSRETVAL
    CALL lib:MISCcds_find_err
    IF (RETVAL != 0) THEN
      RETURN (-1)
    END IF
    CDS EXTRACT DSC:adt_unineg_ctx ALL
    CDS UNLOCK DSC:adt_unineg_ctx
	LET adt_tarif_uneg = adt_unineg_cod
	REFRESH adt_tarif_uneg
	
END SUB

/*******/
