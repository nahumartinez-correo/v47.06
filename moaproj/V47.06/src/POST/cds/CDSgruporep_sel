/****o* Pendientes de agrupación/OFB:post:CDSgruporep_sel
* NOMBRE
*   OFB:post:CDSgruporep_sel
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postcds.h"

SUB post:CDSgruporep_sel

offset_0000:

    MOVE adt_gruporep_cod TO tmp_gruporep_cod
    CDS SELECT FROM DSC:GrupoRep_tbl BECOMES DSC:GrupoRep_ctx
    let siaf:last_key = LASTKEY
    CALL post:ABMGrupoRep_clear
    CDS MULTIDISPLAY DSC:GrupoRep_ctx LISTadt_gruporep
    IF (CDSRETVAL == 0x01FD) THEN
        GOTO offset_0000
    END IF
    LET lib:err_retval = CDSRETVAL
    IF (lib:err_retval == 0) THEN
        MOVE tmp_gruporep_cod TO adt_gruporep_cod
        CALL post:CDSgruporep_load
        IF (RETVAL == 0) THEN
        	CALL post:ABMGrupoRep_refresh
        END IF
        RETURN (-2)
    END IF
    LET lib:err_retval = CDSRETVAL
    CALL lib:MISCcds_find_err
    IF (RETVAL != 0) THEN
    	CALL post:ABMGrupoRep_clear
        CALL post:ABMGrupoRep_refresh
        RETURN (-1)
    END IF
    CDS EXTRACT DSC:GrupoRep_ctx ALL
    CDS UNLOCK DSC:GrupoRep_ctx
	//journalizacion ABM GrupoRep
	CALL csr:proc_list(F_COPY,LST:LISTgruporep_flds,LST:LISTbck_gruporep)
END SUB

/*******/
