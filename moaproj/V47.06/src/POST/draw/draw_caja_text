#include "postdraw.h"

SUB post:draw_caja_text
        
    IF (siaf:codigo_siaf == 12601 || siaf:codigo_siaf == 12801)
        DRAW  TEXT 22,160,tipo_sobre
    ELSE        
        DRAW  TEXT 22,160,tipo_caja        
    END IF
    
END SUB    

