/****o* Pendientes de agrupación/OFB:post:SENDuen
* NOMBRE
*   OFB:post:SENDuen
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   post:TRANabm_uen
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postSEND.h"

SUB post:SENDuen
    LOCAL l_tabla LIKE admin:tabla 
    LOCAL l_INDCamposTablas LIKE post:INDCamposTablas 

    //SMC Autorizo Modificaciones
    call post:MISCAutorizoModif("UEN")
    if (retval < 0) then
	   return (-2)
    endif

    drv:save_button = drv:last_button
    IF (TESTFLAG(admin:mode_flags,update)) THEN
        CALL CDSuen_save
        CALL drv:side_bar_disable
        RESETFLAG admin:mode_flags,update
        ;SMC journalizo Modificacion
        call post:MISCJournalTablas
    ELSE
        IF (TESTFLAG(admin:mode_flags,delete)) THEN
            RESETFLAG admin:mode_flags,delete
            CALL post:MISCind_uen
            ;SMC Modificacion para Testear por impactos antes de Borrar.
            ;    Si se detecta impacto, se pregunta si se desea imprimir el reporte
            call lib:MISCchk_impacto
            if (retval < 0) then
                return (retval)
            endif
            CALL lib:adt_delete_verify
            IF (RETVAL == 0) THEN
                CALL drv:side_bar_disable
                CALL CDSuen_del
                IF (RETVAL != 0) THEN
                    drv:button = drv:save_button
                    CALL drv:but_up_enable
                    RETURN (-1)
                ELSE
                    ;SMC journalizo Modificacion
                    call post:MISCJournalTablas
                    CALL CSR:proc_list(170,LST:LISTuen_flds)
                    CALL CSR:proc_list(190,LST:LISTuen_flds)
                    RETURN (-1)
                END IF
            END IF
        ELSE
            IF (TESTFLAG(admin:mode_flags,imprimir)) THEN
                RESETFLAG admin:mode_flags,imprimir
                /*
                siaf:adt_tabla_context = DSC:adt_uen_context
                siaf:INDencab_lis = OFB:MISCencab_uen
                siaf:INDselect_tabla = OFB:CDSuen_select
                siaf:INDpaso_str_tabla = OFB:MISCpaso_str_uen
                siaf:INDestado_clave = OFB:MISCclave_uen
                siaf:INDadt_sval = 0
                CALL siaf:TRANimprimo_driver*/
                
                //Inicio gpb:AUT-2415 
                /*CALL MISCdumptable 
                  CALL CDSdumptable*/  
						    //resguardo los valores originales
						    LET l_tabla = admin:tabla 
						    LET l_INDCamposTablas = post:INDCamposTablas                  
                CALL MISCdumptableNew 
								admin:tabla = "UEN"
								post:INDCamposTablas = LST:post:LISTuen_flds
                CALL CDSdumptableNew  
                //recupero los valores originales
								LET admin:tabla = l_tabla
								LET post:INDCamposTablas = l_INDCamposTablas
                //Fin gpb:AUT-2415 
                
                CALL CSR:proc_list(170,LST:LISTuen_flds)
                CALL CSR:proc_list(190,LST:LISTuen_flds)
                RETURN (-1)
            END IF
        END IF
        RETURN (-1)
    END IF
    CALL CSR:proc_list(170,LST:LISTuen_flds)
    CALL CSR:proc_list(190,LST:LISTuen_flds)
    RETURN (-1)
END SUB

/*******/
