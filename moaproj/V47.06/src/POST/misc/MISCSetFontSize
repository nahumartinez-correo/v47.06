/****o* Pendientes de agrupación/OFB:post:MISCSetFontSize
* NOMBRE
*   OFB:post:MISSGetFontSize
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   se utiliza para establecer el pie de página y la longitud de desplazamiento del mismo.
* SALIDAS
*   Cadena que contiene los metacaracteres de pie de página y longitud
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postmisc.h"

SUB post:MISCSetFontSize(p_txS)
	LOCAL l_valor LIKE common:csr_string
	
	l_valor = p_txS
	
  select (l_valor)
      case 70
          prt textsize *70
      break
      case 71
          prt textsize *71
      break
      case 72
          prt textsize *72
      break
      case 73
          prt textsize *73
      break
      case 74
          prt textsize *74
      break
      case 75
          prt textsize *75
      break
     	case 76
          prt textsize *76
      break
      case 77
          prt textsize *77
      break
      case 78
          prt textsize *78
      break
      case 79
          prt textsize *79
      break
      case 80
          prt textsize *80
      break
      case 81
          prt textsize *81
      break
      case 82
          prt textsize *82
      break
      case 83
          prt textsize *83
      break
      case 84
          prt textsize *84
      break
      case 85
          prt textsize *85
      break
     	case 86
          prt textsize *86
      break
      case 87
          prt textsize *87
      break
      case 88
          prt textsize *88
      break
      case 89
          prt textsize *89
      break
      case 90
          prt textsize *90
      break
      case 91
          prt textsize *91
      break
      case 92
          prt textsize *92
      break
      case 93
          prt textsize *93
      break
      case 94
          prt textsize *94
      break
      case 95
          prt textsize *95
      break
     	case 96
          prt textsize *96
      break
      case 97
          prt textsize *97
      break
      case 98
          prt textsize *98
      break
      case 99
          prt textsize *99
      break
      case 100
          prt textsize *100
      break
      case 101
          prt textsize *101
      break
      case 102
          prt textsize *102
      break
      case 103
          prt textsize *103
      break
      case 104
          prt textsize *104
      break
      case 105
          prt textsize *105
      break
      case 106
          prt textsize *106
      break
      case 107
          prt textsize *107
      break
      case 108
          prt textsize *108
      break
      case 109
          prt textsize *109
      break		        
      case 110
          prt textsize *110
      break
      case 115
          prt textsize *115
      break
      case 120
          prt textsize *120
      break
      case 130
          prt textsize *130
      break
      case 140
          prt textsize *140
      break
      case 150
          prt textsize *150
      break
  end select
	
END SUB

