/****o* Registro VAP/OFB:post:MISClst_RepVAP_enc
* NOMBRE
*   OFB:post:MISClst_RepVAP_enc
* DESCRIPCION
*   Especifica titulo, escala , cantidad de lineas y tipo de fuente 
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   post:MISCrepPzasVAP
* DOCUMENTACION
*   NI50437
* SOURCE
*/
#include "postmisc.h"

SUB post:MISClst_RepVAP_enc
	LOCAL link_num       LIKE siaf:i //canal de comunicacion del archivo
	LOCAL file_name      LIKE common:csr_string
	LOCAL p_lineas_cl    LIKE siaf:i
	LOCAL l_imprimible   LIKE siaf:i
	LOCAL report_name    LIKE common:csr_string
	LOCAL p_scale          LIKE siaf:i
	LOCAL p_lines          LIKE siaf:i
	LOCAL p_textfont         LIKE siaf:i
	
	//seteo las variables del encabezado
	link_num = 4
	file_name = "RepVAP.lis"
	p_lineas_cl = 0
	
 	//si el reporte se imprime vertical
 	//l_imprimible = 100
	l_imprimible = 122
	
	//Sucursal y al Cierre Forzado de Sucursal
	SELECT (siaf:codigo_siaf)
		CASE 9540
			report_name = "REPORTE DE ENVIOS VAP MANUAL AL CIERRE"
		BREAK
		CASE 9550                
			report_name = "REPORTE DE ENVIOS VAP MANUAL AL CIERRE FORZADO"
		BREAK
		CASE ELSE                                
			report_name = "REPORTE DE ENVIOS VAP MANUAL"
		BREAK
	END SELECT

  //si el reporte se imprime vertical
	p_scale = 100
	p_lines = 66
	
	p_textfont = 0
	
  CALL post:MISClst_RepVAP_Enc1(p_lineas_cl,l_imprimible,link_num,report_name,p_scale,p_lines,p_textfont) 
 
END SUB

/*******/
