#include "postmisc.h"

SUB post:MISCtiene_aduana
    IF (common:br_aduana == "N")
        MSGBOX "NO SE PUEDE INGRESAR A ESTE PRODUCTO. LA SUCURSAL NO TIENE ADUANA",0x00,"AVISO"
        RETURN (-2)
    END IF
END SUB

