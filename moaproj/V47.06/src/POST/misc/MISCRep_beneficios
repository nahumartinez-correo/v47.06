/****o* Pendientes de agrupación/OFB:post:MISCRep_beneficios
* NOMBRE
*   OFB:post:MISCRep_beneficios
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postmisc.h"

SUB post:MISCRep_beneficios
	local copias_aux LIKE siaf:i //para cant. de copias del listado.

	// Abro el archivo
	call post:MISCopen_beneficios
	if ( retval < 0 )
		return -2
	end if
	
	// Encabezado
	let siaf:lineas_cl = 0
	let siaf:indica_calcu = 0 // inicializa cant. de hojas
	call post:MISCenc_beneficios
	if ( retval < 0 )
		return -2
	end if
	
	// Genero el listado
	call post:MISCwri_beneficios
	
	
    	lib:err_retval = retval
    	FYIMSG CLEAR
    	CLEARFIELD common:csr_string
    	if (lib:err_retval == -8)
    		//listados al cierre de la sucursal
    		call csr:file_close(1)
    		return -2
    	end if
	
    	call csr:file_close(1)
    	FYIMSG LST:common:fyimsg_list, \
	"(L) SE ESTA IMPRIMIENDO EL LISTADO DE BENEFICIOS "
		
		//cantidad de copias
    	LET copias_aux = 1

    	WHILE (copias_aux <= 2)
			CALL post:MISCimp_beneficios
 			LET copias_aux = copias_aux + 1
    	END WHILE	
	
	
END SUB

/*******/
