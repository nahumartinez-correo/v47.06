#include "postmisc.h"

SUB post:MISCopen_GirCrem
    ;LMB - PS 9780 - 14.07.2008
	;FPRINT siaf:dnix_cmd,"/te mp/GirCrem.lis"
    FPRINT siaf:dnix_cmd, "%s%s", common:dirTemporal, "GirCrem.lis"
    
    CALL CSR:file_open 1 siaf:dnix_cmd "w"
    IF (RETVAL < 0) THEN
      ERRMSG LST:common:errmsg_list, \
"Error al abrir el archivo para el listado"
      RETURN (-2)
    END IF
END SUB

