/****o* Pendientes de agrupación/OFB:post:MISCSetPosTabH
* NOMBRE
*   OFB:post:MISCSetPosTabH
* DESCRIPCION
*   Setea la posicion horizontal donde se va a escribir la cadena
* ENTRADAS
*   p_psTabH = Posicion Horizontal(dada en decipoints) 
* SALIDAS
*   Cadena que contiene los metacaracteres de pie de página y longitud
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   post:MISCSetPosTab
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postmisc.h"

SUB post:MISCSetPosTabH(p_psTabH)
	LOCAL l_valor LIKE common:csr_string
	
	l_valor = p_psTabH
	
  select (l_valor)
     case 3000
          PRT POSITIONABS -,*3000
      break
      case 3100
          PRT POSITIONABS -,*3100
      break
      case 3200
          PRT POSITIONABS -,*3200
      break
      case 3300
          PRT POSITIONABS -,*3300
      break
      case 3400
          PRT POSITIONABS -,*3400
      break
      case 3500
          PRT POSITIONABS -,*3500
      break
      case 3600
          PRT POSITIONABS -,*3600
      break
      case 3700
          PRT POSITIONABS -,*3700
      break
      case 3800
          PRT POSITIONABS -,*3800
      break
      case 3900
          PRT POSITIONABS -,*3900
      break
      case 3950
          PRT POSITIONABS -,*3950
      break
      case 4000
          PRT POSITIONABS -,*4000
      break
      case 4010
          PRT POSITIONABS -,*4010
      break
      case 4020
          PRT POSITIONABS -,*4020
      break  
       case 4030
          PRT POSITIONABS -,*4030
      break 
      case 4040
          PRT POSITIONABS -,*4040
      break 
      case 4050
          PRT POSITIONABS -,*4050
      break  
      case 4060
          PRT POSITIONABS -,*4060
      break
      case 4070
          PRT POSITIONABS -,*4070
      break
        case 4075
          PRT POSITIONABS -,*4075
      break    
      case 4080
          PRT POSITIONABS -,*4080
      break
      case 4090
          PRT POSITIONABS -,*4090
      break           
      case 4100
          PRT POSITIONABS -,*4100
      break
      case 4150
          PRT POSITIONABS -,*4150
      break
      case 4200
          PRT POSITIONABS -,*4200
      break
      case 4300
          PRT POSITIONABS -,*4300
      break
      case 4350
          PRT POSITIONABS -,*4350
      break
      case 4400
          PRT POSITIONABS -,*4400
      break
      case 4450
          PRT POSITIONABS -,*4450
      break
      case 4500
          PRT POSITIONABS -,*4500
      break
      case 4600
          PRT POSITIONABS -,*4600
      break     
      case 4700
          PRT POSITIONABS -,*4700
      break
      case 4800
          PRT POSITIONABS -,*4800
      break
      case 4900
          PRT POSITIONABS -,*4900
      break
      case 4950
          PRT POSITIONABS -,*4900
      break
      case 5000
          PRT POSITIONABS -,*5000
      break
      case 5250
          PRT POSITIONABS -,*5250
      break
  end select
	
END SUB

/*******/
