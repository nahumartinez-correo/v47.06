SUB post:MISCdeshabDevolPieza
    /* T&T: Devolucio de Pieza */
    /* Jun. 1998 - B. Petrella -            */
      let drv:button = 1 /* Combo asoc al cpo. CteOp */
    call drv:but_disable
      let drv:button = 2 /* Combo asoc al cpo. Apertura */
    call drv:but_disable
      let drv:button = 3 /* Combo asoc. al cpo. clase de cont. */
    call drv:but_disable
    nextfield adt_corteop_desc
END SUB

