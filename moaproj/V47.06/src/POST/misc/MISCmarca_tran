                altercontrolset check_tran_4
                break
        case 5
                setflag TranAnul_check,item_5
                altercontrolset check_tran_5
                break
        case 6
                setflag TranAnul_check,item_6
                altercontrolset check_tran_6
                break
        case 7
                setflag TranAnul_check,item_7
                altercontrolset check_tran_7
                break
        case 8
                setflag TranAnul_check,item_8
                altercontrolset check_tran_8
                break
        case 9
                setflag TranAnul_check,item_9
                altercontrolset check_tran_9
                break
        case 10
                setflag TranAnul_check,item_10
                altercontrolset check_tran_10
                break
        case 11
