#include "postmisc.h"

SUB post:MISCmarca_tran(idx)
        select (idx)
        case 0
                setflag TranAnul_check,item_0
                altercontrolset check_tran_0
                break
        case 1
                setflag TranAnul_check,item_1
                altercontrolset check_tran_1
                break
        case 2
                setflag TranAnul_check,item_2
                altercontrolset check_tran_2
                break
        case 3
