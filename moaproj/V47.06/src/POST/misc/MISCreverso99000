/****o* Pendientes de agrupación/OFB:post:MISCreverso99000
* NOMBRE
*   OFB:post:MISCreverso99000
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postmisc.h"

SUB post:MISCreverso99000

       Call siaf:SVALnivel_sup
       If (RETVAL < 0) THEN
         //UngetKey RT_ESC
         Return (Retval) //-2
       End If
       post:siaf_flags_aux = siaf:siaf_flags

	 /* Busco las transacciones 99046 de esta 99056 */
	 CDS SELECT FROM DSC:tlr:TABjnl BECOMES dsc:post:Jounarl_anul_Ctx where \
 		siaf:codigo_siaf == "99000" && \
		post:Pieza == post:Pieza && \
		post:Saco == post:Saco && \
		post:TipoServ_cod == TipoServ_cod && \
		tlr:jnl_sequence_corr == "" && \
		siaf:tiene_reverso == "S" && \
		tlr:jnl_status != 100

       Call siaf:MISClleno_modalidad
       UngetKey 0x0191 /* RT_SEND */
       CALL tlr:JNLtran
       Call post:CDSbusca_tabla (dsc:post:Jounarl_anul_Ctx )
       While ( Retval >= 0 )
    	   CALL csr:trimfield post:jnl_flia_tran
         siaf:adt_tran_codigo = siaf:codigo_siaf
         If (testflag(hcomm:state_flags,on_line) || siaf:posible_off == "S" )
               Call  post:MISCreversa_transac
         Else
               // Salteo la Transaccion
         Endif
         Call post:CDSbusca_tabla (dsc:post:Jounarl_anul_Ctx )
        End While
        Call post:CDSfin_tabla(dsc:post:Jounarl_anul_Ctx )
        Call post:MISCclear_campos_jou
        Resetflag lib:state_flags,scan_mode
End Sub

/*******/
