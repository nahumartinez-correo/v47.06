/****o* Pendientes de agrupación/OFB:post:MISCumov_encabez
* NOMBRE
*   OFB:post:MISCumov_encabez
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postmisc.h"

SUB post:MISCumov_encabez
    common:csr_string = "   "
    CALL MISCwrite_umov
    FPRINT common:csr_string,"              %40.40s              PAG: %3.3s",common:emp_desc,siaf:cta_hojas
    CALL MISCwrite_umov
    common:csr_string = "   "
    CALL MISCwrite_umov

   select (siaf:codigo_siaf_list)
        case 9540
    		FPRINT common:csr_string,"      CONSULTA DE ULTIMOS MOVIMIENTOS DE MERCHANDISING AL CIERRE"
    		break
        case 9550
		    FPRINT common:csr_string,"      CONSULTA DE ULTIMOS MOVIMIENTOS DE MERCHANDISING AL CIERRE FORZADO"

    		break
        case else
		    FPRINT common:csr_string,"             CONSULTA DE ULTIMOS MOVIMIENTOS DE MERCHANDISING"
            break
    end select   

    ;FPRINT common:csr_string,"             CONSULTA DE ULTIMOS MOVIMIENTOS DE MERCHANDISING"
    CALL MISCwrite_umov
    FPRINT common:csr_string,"                  ENTRE LOS DIAS %8e Y %8e",adt_umov_fecha_ini,adt_umov_fecha_fin
    CALL MISCwrite_umov
    FPRINT common:csr_string,"                              Sucursal:%s",common:sucursal_activa
    CALL MISCwrite_umov
    common:csr_string = "   "
END SUB

/*******/
