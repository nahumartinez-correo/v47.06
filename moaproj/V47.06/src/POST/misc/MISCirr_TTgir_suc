/****o* Pendientes de agrupación/OFB:post:MISCirr_TTgir_suc
* NOMBRE
*   OFB:post:MISCirr_TTgir_suc
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postmisc.h"

SUB post:MISCirr_TTgir_suc
    LOCAL oper_irre  LIKE tlr:jnl_operator
    LOCAL loc_modif  LIKE post:cant_barras  
    
	loc_modif = 0
	oper_irre = -1

    WHILE (cdsretval == 0)
    
        CDS EXTRACT DSC:giros:TTgiros_ctx ALL
            
        IF (oper_irre != giros:operador_ttgiros) THEN
            oper_irre = giros:operador_ttgiros
            common:csr_string = " "
            CALL post:MISCwrite_irre
            FPRINT common:csr_string,"OPERADOR: %-5.5e", oper_irre
            CALL post:MISCwrite_irre
            common:csr_string = "-------------------------------------------------------------------------------------------"
            CALL post:MISCwrite_irre
        END IF

        CALL post:MISCwri_irr_TTgiros
	    loc_modif = 1
        
        CDS FINDNEXT DSC:giros:TTgiros_ctx NOLOCK
	    
    END WHILE
    
    IF (loc_modif == 0) THEN
        IF (siaf:pantalla_impreso == "I") THEN
            FPRINT common:csr_string "\n\n\n    NO EXISTE NINGUNA IRREGULARIDAD EN EL PERIODO SELECCIONADO"
	        CALL post:MISCwrite_irre
	        CALL post:MISCimprime_irregu
	    ELSE
	        MSGBOX "NO EXISTE NINGUNA IRREGULARIDAD EN EL PERIODO SELECCIONADO",0x00000000,"Error"
	    END IF
	    CDS ENDSELECT DSC:giros:TTgiros_ctx
	    RETURN (-2)
    END IF

    CALL post:MISCimprime_irregu
    CDS ENDSELECT DSC:giros:TTgiros_ctx
END SUB

/*******/
