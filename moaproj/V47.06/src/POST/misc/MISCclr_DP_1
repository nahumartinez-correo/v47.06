    clearfield adt_grucorte_cod 	/* Grupo de Cte. Op. */
    clearfield adt_apertura_desc 	/* Apertura */
    clearfield adt_apertura_cod 	/* Apertura */
    clearfield DetalleContenido     /* Muestra las piezas individuales */

END SUB

