/****o* Pendientes de agrupación/OFB:post:MISCprocesoCalc
* NOMBRE
*   OFB:post:MISCprocesoCalc
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postmisc.h"

SUB post:MISCprocesoCalc
    LOCAL LMetaData   LIKE common:csr_string_4096
    LOCAL INDICE_INF  LIKE post:cantidad
    local l_retval    like lib:err_retval
    LOCAL L_item      LIKE siaf:idx
	local l_str		like common:csr_string_4096

    INDICE_INF = 0
	L_item = PL_GETITEM

	CLEARFIELD LMetaData
	CALL csr:magic_myPickcontrol DSC:PickTabla_mdata LMetaData L_item
	l_retval = retval
	WHILE (l_retval >= 0)
		; Cargo información de meta data para este campo en particular.
		CALL post:MISCcargaMdataCampo(LMetaData)

		; ejecuto rutina de cálculo de los
		if (adt_mdata_tipoProc == "C")
			if (adt_mdata_calculo.numchars != 0)
			    call post:MISCpasarAIndirecto(adt_mdata_calculo, "OFB")
			    let FLD:post:INDconsulta = FLD:post:INDtmp
			    if (INDconsulta != 0)
					call FLD:post:INDconsulta(common:csr_string)
					if (retval != VERDAD)
				    	FPRINT l_str "Falló rutina de cálculo - %s.\n", common:csr_string
					    call post:MISClogCargaMas(mdata_directorio, mdata_abm, l_str)
				    	return -2
					endif
				else
			    	FPRINT l_str "ERROR GRAVE en metadata. No se puede ejecutar rutina de cálculo - %s.\n", adt_mdata_calculo
				    call post:MISClogCargaMas(mdata_directorio, mdata_abm, l_str)
			    	return -2
				endif
			endif
		endif
		L_item = L_item + 1

		CLEARFIELD LMetaData
		CALL csr:magic_myPickcontrol DSC:PickTabla_mdata LMetaData L_item
		l_retval = retval
    end while
END SUB

/*******/
