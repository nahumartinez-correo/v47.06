/****o* Pendientes de agrupación/OFB:post:MISCencab_comp_pdte
* NOMBRE
*   OFB:post:MISCencab_comp_pdte
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postmisc.h"

SUB post:MISCencab_comp_pdte
	LOCAL l_link_num LIKE siaf:i //canal de comunicacion del archivo
	LOCAL l_report_name LIKE common:csr_string
	LOCAL l_lineas_cl LIKE siaf:i
	LOCAL l_imprimible LIKE siaf:i
	LOCAL l_lines LIKE siaf:i
	LOCAL l_orientation LIKE siaf:i
	LOCAL l_scale LIKE siaf:i
	LOCAL l_size LIKE siaf:i
	LOCAL l_font LIKE common:csr_string
	LOCAL l_style LIKE siaf:i
	LOCAL l_titulo_rep LIKE common:csr_string
	
	//gpb:PS10715
	LET l_titulo_rep = "CONSULTA DE COMPROBANTES DE IMPOSICION DE LA SUCURSAL"

	//seteo las variables del encabezado
	l_link_num = 5
	l_lineas_cl = 0
	l_imprimible = 75
	l_scale = 100
	l_lines = 65
	l_size = 100
	l_orientation = 0 //0= vertical ; 1=horizontal
	l_font = "Courier New"
	l_style = 0

	SELECT (siaf:codigo_siaf_list)
		CASE 9540
			FPRINT l_report_name "CONSULTA ANALITICA DE MOTIVOS DE LA SUCURSAL AL CIERRE"
		BREAK
		CASE 9550
			FPRINT l_report_name "CONSULTA ANALITICA DE MOTIVOS DE LA SUCURSAL AL CIERRE FORZADO"
		BREAK
	END SELECT
	
	SELECT (tlr:tran_mnemonic)
		CASE "ESTASUCU"
			FPRINT l_report_name "            %s" l_titulo_rep
		BREAK
		CASE "CIESUCU"
			FPRINT l_report_name "       %s AL CIERRE" l_titulo_rep
		BREAK
		CASE "CIEFOSU"
			FPRINT l_report_name "   %s AL CIERRE FORZADO" l_titulo_rep
		BREAK
		CASE ELSE
			FPRINT l_report_name "            %s" l_titulo_rep
		BREAK
	END SELECT 

	CALL post:MISClst_doc_enc_gral(l_link_num,l_report_name,l_lineas_cl,l_imprimible,l_lines,l_orientation,l_scale,l_size,l_font,l_style)  

	MOVE "---------------------------------------------------------------------------" TO common:csr_string
	CALL post:MISCwrite_doc_gral(5,FLD:common:csr_string)
	MOVE "          CLIENTE                                           NRO COMPROBANTE " TO common:csr_string    
	CALL post:MISCwrite_doc_gral(5,FLD:common:csr_string)
	MOVE "---------------------------------------------------------------------------" TO common:csr_string
	CALL post:MISCwrite_doc_gral(5,FLD:common:csr_string)

END SUB

/*******/
