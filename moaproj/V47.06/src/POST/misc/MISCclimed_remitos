/****o* Pendientes de agrupación/OFB:post:MISCclimed_remitos
* NOMBRE
*   OFB:post:MISCclimed_remitos
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postmisc.h"

SUB post:MISCclimed_remitos

	call csr:trimfield BulkFld_105
	call csr:trimfield BulkFld_106
	call csr:trimfield BulkFld_107
	call csr:trimfield BulkFld_111

	if (post:BulkFld_105.numchars != 0 )
		call post:MISCclimed_con_remi(post:BulkFld_105,post:Longitud_105)
		if (retval < 0 )
			return -2
		end if
	end if

	if (post:BulkFld_106.numchars != 0 )
		call post:MISCclimed_con_remi(post:BulkFld_106,post:Longitud_106)
		if (retval < 0 )
			return -2
		end if
	end if

	if (post:BulkFld_107.numchars != 0 )
		call post:MISCclimed_con_remi(post:BulkFld_107,post:Longitud_107)
		if (retval < 0 )
			return -2
		end if
	end if

	if (BulkFld_111.NUMCHARS != 0)
		call MISCadelantos_climed(BulkFld_111,Longitud_111,CM_CREM)
		if (retval < 0 )
			return -2
		end if
	end if

END SUB

/*******/
