/****o* Pendientes de agrupación/OFB:post:MISCencab_listados
* NOMBRE
*   OFB:post:MISCencab_listados
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postmisc.h"

SUB post:MISCencab_listados

    LET common:csr_string = "   "
    CALL siaf:MISCwrite_lis
    FPRINT common:csr_string, \
    "                           CORREO ARGENTINO"
    CALL siaf:MISCwrite_lis
      FPRINT common:csr_string, \
    "                                                                    PAGINA: %-3.3s", siaf:cta_hojas    
    CALL siaf:MISCwrite_lis
    LET common:csr_string = "   "
    CALL siaf:MISCwrite_lis
 
    FPRINT common:csr_string, \
    "                   LISTADO DE LA TABLA LISTADOS"
    CALL siaf:MISCwrite_lis
    LET common:csr_string = "   "
    CALL siaf:MISCwrite_lis
    LET common:csr_string = "   "
    CALL siaf:MISCwrite_lis
    FPRINT common:csr_string, \
    "   NOMBRE DEL LISTADO                              INCLUYE      CANT.COPIAS"
    CALL siaf:MISCwrite_lis
    LET common:csr_string = "   "
    CALL siaf:MISCwrite_lis
    
END SUB

/*******/
