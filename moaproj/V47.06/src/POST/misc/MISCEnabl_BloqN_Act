#include "postmisc.h"

// Habilita El Menu de Actividades y BlqNum
Sub post:MISCEnabl_BloqN_Act
   let drv:button = TFB2
   call drv:but_redraw
   call drv:but_enable
   let drv:button = TFB3
   call drv:but_redraw
   call drv:but_enable
END SUB

