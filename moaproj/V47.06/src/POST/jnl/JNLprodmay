/****o* Pendientes de agrupación/OFB:post:JNLprodmay
* NOMBRE
*   OFB:post:JNLprodmay
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postjnl.h"

SUB post:JNLprodmay   //tes:JNLventa_ev
	local x like siaf:i

    post:cantidad = post:adt_pmaux_cant
	nis_oficina = tlr:jnl_branch
	cli_tipo_mosaic = CLI_MOSAIC_MAY	
    
   	//Cargo el vector pedido por Nina
   	cont_simpNac = cantidad
   	cont_simpInt = 0
   	CALL MISCvec_dest_cant
    
    cds append dsc:tlr:CONjnl_write \
    FLD:adt_pmaux_codmat    \
    FLD:adt_pmaux_descmat   \
    FLD:adt_pmaux_cant      \
    FLD:adt_pmaux_presu     \
    FLD:adt_pmaux_um        \
    FLD:siaf:cantidad       \
    FLD:post:cantidad       \
    FLD:post:PreDForFis     \
    fld:post:adt_pmaux_clMosaic

	/*Agrego el campo indirecto con rutina de impresión de ticket para Nota de Crédito
	Gabriel Falciola*/

	CDS APPEND DSC:tlr:CONjnl_write FLD:post:INDticket_laser
	call post:MISCcampos_reporte(0,post:cantidad,0,NO_TARIFA)
	CALL csr:proc_list(F_APPEND,DSC:tlr:CONjnl_write,LST:LISTreporte,0)	
	
	x = 0
	while (x < 2)
		Cds Append DSC:tlr:CONjnl_write  \
			vec_dest[x] \
			vec_cant[x]
		x = x + 1
	END while
	
	Cds Append DSC:tlr:CONjnl_write  \
		nis_oficina \
		cli_tipo_mosaic


END SUB

/*******/
