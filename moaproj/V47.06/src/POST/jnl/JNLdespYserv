/****o* Pendientes de agrupación/OFB:post:JNLdespYserv
* NOMBRE
*   OFB:post:JNLdespYserv
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postjnl.h"

SUB post:JNLdespYserv
    /* Deposito y Servicios - JNL    */
    /* B. Petrella - Agos. 1998 -    */

    /* Registro Minimo para journalizar la transac. 99090 */

    CDS APPEND DSC:tlr:CONjnl_write \
        post:adt_corteop_desc \
        post:AperDesc \
        post:adt_opc_desc \
        post:cod_postal \
	  post:TipoDespYserv \
        post:SiNoTT \
        post:Pieza  \
        post:EnvioGdesCltes

    /* Campos necesarios de journalizar para que el registro */
    /* sea leido desde llenado de contenedor (Si No es Encomienda)*/
    /* o desde llenado de saca (Si es encomienda) */

    call MISCclearJNLllen
    CDS APPEND DSC:tlr:CONjnl_write \
        post:adt_corteop_cod as jnl_corteop_cod \
        post:adt_corteop_guiacont as post:jnl_corteop_guiacont \
	  post:adt_corteop_exprap as jnl_expedic_rapida \
        post:jnl_guia_cont \
        post:jnl_fecha_llencont\
        post:jnl_hora_llencont \
        post:jnl_guia_saca \
        post:jnl_fecha_llensaca \
        post:jnl_hora_llensaca
    CDS APPEND DSC:tlr:CONjnl_write \
        post:cod_postal as post:cp \
        post:adt_aper_cod as jnl_corteop_apert \
        post:Pieza as track_trace \
        post:adt_corteop_guiacont

    if (adt_opc_opci.numchars != 0)
        let cant_opc = 1
        let cod_opc[0] = adt_opc_opci
        fprint cor_opc[0], "%1.1s%1.1s", adt_opc_cortepropio, \
                               adt_opc_disccp
    end if
    CDS APPEND DSC:tlr:CONjnl_write \
        post:cant_opc \
        post:cod_opc[0]  \
        post:cod_opc[1]  \
        post:cod_opc[2]  \
        post:cod_opc[3]  \
        post:cod_opc[4]  \
        post:cod_opc[5]
    CDS APPEND DSC:tlr:CONjnl_write \
        post:cor_opc[0]  \
        post:cor_opc[1]  \
        post:cor_opc[2]  \
        post:cor_opc[3]  \
        post:cor_opc[4]  \
        post:cor_opc[5]
END SUB

/*******/
