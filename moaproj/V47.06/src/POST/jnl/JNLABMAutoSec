#include "postjnl.h"

SUB post:JNLABMAutoSec

	/*Seteo el nombre de la tabla - Gabriel Falciola*/
	admin:tabla = DSC:adt_secuen_table

	cds append dsc:tlr:CONjnl_write \
	op:op_login_id as op:adt_login_id

	call csr:proc_list(F_APPEND,dsc:tlr:CONjnl_write,lst:post:LISTsecuen_flds,0)
	call csr:proc_list(F_APPEND,dsc:tlr:CONjnl_write,lst:post:LISTbck_AutoSec,0)
END SUB

