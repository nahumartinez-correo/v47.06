/****o* Pendientes de agrupación/OFB:post:adt_to_bck30
* NOMBRE
*   OFB:post:adt_to_bck30
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postofb.ofb.h"

SUB post:adt_to_bck30

	LET bck_prodvs_cod = adt_prodvs_cod
	MOVE adt_prodvs_codbarra TO bck_prodvs_codbarra
	MOVE adt_prodvs_lectob TO bck_prodvs_lectob
	LET bck_prodvs_tarifa = adt_prodvs_tarifa
	LET bck_prodvs_aliciva = adt_prodvs_aliciva
	MOVE adt_prodvs_formfisca TO bck_prodvs_formfisca
	MOVE adt_prodvs_deb_cred TO bck_prodvs_deb_cred
	MOVE adt_prodvs_cble1 TO bck_prodvs_cble1
	MOVE adt_prodvs_cble2 TO bck_prodvs_cble2
    MOVE adt_prodvs_espv_cod TO bck_prodvs_espv_cod
    MOVE adt_prodvs_espv_scod TO bck_prodvs_espv_scod
    MOVE adt_prodvs_ctl_stock TO bck_prodvs_ctl_stock
   move adt_prodvs_desc to bck_prodvs_desc
    move adt_prodvs_abrev to bck_prodvs_abrev
    move adt_prodvs_bonifCM to bck_prodvs_bonifCM
    move adt_prodvs_rel_vol to bck_prodvs_rel_vol
    move adt_prodvs_prepaq to bck_prodvs_prepaq
END SUB

