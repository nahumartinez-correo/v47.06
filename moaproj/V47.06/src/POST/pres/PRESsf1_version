/****o* Pendientes de agrupación/OFB:post:PRESsf1_version
* NOMBRE
*   OFB:post:PRESsf1_version
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postpres.h"

SUB post:PRESsf1_version
   LOCAL MuestraMensaje like siaf:i

   MuestraMensaje = 1
   siaf:codigo_siaf = 6830
   ;SMC 16/02/1999 Agrego llamado a la PresFechaHora
   CALL siaf:PRESfecha_hora
   IF (RETVAL < 0) THEN
        RETURN (-1)
   END IF
   call post:PRESsf1_cuentacorr
   if (retval < 0) then
        Return (-1)
   end if

   Call post:MISCget_version_bob(MuestraMensaje)
   If ( Retval < -1 )
      Return -1
   endif
   // Me fijo la version de Bob
   Call csr:date_compare(common:system_date,post:fecha)
   If ( Retval == 0 || retval == 2)
		let post:fecha = common:system_date
		MSGBOX "La actualización de Versión ya fue ejecutada. Por problemas, comuníquese con soporte.",OFB_OK,"Error"
		Return -1
   Endif

   ;siaf:posible_off = "S"
   ;siaf:nombre_transac = "ACTUALIZACION DE LA VERSION DE PAGO FACIL"
   Protfield fecha
   Protfield hora
   Refresh fecha
   Refresh hora
End Sub

/*******/
