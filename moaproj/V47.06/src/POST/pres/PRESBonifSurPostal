/****o* Pendientes de agrupación/OFB:post:PRESBonifSurPostal
* NOMBRE
*   OFB:post:PRESBonifSurPostal
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postpres.h"

SUB post:PRESBonifSurPostal
   siaf:codigo_siaf =  99095
   post:INDPresuD_Dat = OFB:post:CDSBonifSurPosAppend
   Call siaf:PRESfecha_hora
   If ( Retval < 0 )
      Return -1
   Endif
   Protfield medio_pago_desc
   Protfield siaf:importe
   Protfield NroCompPago

   if (estado_presu == 4)
   		protfield siaf:moneda
   end if

   Call post:MISCLeo_PresDet
   If ( Retval < 0 )
      Return -1
   Endif

   if (post:estado_presu == 3)
   		monedaNC = siaf:moneda
   		post:imp_d = siaf:importe
   end if
   if (post:estado_presu == 2 || post:estado_presu == 3) then
       if (siaf:adt_tran_med_pago != "S") then
          post:captura_mp = siaf:adt_tran_med_pago
          protfield siaf:moneda
          siaf:moneda = 99
          clearfield post:medio_pago_desc
          DISABLECONTROL drv:PBcb0
         drv:last_button = 0
       end if
    end if
END SUB

/*******/
