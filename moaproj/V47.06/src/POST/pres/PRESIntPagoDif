#include "postpres.h"

SUB post:PRESIntPagoDif
	
	siaf:codigo_siaf =  99160
	siaf:moneda = "99"
	
  	Call siaf:PRESfecha_hora
  	If ( Retval < 0 )
      Return -1
    Endif
    
    CALL post:CDSpresudet_load
      
END SUB

