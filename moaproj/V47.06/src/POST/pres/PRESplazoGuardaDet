#include "postpres.h"

SUB post:PRESplazoGuardaDet
	
	siaf:nombre_transac.HIGHLITE = 0
    siaf:nombre_transac = "DET. MODIFICACION PLAZO GUARDA"
    siaf:codigo_siaf = 99076
    siaf:tiene_reverso = "N"
    siaf:imprime_comprobante = "N"
    siaf:posible_off = "S"
    siaf:afecta_efectivo = " "
	call siaf:PRESfecha_hora
    if (retval < 0) then
        return (-1)
    end if
	
END SUB

