/****o* Pendientes de agrupación/OFB:post:PRTEncabezado_Sector
* NOMBRE
*   OFB:post:PRTEncabezado_Sector
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postprt.h"

SUB post:PRTEncabezado_Sector (cant_pag)

  /*Encabezado de la Transaccion:CONSULTA DE SECTORES*/
    PRT  ORIENTATION 0 
    common:csr_string = "    "
    CALL MISCwrite_lis5
    PRT  SCALE 100
    PRT  MARGIN *7920 *6120 0 0 0 0
    PRT  STYLE 0
    PRT  TEXTFONT 100 
    PRT  TEXTFONTNAME "Courier New"
    PRT  TEXTSIZE *120 
    FPRINT common:csr_string "                                                                Pág. Nº: %3e", cant_pag
    CALL MISCwrite_lis5
    FPRINT common:csr_string "Sucursal %5e %-35.35e       Fecha de Emisi\xF3n: %-8e", common:sucursal_activa, common:br_bank_name, common:system_date
    CALL MISCwrite_lis5
    PRT  STYLE 1
    PRT  TEXTSIZE *160 
    common:csr_string = "                        Consulta de Sectores"
    CALL MISCwrite_lis5
    PRT  TEXTSIZE *120 
    common:csr_string = "       SECTOR       DESCRIPCION                   UNIDAD DE NEGOCIO      EN USO"
    CALL MISCwrite_lis5
    common:csr_string = "   "
    CALL MISCwrite_lis5
    
END SUB

/*******/
