/****o* Pendientes de agrupación/OFB:post:PRTConsulta_Sector
* NOMBRE
*   OFB:post:PRTConsulta_Sector
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postprt.h"

SUB post:PRTConsulta_Sector

    LET nro_pag_CT = 1
    LET siaf:INDencab_lis = OFB:PRTEncabezado_Sector
    LET siaf:i = 1
    CALL PRTEncabezado_Sector (nro_pag_CT)        
        
    WHILE (lib:err_retval != -801)
        CDS EXTRACT DSC:adt_sectores_ctx ALL
        PRT  STYLE 0
        PRT  TEXTSIZE *120
        FPRINT common:csr_string "       %-4.4e            %-25.25e   %-6.6e        %-1.1e", \
        adt_sectores_cod,adt_sectores_desc,adt_sectores_ttbu,adt_sectores_useflag
        CALL MISCwrite_lis5
        CDS FINDAGAIN DSC:adt_sectores_ctx NOLOCK
        CDS FINDNEXT DSC:adt_sectores_ctx NOLOCK
        LET lib:err_retval = CDSRETVAL
    END WHILE
    /*Imprimo el pie de pagina*/
    common:csr_string = "    "
    CALL MISCwrite_lis5
    PRT  STYLE 30
    PRT  TEXTSIZE *120 
    FPRINT common:csr_string "                                 CANT. PAG.: %3e" nro_pag_CT
    CALL MISCwrite_lis5  
    CDS ENDSELECT DSC:adt_sectores_ctx 
END SUB

/*******/
