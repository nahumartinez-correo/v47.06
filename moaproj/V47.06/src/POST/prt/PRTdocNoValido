     PRT  TEXTSIZE *220
     PRT  POSITIONABS *300 *2838
     PRT  PRINT "X"
     PRT  PENPAT 0
     PRT  STYLE 30
     PRT  TEXTSIZE *120
     PRT  TEXTFONTNAME ""
     PRT  TEXTSIZE *100
     PRT  POSITIONABS *561 *2350
     PRT  PRINT "Documento no v\xE1lido como factura"
