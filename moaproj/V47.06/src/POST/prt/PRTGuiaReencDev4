/****o* Pendientes de agrupación/OFB:post:PRTGuiaReencDev4
* NOMBRE
*   OFB:post:PRTGuiaReencDev4
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postprt.h"

SUB post:PRTGuiaReencDev4
LOCAL pagina LIKE j_idx
LOCAL CantLin LIKE post:j_idx
LOCAL GC LIKE post:adt_stockp_rem_nom
LOCAL NroLote LIKE post:adt_stockp_lote
LOCAL CantPag LIKE post:j_idx
LOCAL CodTrack LIKE post:adt_codtrack_grupo
LOCAL CantTot LIKE post:j_idx
LOCAL CantFin LIKE post:j_idx
LOCAL CPzas LIKE post:j_idx
LOCAL AuxLin LIKE LineaImpresion

LET CantTot = 0
LET CantFin = 0
LET CantLin = 0
LET pagina = 1


CDS SELECT FROM DSC:post:DevPza4 BECOMES DSC:post:DevPza4_ctx WHERE \
	    (post:CodOperador == op:op_operator_id && \
	     post:MarcaIncluye == ">>" && adt_stockp_reenc == "S" && \
	     adt_stockp_reenc_cul == "S")
	     
CDS FINDFIRST DSC:post:DevPza4_ctx LOCK
LET lib:err_retval = CDSRETVAL
IF (lib:err_retval >= 0)
	CDS EXTRACT DSC:DevPza4_ctx all
END IF
CLEARFIELD LineaImpresion
CALL post:PRThdr_DP3("Reenc")

DO
   LET GC = post:adt_stockp_rem_nom
   LET LineaImpresion = post:adt_stockp_rem_nom
   PRT  PRINT EDIT post:LineaImpresion
   PRT  NEWLINE
   PRT  CARRIAGERETURN
   Let CantLin = CantLin + 1
   PRT  NEWLINE
   PRT  CARRIAGERETURN
   Let CantLin = CantLin + 1

   DO
	LET NroLote = post:adt_stockp_lote
	FPRINT LineaImpresion, "LOTE NRO.: %s ", NroLote
	PRT  PRINT EDIT post:LineaImpresion
	PRT  NEWLINE
	PRT  CARRIAGERETURN
	Let CantLin = CantLin + 1
	PRT  NEWLINE
	PRT  CARRIAGERETURN
	Let CantLin = CantLin + 1

	DO
		IF (CPzas == 0)
			CLEARFIELD LineaImpresion
			LET LineaImpresion = "                                                                                                    "
			FPRINT LineaImpresion, "%-32.32s", Pieza
		ELSE
			IF (CPzas == 1)
				FPRINT AuxLin, "%-32.32s", LineaImpresion
				FPRINT LineaImpresion, "%-32.32s  %-32.32s", AuxLin, Pieza
			ELSE
				FPRINT AuxLin, "%-66.66s", LineaImpresion
				FPRINT LineaImpresion, "%-66.66s  %-32.32s", \
					AuxLin, Pieza
			END IF
		END IF
		LET CPzas = CPzas + 1
		LET CantTot = CantTot + 1
		IF (CPzas == 3)
			IF (CantLin > 40)
				DO
					IF (CantLin >= 38)
						BREAK
					END IF
					PRT  NEWLINE
					PRT  CARRIAGERETURN
					CantLin = CantLin + 1
				LOOP
				LET CantLin = 0
				SWAPDUP LineaImpresion
				CLEARFIELD LineaImpresion
				fprint LineaImpresion \
					"                                                                                 Página: %03.3d" \
				      pagina

				PRT  PRINT EDIT post:LineaImpresion
				PRT  NEWLINE
				PRT  CARRIAGERETURN
				Let CantLin = CantLin + 1
                PRT FORMFEED
				CLEARFIELD LineaImpresion
				CALL post:PRThdr_DP3("Reenc")
				LET pagina = pagina + 1

				LET LineaImpresion = post:adt_stockp_rem_nom
				PRT  PRINT EDIT post:LineaImpresion
				PRT  NEWLINE
				PRT  CARRIAGERETURN
				Let CantLin = CantLin + 1
				PRT  NEWLINE
				PRT  CARRIAGERETURN
				Let CantLin = CantLin + 1

				FPRINT LineaImpresion, "LOTE NRO.: %s ", NroLote
				PRT  PRINT EDIT post:LineaImpresion
				PRT  NEWLINE
				PRT  CARRIAGERETURN
				Let CantLin = CantLin + 1
				PRT  NEWLINE
				PRT  CARRIAGERETURN
				Let CantLin = CantLin + 1
				SWAPDUP LineaImpresion
			END IF

		//	PRT  TEXTSIZE *110
			PRT  PRINT EDIT post:LineaImpresion
			PRT  NEWLINE
			PRT  CARRIAGERETURN
		//	PRT  TEXTSIZE *120
			Let CantLin = CantLin + 1
			Let CPzas = 0
			CLEARFIELD post:LineaImpresion
		END IF

		CDS FINDNEXT DSC:post:DevPza4_ctx LOCK
		LET lib:err_retval = CDSRETVAL
		IF (lib:err_retval >= 0)
			CDS EXTRACT DSC:DevPza4_ctx all
		END IF

		IF (GC != post:adt_stockp_rem_nom || \
		    NroLote != post:adt_stockp_lote || \
		    lib:err_retval < 0)
		//	PRT  TEXTSIZE *110
			PRT  PRINT EDIT post:LineaImpresion
			PRT  NEWLINE
			PRT  CARRIAGERETURN
		//	PRT  TEXTSIZE *120
			Let CantLin = CantLin + 1
			CLEARFIELD LineaImpresion
			FPRINT LineaImpresion, "                TOTAL PIEZAS %s: %3d", \
				GC, \
				CantTot
			LET CantFin = CantFin + CantTot
			LET CantTot = 0
			LET CPzas = 0
			PRT  PRINT EDIT post:LineaImpresion
			PRT  NEWLINE
			CLEARFIELD LineaImpresion
			Let CantLin = CantLin + 1
			PRT  NEWLINE
			PRT  CARRIAGERETURN
			Let CantLin = CantLin + 1

			BREAK
		END IF
	LOOP
	IF (lib:err_retval < 0 || GC != post:adt_stockp_rem_nom)
		PRT  NEWLINE
		CLEARFIELD LineaImpresion
		Let CantLin = CantLin + 1
		PRT  NEWLINE
		PRT  CARRIAGERETURN
		Let CantLin = CantLin + 1
		BREAK

	  END IF
	LOOP
	IF (lib:err_retval < 0)
		BREAK
   	END IF
LOOP

PRT  NEWLINE
PRT  CARRIAGERETURN
PRT  NEWLINE
PRT  CARRIAGERETURN

DO
	if (CantLin >= 35)
		break
	end if
      PRT  NEWLINE
      PRT  CARRIAGERETURN
      LET CantLin = CantLin + 1
LOOP

//CantFin = CantFin + cant_simp_esp

clearfield LineaImpresion
fprint LineaImpresion "                                   %031.31s %6.6d - %10.10s %-8.8s" \
	"Total de piezas que se remiten:" \
      CantFin  \
	"Operador: " \
      op:op_login_id

PRT  PRINT EDIT post:LineaImpresion
PRT  NEWLINE
PRT  CARRIAGERETURN

PRT  NEWLINE
PRT  CARRIAGERETURN

PRT  NEWLINE
PRT  CARRIAGERETURN
fprint LineaImpresion "                                 %052.52s" \
	"Espacio reservado para intercalación Legajo archivo."
PRT  PRINT EDIT post:LineaImpresion

PRT  NEWLINE
PRT  CARRIAGERETURN

fprint LineaImpresion \
	"                                                                                     Página: %03.3d" \
      pagina

PRT  PRINT EDIT post:LineaImpresion
PRT  NEWLINE

PRT  CARRIAGERETURN
PRT  NEWLINE

END SUB

/*******/
