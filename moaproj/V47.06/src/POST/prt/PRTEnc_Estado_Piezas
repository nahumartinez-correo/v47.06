/****o* Pendientes de agrupación/OFB:post:PRTEnc_Estado_Piezas
* NOMBRE
*   OFB:post:PRTEnc_Estado_Piezas
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postprt.h"

SUB post:PRTEnc_Estado_Piezas (cant_pag)

  /*REPORTE DE ESTADO DE PIEZAS*/
    PRT  ORIENTATION 1 //Imprime en forma apaisada
    PRT  SCALE 100
    /*PRT  MARGIN *7920 *6120 0 0 0 0*/
    PRT  STYLE 0
    PRT  TEXTFONT 100 
    PRT  TEXTFONTNAME "Courier New"
    PRT  TEXTSIZE *120 
    FPRINT post_string_aux "                                                                                Pág. Nº: %3e", cant_pag
    CALL MISCprt_rep1
    FPRINT post_string_aux "Sucursal %5e %-35.35e                              Fecha de Emisi\xF3n: %-8e", common:sucursal_activa, common:br_bank_name, common:system_date
    CALL MISCprt_rep1
    PRT  STYLE 1
    PRT  TEXTSIZE *160 
    post_string_aux = "                                 Registro de Piezas"
    CALL MISCprt_rep1
    PRT  STYLE 30
    PRT  TEXTSIZE *120 
    /*Imprimir el descriptor que corresponda segun la seleccion*/
    CALL MISCimprimir_dsc
    CALL MISCprt_rep1
    PRT  TEXTSIZE *120 
    post_string_aux = "FECHA REC  PIEZA/REMITENTE                  LOTE      REN DEV VD CREM INTER DESTINO             APART Nº OBSERV" 
    CALL MISCprt_rep1
    post_string_aux = "   "
    CALL MISCprt_rep1
    
END SUB

/*******/
