/****o* Pendientes de agrupación/OFB:post:PRTsoloReenc
* NOMBRE
*   OFB:post:PRTsoloReenc
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postprt.h"

SUB post:PRTsoloReenc(tipo_guia)
    /*Si hay piezas seleccionadas que solamente sean Simple Especial Imprime 
    la guia de contenedor/saca/despacho solamente con la cantidad de piezas
    seleccionadas*/
    LOCAL cant LIKE j_idx
    LOCAL pagina LIKE j_idx
    LOCAL CantContFin LIKE j_idx
    LOCAL CantSacasFin LIKE j_idx
    
    /*Verifico si las piezas seleccionadas son SOLO Reencaminadas*/
    CALL CDSsoloReenc("Expedicion",DSC:DetTmp,DSC:DetTmp_ctx)
    IF (RETVAL < 0)
      RETURN (-2)//Encontro al menos 1 pieza que no era Reencaminada
    END IF
	
	/*MFM (20.11.2001) Para que no se imprima la guía normal en blanco*/
	RETURN (0)//Todas las Piezas seleccionadas son Reencaminadas
   
   /********************************************************************/ 
   
    Let pagina = 1
    
    /*Imprimo la guia correspondiente*/
    IF (tipo_guia == "Desp") 
        // Cabecera y cuerpo listado de Despachos - Transferencias
        CALL post:PRThdrDespacho("Desp")
        DO
            IF (cant >= 25)
		        BREAK
	        END IF
            PRT NEWLINE
            PRT CARRIAGERETURN
            LET cant = cant + 1
        LOOP
        PRT PRINT EDIT post:leyenda
        PRT NEWLINE
        PRT CARRIAGERETURN
        LET cant = cant + 1
        FPRINT LineaImpresion "       "
        PRT PRINT EDIT post:LineaImpresion
        PRT NEWLINE
        PRT CARRIAGERETURN
        LET cant = cant + 1
        FPRINT LineaImpresion "                                   %031.31s %5.5d - %-8.8s" \
    		"Total de Piezas que se remiten:" \
	        cant_simp_esp \
	        op:op_login_id
        PRT PRINT EDIT post:LineaImpresion
        PRT NEWLINE
        PRT CARRIAGERETURN
        cant = cant + 1
        FPRINT LineaImpresion "                                   %31.31s %5.5d - %-8.8s" \
	        "Total de Cont. que se remiten:" \
	        CantContFin \
	        op:op_login_id
        PRT PRINT EDIT post:LineaImpresion
        PRT NEWLINE
        PRT CARRIAGERETURN
        cant = cant + 1
        FPRINT LineaImpresion "                                   %31.31s %5.5d - %-8.8s" \
	        "Total de Sacas que se remiten:" \
		    CantSacasFin \
		    op:op_login_id
        PRT PRINT EDIT post:LineaImpresion
        PRT NEWLINE
        PRT CARRIAGERETURN
        cant = cant + 1
        PRT NEWLINE
        PRT CARRIAGERETURN
        cant = cant + 1
        CLEARFIELD LineaImpresion
        LET LineaImpresion = "       Despachado por:                               Fecha:                                        Hora:"
        PRT PRINT EDIT post:LineaImpresion
        PRT NEWLINE
        PRT CARRIAGERETURN
        LET cant = cant + 1
        CLEARFIELD LineaImpresion
        LET LineaImpresion = "       Recibido por:                                     Fecha:                                        Hora:"
        PRT PRINT EDIT post:LineaImpresion
        PRT NEWLINE
        PRT CARRIAGERETURN
        LET cant = cant + 1
        CALL CDScheckSum ("NoReenc")
        CLEARFIELD LineaImpresion
        /*FPRINT LineaImpresion "%s %06.6x" "V3.22",itemcheck
        PRT PRINT EDIT post:LineaImpresion
        PRT NEWLINE
        PRT CARRIAGERETURN
        LET cant = cant + 1*/
        PRT NEWLINE
        PRT CARRIAGERETURN
        LET cant = cant + 1
        
        //PS 7686 - Muestro el checksum de la nueva version V3.23
		FPRINT LineaImpresion "%s %04.4x","Checksum:",itemcheckV23 //para que lo muestre en hexadecimal
		PRT  PRINT EDIT post:LineaImpresion
		PRT  NEWLINE
		PRT  CARRIAGERETURN
		Let cant = cant + 1
		
		PRT NEWLINE
        PRT CARRIAGERETURN
        LET cant = cant + 1
        FPRINT LineaImpresion "                         %052.52s" \
	        "Espacio reservado para intercalación Legajo archivo."
        PRT PRINT EDIT post:LineaImpresion
        PRT NEWLINE
        PRT CARRIAGERETURN
        PRT NEWLINE
        PRT CARRIAGERETURN
        FPRINT LineaImpresion \
	        "                                                                                 Página: %03.3d" \
	        pagina
        PRT PRINT EDIT post:LineaImpresion
        PRT NEWLINE
        PRT CARRIAGERETURN                
        
    ELSE
        IF (tipo_guia == "Saca") 
            /*Cuerpo de la guia de saca vacia*/
            IF (SacaUnica == "N")
                LET LineaImpresion = "Guía de Detalle de Saca"
            ELSE
	            LET LineaImpresion = "Guía de Detalle de Saca Unica"
            END IF
            CALL PRThdrSaca("Saca")
            PRT NEWLINE
            PRT CARRIAGERETURN
            LET cant = cant + 1
            DO
	            IF (cant >= 36)
		            BREAK
		        END IF
                PRT NEWLINE
                PRT CARRIAGERETURN
                LET cant = cant + 1
            LOOP
            CLEARFIELD LineaImpresion
            FPRINT LineaImpresion "                                   %031.31s %5.5d - %10.10s %-8.8s" \
	            "Total de Piezas que se remiten:" \
                cant_simp_esp \
	            "Operador: " \
                op:op_login_id
            PRT PRINT EDIT post:LineaImpresion
            PRT NEWLINE
            PRT CARRIAGERETURN
            LET cant = cant + 1
            CLEARFIELD LineaImpresion
            FPRINT LineaImpresion "                                   %037.37s %5.5d - %10.10s %-8.8s" \
	            "Total de Contenedores que se remiten:" \
                CantContFin  \
	            "Operador: " \
                op:op_login_id
            PRT PRINT EDIT post:LineaImpresion
            PRT NEWLINE
            PRT CARRIAGERETURN            
            PRT NEWLINE
            PRT CARRIAGERETURN
            LET cant = cant + 1            
        
        ELSE
            /*Cuerpo de la guia de contenedor vacia*/
            DO
	            IF (cant >= 42)
	                BREAK
	            END IF
	            PRT NEWLINE
	            PRT CARRIAGERETURN
	            LET cant = cant + 1
            LOOP
            /* Linea modif. por el Correo Arg. - Jul.´98 */
            CLEARFIELD LineaImpresion
            FPRINT LineaImpresion "                       %031.31s %5.5d - %09.9s %-8.8s" \
	            "Total de piezas que se remiten:" \
	            cant_simp_esp \
	            "Operador:" \
	            op:op_login_id
            PRT PRINT EDIT post:LineaImpresion
            PRT NEWLINE
            PRT CARRIAGERETURN
            LET cant = cant + 1        
        END IF
        
        /*********** PIE DE PAGINA PARA CONTENEDORES Y SACAS **************/ 
        /* Rutina que calcula el CheckSum de todos los T&T que contiene el contenedor */
        call CDScheckSum ("NoReenc")
        clearfield LineaImpresion
        /*fprint LineaImpresion "%s %06.6x" "V3.22",itemcheck
        PRT  PRINT EDIT post:LineaImpresion
        PRT  NEWLINE
        PRT  CARRIAGERETURN
        Let cant = cant + 1*/
        //PS 7686 - Muestro el checksum de la nueva version de T&T
        clearfield LineaImpresion
        fprint LineaImpresion "%s %04.4x","Checksum:",itemcheckV23
        PRT  PRINT EDIT post:LineaImpresion
        PRT  NEWLINE
        PRT  CARRIAGERETURN
        Let cant = cant + 1
        fprint LineaImpresion "                         %052.52s" \
	        "Espacio reservado para intercalación Legajo archivo."
        PRT  PRINT EDIT post:LineaImpresion
        PRT  NEWLINE
        PRT  CARRIAGERETURN
        PRT  NEWLINE
        PRT  CARRIAGERETURN
        fprint LineaImpresion \
    	    "                                                                                 Página: %03.3d" \
    	    pagina
        PRT  PRINT EDIT post:LineaImpresion
        PRT  NEWLINE
        PRT  CARRIAGERETURN
        PRT  NEWLINE
        /********* FIN PIE DE PAGINA GUIA SACA/CONTENEDORES ***************/
        
    END IF    
	RETURN (0)//Todas las Piezas seleccionadas son Reencaminadas
END SUB

/*******/
