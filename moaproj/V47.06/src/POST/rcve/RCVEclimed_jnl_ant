/****o* Pendientes de agrupación/OFB:post:RCVEclimed_jnl_ant
* NOMBRE
*   OFB:post:RCVEclimed_jnl_ant
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postrcve.h"

SUB post:RCVEclimed_jnl_ant

	call post:RCVEhdr_climedio
	if (hcomm:tran_status == HOST_ACCEPTED )
		call post:RCVEgral_climedio
		if ( retval < 0 )
			return -2
		end if

		call op:CDSop_marca_reverso(SET_MARCA_REV)

		if (!testflag(tlr:controla_firma,cancelacion))
			if (!testflag(giros:tran_send_mode,consulta_simple))
				//msgbox giros:mensaje_transaccion,OFB_OK,"Mensaje"
			end if
		end if
	else
		return -2
	end if
END SUB

