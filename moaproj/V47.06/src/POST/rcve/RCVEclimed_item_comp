/****o* Pendientes de agrupación/OFB:post:RCVEclimed_item_comp
* NOMBRE
*   OFB:post:RCVEclimed_item_comp
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postrcve.h"

SUB post:RCVEclimed_item_comp

	call post:RCVEhdr_climedio
	if (hcomm:tran_status == HOST_ACCEPTED )
		call post:RCVEgral_climedio
		if ( retval < 0 )
			return -2
		end if
		if (!testflag(tlr:controla_firma,cancelacion))
			//msgbox giros:mensaje_transaccion,OFB_OK,"Mensaje"
		end if
	else
		if (!testflag(tlr:controla_firma,cancelacion))
	        	if (giros:CodRespuesta_comm != 98 && \
			    giros:CodRespuesta_comm != 95 && giros:CodRespuesta_comm != 09) then
				rechazoDiferido = true
				CALL CDSadt_presuact_save
				msgbox \
				"LA SESION SERÁ REVERSADA AUTOMATICAMENTE",\
				0X00,"ADVERTENCIA"
				CALL post:MISCajusteUP 
			endif
		end if


	end if
END SUB

/*******/
