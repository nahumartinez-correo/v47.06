#include "postlist.h"

LIST post:LISTcierreLote
	MEMBER	post:adt_lcie_index					  // Índice del registro
	MEMBER	post:adt_lcie_acquirerCod		  // Código de procesador 
	MEMBER	post:adt_lcie_batchNumber		  // Número de lote 
	MEMBER	post:adt_lcie_issuerCode		  // Código de tarjeta 
	MEMBER	post:adt_lcie_purchaCount	    // Cantidad de ventas 
	MEMBER	post:adt_lcie_purchaAmoun  	  // Monto total de ventas 
	MEMBER	post:adt_lcie_voidCount			  // Cantidad anulaciones de venta 
	MEMBER	post:adt_lcie_voidAmount		  // Monto total de anulaciones 
	MEMBER	post:adt_lcie_refundCount		  // Cantidad de devoluciones venta 
	MEMBER	post:adt_lcie_refundAmoun		  // Monto total de devoluciones 
	MEMBER	post:adt_lcie_refvoidCoun		  // Cantidad anulaciones devolución 
	MEMBER	post:adt_lcie_refvoidAmou	    // Monto total anul. Devolución
	MEMBER	post:adt_lcie_date						// Fecha ("DD/MM/AAAA")  
	MEMBER	post:adt_lcie_time						// Hora ("HH:MM:SS")
	MEMBER	post:adt_lcie_terminalID      // Terminal ID
END LIST
