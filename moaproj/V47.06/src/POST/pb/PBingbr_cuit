/****o* ABM´s/OFB:post:PBingbr_cuit
* NOMBRE
*   OFB:post:PBingbr_cuit
* DESCRIPCION
*   Combo del campo socumento del abm Ingresos Brutos
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   LST:post:BUTadt_CliIngBr
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postpb.h"

SUB post:PBingbr_cuit

    CLEARFIELD CloSucAlt
    CLEARFIELD CloFecAct

    drv:button = drv:last_button	
    CALL drv:but_down_disable   
    call drv:all_but_disable
    

    CLEARFIELD CloSucAlt
    CLEARFIELD CloFecAct
    drv:button = drv:last_button
    CALL drv:but_down_disable
    drv:button = 3
    CALL drv:but_disable
    drv:button = 1
    CALL drv:but_disable
    CALL CDSadt_clicuit_sel
    IF (retval == 0) THEN
      drv:button = 25
      CALL drv:but_disable
      drv:button = 26
      CALL drv:but_disable
      drv:button = 27
      CALL drv:but_disable
      drv:button = 28
      CALL drv:but_disable
      adt_TipoDoc_tipo = CloTipDoc
      call CDSTipoDocu_load
      SETVALDONE adt_TipoDoc_tipo
    ELSE
    	CLEARFIELD CloCuit
    	CLEARFIELD CloRazon
    	CLEARFIELD CloNombre    
    	clearfield CloApellido
		CLEARFIELD adt_TipoDoc_tipo
		CLEARFIELD adt_TipoDoc_desc
		CLEARFIELD CloCatCodIB
		CLEARFIELD adt_prvta_pcia_cod
		CLEARFIELD adt_pcia_desc
		CLEARFIELD IngBCod
		CLEARFIELD CloNroInb
		CLEARFIELD CloSucAlt
		CLEARFIELD CloFecAct
		CLEARFIELD CloCoefPartProv
		CLEARFIELD CloExento
    	REFRESH CloCuit
    	REFRESH CloRazon
    	REFRESH CloNombre  
    	REFRESH CloApellido  
		REFRESH adt_prvta_pcia_cod
		REFRESH adt_pcia_desc
		REFRESH IngBCod
		REFRESH CloNroInb
		REFRESH CloSucAlt
		REFRESH CloFecAct
		REFRESH CloCoefPartProv
		REFRESH CloExento
		REFRESH CloCatCodIB
		REFRESH adt_TipoDoc_tipo
		REFRESH adt_TipoDoc_desc

    call drv:back_but_status
    drv:button = drv:last_button
    CALL drv:but_up_enable
      return -2
      
    ENDIF

    IF (CloCuit.NUMCHARS == 0) THEN
       NEXTFIELD CloCuit
       RETURN (-1)
    END IF


    REFRESH CloCuit
    REFRESH CloRazon
    REFRESH CloNombre
    REFRESH CloApellido
    NEXTFIELD adt_prvta_pcia_cod
    CLEARFIELD adt_prvta_pcia_cod
    CLEARFIELD adt_pcia_desc
    CLEARFIELD IngBCod
    CLEARFIELD CloNroInb
    CLEARFIELD CloSucAlt
    CLEARFIELD CloFecAct
    CLEARFIELD CloCoefPartProv
    CLEARFIELD CloExento

    REFRESH adt_prvta_pcia_cod
    REFRESH adt_pcia_desc
    REFRESH IngBCod
    REFRESH CloNroInb
    REFRESH CloSucAlt
    REFRESH CloFecAct
    REFRESH CloCoefPartProv
    REFRESH CloExento
	REFRESH CloCatCodIB
	REFRESH adt_TipoDoc_tipo
	REFRESH adt_TipoDoc_desc
    /*Chequeo en la tabla CatIngBr los atributos que indican cómo deben manejarse
    el porcentaje de participación y el número de inscripción - Gabriel Falciola*/
    adt_CatIngBr_cod = CloCatCodIB
    call CDSCatIngBr_load
    if (adt_CatIngBr_reqpart == "N")
    	CloCoefPartProv = 1
    	protfield CloCoefPartProv
    else
    	clearfield CloCoefPartProv
    	unprotfield CloCoefPartProv
    end if
    refresh CloCoefPartProv
	
	PROTFIELD CloRazon
	PROTFIELD CloNombre
	PROTFIELD CloApellido

    call drv:back_but_status
    drv:button = drv:last_button
    CALL drv:but_up_enable


END SUB

/*******/
