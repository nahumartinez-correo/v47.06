#include "postpb.h"

SUB post:PBant_pie
    SELECT (opcion)
        CASE 3
            opcion = 2
            BREAK
        CASE 2
            opcion = 1
            BREAK
    END SELECT
    SETFLAG screen_consulta,anterior
    RESETFLAG screen_consulta,proximo
    RESETFLAG screen_consulta,cancelar
    RESETFLAG screen_consulta,actualizar
    UNGETKEY RT_SEND
END SUB

