#include "postpb.h"

SUB post:PBfin_RecepTT_UP
	
	setvaldone cant_piezasUP
	
	setvaldone post:apertura
	
	SETVALDONE prod_PrePaq_cod

    /*LET drv:button = CB4 //drv:PBcb4 Modificar
