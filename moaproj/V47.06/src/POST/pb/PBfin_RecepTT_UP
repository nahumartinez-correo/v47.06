	LET drv:button = CB10*/

	drv:button = drv:last_button
    CALL drv:but_down_disable
    
	CALL MISCverif_fin_UP("recep")
	IF (retval < 0)
		drv:button = drv:last_button
    	CALL drv:but_up_enable	
		return -2
	END IF
    	    
    setvaldone piezaUP
    
    ungetkey RT_SEND

END SUB

