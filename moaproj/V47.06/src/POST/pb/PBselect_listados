/****o* Pendientes de agrupación/OFB:post:PBselect_listados
* NOMBRE
*   OFB:post:PBselect_listados
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   NI50572 - Digitalizacion de Reportes
* SOURCE
*/
#include "postpb.h"

SUB post:PBselect_listados
	LOCAL l_last_button LIKE drv:last_button
	
	l_last_button = drv:last_button
	drv:button = drv:last_button
	CALL drv:but_down_disable
	CALL drv:all_but_disable

	CALL CDSlistados_incluye
    
	IF (RETVAL == 0) THEN
		MOVE adt_list_incluye TO adt_list_incluye
		CALL CSR:proc_list(F_REFRESH,LST:LISTlistados_fld)
		SETVALDONE adt_list_incluye
		drv:button = 25
		CALL drv:but_enable
		drv:button = 26
		CALL drv:but_enable
		drv:button = 27
		CALL drv:but_disable     
  END IF

	CALL drv:back_but_status
	drv:button = l_last_button
	CALL drv:but_up_enable
    
END SUB    

/*******/
