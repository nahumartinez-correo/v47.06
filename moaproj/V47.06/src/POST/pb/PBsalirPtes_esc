#include "postpb.h"

SUB post:PBsalirPtes_esc
	/* Sale de la pantalla de Pendientes */
	/* al hacer un Corte de Linea  - Cierre de Suc. con Esc.    */
	/* Jul. 1998 - B. Petrella -         */
    ungetkey 0x0190 /* Esc */
END SUB

