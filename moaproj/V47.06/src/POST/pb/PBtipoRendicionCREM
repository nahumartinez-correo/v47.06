/****o* Cobro por Cuenta de Terceros/OFB:post:PBtipoRendicionCREM
* NOMBRE
*   OFB:post:PBtipoRendicionCREM
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   post:BUTreporte_CREM
* DOCUMENTACION
*   https://jirats.siemens.com.ar/jira/browse/AUT-1684
* SOURCE
*/
#include "postpb.h"

SUB post:PBtipoRendicionCREM
    CALL drv:but_down_disable
    CALL drv:all_but_disable
    
    SETFLAG drv:mode_flags,picklist_mode
    COMBOPICK drv:PBcb1
    RESETFLAG drv:mode_flags,picklist_mode
    INIT 33,0
    refresh tipo_rend_CCT
    PROTFIELD tipo_rend_CCT

    NEXTFIELD comitente_CCT
    CALL drv:back_but_status 
    drv:button = drv:last_button
    CALL drv:but_up_enable
END SUB

/*******/
