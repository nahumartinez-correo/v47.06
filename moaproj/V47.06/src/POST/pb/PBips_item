#include "postpb.h"

SUB post:PBips_item
//mgi:NI50387 - IPS
    SETFLAG drv:mode_flags,picklist_mode
    COMBOPICK drv:PBcb13
    RESETFLAG drv:mode_flags,picklist_mode
    INIT 33,0
    NEXTFIELD adt_serv_ips_item
    drv:button = drv:last_button
    CALL drv:but_up_enable
END SUB  

