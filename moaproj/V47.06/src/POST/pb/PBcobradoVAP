/****l* Registro VAP/OFB:post:PBcobradoVAP
* NOMBRE
*   OFB:post:PBcobradoVAP
* DESCRIPCION
*   Despliega el picklist de seleccion del valor de cobro VAP
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   post:BUTrepPzasVAP
* DOCUMENTACION
*   NI50437
* SOURCE
*/
#include "postpb.h"

SUB post:PBcobradoVAP
	LOCAL l_last_button LIKE drv:last_button
	
	l_last_button = drv:last_button 
	drv:button = drv:last_button    
	CALL drv:but_down_disable
	CALL drv:all_but_disable
	
	CALL drv:MISCf_sacombopl ("PICcobradoVAP", common:csr_string,drv:button)
	IF (retval >= 0) THEN
		post:cobradoVAP = common:csr_string
		CALL CSR:trimfield(post:cobradoVAP)
		REFRESH post:cobradoVAP
	END IF
	
	CALL drv:back_but_status
	drv:button = l_last_button
	CALL drv:but_up_enable

END SUB

/*******/
