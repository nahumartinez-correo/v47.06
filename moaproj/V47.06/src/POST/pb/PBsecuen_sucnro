/****o* Pendientes de agrupación/OFB:post:PBsecuen_sucnro
* NOMBRE
*   OFB:post:PBsecuen_sucnro
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postpb.h"

SUB post:PBsecuen_sucnro
    drv:button = drv:last_button
    CALL drv:but_disable
   	CALL CDSadt_suactiva
   	FLD:post:adt_secuen_sucnro = FLD:admin:adt_br_branch
   	refresh adt_secuen_sucnro
   	refresh admin:adt_br_CEC
   	clearfield adt_secuen_cod
   	IF (RETVAL == 0) THEN
       /* habilito Actualizar */    
       drv:button = 25
       CALL drv:but_enable
       drv:button = 28
       CALL drv:but_disable
   	ELSE
       drv:button = 25
       CALL drv:but_disable
       drv:button = 28
       CALL drv:but_disable
   	END IF   	   	
   	refresh adt_secuen_cod
    CALL CSR:proc_list(170,LST:LISTsecuen_flds2)
    CALL CSR:proc_list(190,LST:LISTsecuen_flds2)
            
    drv:button = drv:last_button
    CALL drv:but_up_enable
END SUB

/*******/
