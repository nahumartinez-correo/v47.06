/****o* Pendientes de agrupación/OFB:post:PBumov_cod_desde
* NOMBRE
*   OFB:post:PBumov_cod_desde
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postpb.h"

SUB post:PBumov_cod_desde
    drv:button = drv:last_button
    CALL drv:but_down_disable
    drv:button = 1
    CALL drv:but_disable
    CALL CDSprodvs_sel
    if (retval < 0) then
	clearfield umov_codprod_desde
	clearfield umov_barra_desde
	clearfield umov_desc_desde
    endif
    umov_codprod_desde = adt_prodvs_cod
    umov_barra_desde = adt_prodvs_codbarra
    umov_desc_desde = adt_prodvs_desc
    refresh umov_codprod_desde
    refresh umov_barra_desde
    refresh umov_desc_desde
    nextfield umov_codprod_hasta
    drv:button = drv:last_button
    CALL drv:but_up_enable
    drv:button = 1
    CALL drv:but_enable
END SUB

/*******/
