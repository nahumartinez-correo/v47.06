/****o* Pendientes de agrupación/OFB:post:PBdespachos
* NOMBRE
*   OFB:post:PBdespachos
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postpb.h"

SUB post:PBdespachos
    drv:button = 28
    CALL drv:but_disable
    drv:button = drv:last_button
    CALL drv:but_down_disable
    drv:button = 0
    CALL drv:but_disable
    drv:button = 2
    CALL drv:but_disable
    CALL CDSdespacho_sel
    IF (RETVAL == 0) THEN
        MOVE adt_despacho_cod TO adt_despacho_cod
        CLEARFIELD adt_despacho_desc
        CALL CSR:proc_list(F_REFRESH,LST:LISTdespacho_flds)
        drv:button = 0
        CALL drv:but_enable
        drv:button = 2
        CALL drv:but_enable
        drv:button = 26
        CALL drv:but_disable
        drv:button = 27
        CALL drv:but_disable
    END IF
    IF (LASTKEY == 0x0190) THEN
		drv:button = 0
        CALL drv:but_up_enable
        drv:button = 2
        CALL drv:but_enable
		RETURN (-2)
	END IF
    drv:button = 0
    CALL drv:but_enable
    drv:button = drv:last_button
    CALL drv:but_up_enable
    drv:button = 2
    CALL drv:but_enable
    NEXTFIELD adt_despacho_desc
END SUB

/*******/
