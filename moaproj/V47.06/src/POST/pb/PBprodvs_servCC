/****o* Pendientes de agrupación/OFB:post:PBprodvs_servCC
* NOMBRE
*   OFB:post:PBprodvs_servCC
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postpb.h"

SUB post:PBprodvs_servCC
    drv:button = 1
    CALL drv:but_disable
    drv:button = 2
    CALL drv:but_disable
    drv:button = 3
    CALL drv:but_disable
    drv:button = drv:last_button
    CALL drv:but_down_disable
    CALL CDSprodvs_sel
    IF (RETVAL == 0) THEN
    	/*
	    IF ((post:adt_serv_usa_caja == "S" || post:adt_serv_usa_formu == "S") && adt_prodvs_cod.numchars != 0)
			MSGBOX "NO PUEDE COMPLETAR ESTE CAMPO SI EL SERVICIO UTILIZA TIPO DE CAJA O FORMULARIO",OFB_OK,"ERROR"
			clearfield post:adt_serv_prodvsCC
			refresh post:adt_serv_prodvsCC
		    drv:button = 1
		    CALL drv:but_enable
		    drv:button = 2
		    CALL drv:but_enable
		    drv:button = 3
		    CALL drv:but_enable
		    drv:button = drv:last_button
		    CALL drv:but_up_enable
		    RETURN (-2)
	   	END IF
	   	*/
        fld:adt_serv_prodvsCC = adt_prodvs_cod
        refresh adt_serv_prodvsCC
    END IF
    drv:button = 1
    CALL drv:but_enable
    drv:button = 2
    CALL drv:but_enable
    drv:button = 3
    CALL drv:but_enable
    drv:button = drv:last_button
    CALL drv:but_up_enable
END SUB

