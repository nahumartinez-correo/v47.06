#include "postABM.h"

SUB post:ABMrestClear
    adt_rest_regHab = "N"
	CLEARFIELD adt_rest_regTipo
	adt_rest_prvHab = "N"
	CLEARFIELD adt_rest_prvTipo
	adt_rest_sucHab = "N"
	CLEARFIELD adt_rest_sucTipo
	adt_rest_cliHab = "N"
	CLEARFIELD adt_rest_cliTipo
	adt_rest_upHab  = "N"
	CLEARFIELD adt_rest_upTipo
END SUB

