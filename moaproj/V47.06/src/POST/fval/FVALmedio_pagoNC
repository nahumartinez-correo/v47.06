/*Si la transaccion es un Template, la validacion de las monedas es distinto - Gabriel Falciola*/
SUB post:FVALmedio_pagoNC
/*	Esta rutina no se usa más - Gabriel Falciola
	if (siaf:codigo_siaf != 6900)
		call FVALmedio_pago
		if (retval != 0)
			return (-2)
		end if
	else
		CALL MISCverif_moneda
		IF (RETVAL < 0) THEN
			MSGBOX "MEDIO DE PAGO INEXISTENTE",0x00,"ERROR"
			call post:PBmedio_pagoNC
			IF (RETVAL < 0) THEN
				RETURN (-2)
			end if
		END IF
	end if*/
END SUB

