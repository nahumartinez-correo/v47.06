/****o* Pendientes de agrupación/OFB:post:FVALcaratulaGral
* NOMBRE
*   OFB:post:FVALcaratulaGral
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postfval.h"

SUB post:FVALcaratulaGral
    LOCAL i like idx
    LOCAL SumaNoCero like idx
    SumaNoCero = 0
    /* Valida los 3 primeros caracteres y los 3 ultimos */
    /* deben ser un CTP existente en uneg - Modif. 24/Sept./1999 */
    CALL CDSexisCTP
    IF (RETVAL < 0)
		RETURN (-2)
    END IF

    /* Valida que despues de los 3 primeros caracteres  */
    /* haya 7 dígitos - Modif. 24/Sept./1999 */
    i = 3
    DO
	IF (i > 9)
		BREAK
	END IF

	SELECT (SUBSTR(CaratulaSaca,i,1))
		Case "0"
			i = i + 1
			Break
        	Case "1"
		Case "2"
        	Case "3"
		Case "4"
        	Case "5"
		Case "6"
        	Case "7"
		Case "8"
        	Case "9"
			SumaNoCero = SumaNoCero + 1
			i = i + 1
			Break
		Case ELSE
	            return (-2)
	END SELECT
    LOOP
    IF (SumaNoCero == 0) THEN
		RETURN (-2)
    END IF
    /* La posición 11 debe tener una D - Modif. 24/Sept./1999 */
    IF (SUBSTR(CaratulaSaca,i,1) == "D" || \
	  SUBSTR(CaratulaSaca,i,1) == "E")
		i = i + 1
    ELSE
		RETURN (-2)
    END IF

    /* Las 6 posiciones siguientes deben ser números - Modif. 24/Sept./1999 */
    DO
	IF (i > 16)
		BREAK
	END IF
	SELECT (SUBSTR(CaratulaSaca,i,1))
		Case "0"
        	Case "1"
		Case "2"
        	Case "3"
		Case "4"
        	Case "5"
		Case "6"
        	Case "7"
		Case "8"
        	Case "9"
			i = i + 1
			Break
		Case ELSE
	            return (-2)
	END SELECT
    LOOP

END SUB

/*******/
