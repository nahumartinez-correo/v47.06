#include "postfval.h"

SUB post:FVALrecepContra
    /* B. Petrella - May. 1998    */
    IF (ContraReembolso == 0) then
           let common:csr_string = "El Importe debe ser distinto de 0"
           errmsg common:errmsg_list,FLD:common:csr_string
           return (-2)
    END IF
END SUB

