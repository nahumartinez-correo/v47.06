/****o* Pendientes de agrupación/OFB:post:FVALprodvs_servCC
* NOMBRE
*   OFB:post:FVALprodvs_servCC
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postfval.h"

SUB post:FVALprodvs_servCC
	CALL csr:trimfield(adt_serv_prodvsCC)
	IF ((adt_serv_prodvsCC == "") || (adt_serv_prodvsCC.numchars == 0)) THEN
       	 //setvaldone adt_serv_prodvsCC
      	 RETURN (0)
    END IF
    /*
    IF ((post:adt_serv_usa_caja == "S" || post:adt_serv_usa_formu == "S")  && adt_serv_prodvsCC.numchars != 0)
		MSGBOX "NO PUEDE COMPLETAR ESTE CAMPO SI EL SERVICIO UTILIZA TIPO DE CAJA O FORMULARIO",OFB_OK,"ERROR"
		clearfield post:adt_serv_prodvsCC
		refresh post:adt_serv_prodvsCC
	    RETURN (-2)
   	END IF
   	*/
    fld:adt_prodvs_cod = fld:adt_serv_prodvsCC
    Call post:CDSprodvs_load
    IF (RETVAL != 0) THEN
		drv:button = 1
        call drv:but_disable
        drv:button = 2
        call drv:but_disable
        drv:button = 3
        call drv:but_disable
        call CDSprodvs_sel
    	IF (LASTKEY == RT_ESC) THEN
    		return (-2)
    	end if
    END IF
	fld:adt_serv_prodvsCC = fld:adt_prodvs_cod
	refresh adt_serv_prodvsCC
    //setvaldone adt_serv_prodvsCC
    drv:button = 1
    call drv:but_enable
    drv:button = 2
    call drv:but_enable
    drv:button = 3
    call drv:but_enable
END SUB

/*******/
