/****o* Pendientes de agrupación/OFB:post:FVALIngBProv
* NOMBRE
*   OFB:post:FVALIngBProv
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postfval.h"

SUB post:FVALIngBProv
	CALL CDSadt_cliprov_load
	IF (RETVAL != 0 ) THEN
		CALL CDSadt_cliprov_sel
		if (retval != 0)
			return (-2)
		end if
      	CALL MISCingbr_refresh
	ENDIF

	CALL CDSadt_cliprov_busq

	IF (retval == 0 )
		drv:button = 25
		CALL drv:but_enable
		drv:button = 26
		CALL drv:but_enable
		drv:button = 27
		CALL drv:but_disable
		drv:button = 28
		CALL drv:but_disable
	ELSE
		drv:button = 25
		CALL drv:but_disable
		drv:button = 26
		CALL drv:but_disable
		drv:button = 27
		CALL drv:but_enable
		drv:button = 28
		CALL drv:but_disable
		clearfield IngBCod
		clearfield CloExento
		clearfield CloSucAlt
		clearfield CloFecAct
		refresh IngBCod
		refresh CloExento
		refresh CloSucAlt
		refresh CloFecAct
	    if (adt_CatIngBr_reqpart == "S")
	    	clearfield CloCoefPartProv
	    	refresh CloCoefPartProv
	    end if
	END IF
	adt_prvta_tasa_cod = CloCatCodIB
	call post:CDSProvCatTas_load
	SETVALDONE adt_prvta_pcia_cod
	SETVALDONE CloCuit
	REFRESH adt_prvta_pcia_cod
	REFRESH adt_pcia_desc
	/*Si existe sólo una tasa de Ingresos Brutos, la seteo como default - Gabriel Falciola*/
	call CDSUnicaTasaIngBr(adt_prvta_pcia_cod,CloCatCodIB,adt_prvta_vinc_ivaIB,IvaCod)
	if (retval == 0)
		IngBCod = adt_TasCatIB_cattas
		refresh IngBCod
		NEXTFIELD CloCoefPartProv
	else
		NEXTFIELD IngBCod
	end if
END SUB

/*******/
