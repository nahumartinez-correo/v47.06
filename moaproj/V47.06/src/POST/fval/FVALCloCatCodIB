/****o* Pendientes de agrupación/OFB:post:FVALCloCatCodIB
* NOMBRE
*   OFB:post:FVALCloCatCodIB
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postfval.h"

SUB post:FVALCloCatCodIB
	if (CloInscripto != "N")
 		CALL siaf:FVALcampo_requerido
		IF (RETVAL < 0) THEN
			RETURN (-2)
		END IF
	end if
	adt_CatIngBr_cod = CloCatCodIB
	call CDSCatIngBr_load
	if (retval != 0)
		call post:CDSCatIngBr_sel
		if (retval != 0)
			return (-2)
		end if
	end if
	CloCatCodIB = adt_CatIngBr_cod
	clearfield adt_CatIngBr_cod
	clearfield adt_CatIngBr_desc
	refresh CloCatCodIB
	if (CloCatCodIB_aux != CloCatCodIB)
		call CDSingbr_cat_check
		if (retval != 0)
			return (-2)
		end if
	end if
    /*Chequeo en la tabla CatIngBr los atributos que indican cómo deben manejarse
    el porcentaje de participación y el número de inscripción - Gabriel Falciola*/
    if (adt_CatIngBr_reqnum != "S")
    	clearfield CloNroInb
    	protfield CloNroInb
    else
    	unprotfield CloNroInb
    end if
    refresh CloNroInb
END SUB

/*******/
