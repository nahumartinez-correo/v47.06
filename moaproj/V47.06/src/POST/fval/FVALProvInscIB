/****o* Pendientes de agrupación/OFB:post:FVALProvInscIB
* NOMBRE
*   OFB:post:FVALProvInscIB
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postfval.h"

SUB post:FVALProvInscIB
	post:adt_pcia_cod = CloProvin
	call CDSadt_pcia_load
	if (retval != 0)
		call PBProvInscIB
		if (retval != 0)
			return (-2)
		end if
	end if
    /*Chequeo en la tabla CatIngBr los atributos que indican cómo deben manejarse
    el porcentaje de participación y el número de inscripción - Gabriel Falciola*/
    if (adt_CatIngBr_reqpart == "N")
    	CloCoefPartProv = 1
    	protfield CloCoefPartProv
    else
    	clearfield CloCoefPartProv
    	unprotfield CloCoefPartProv
    end if
    refresh CloCoefPartProv
    if (adt_CatIngBr_reqnum == "N")
    	clearfield CloNroInb
    	protfield CloNroInb
    else
    	unprotfield CloNroInb
    end if
    refresh CloNroInb
	clearfield post:adt_pcia_cod
	clearfield IngBCod
	clearfield CloExento
	refresh IngBCod
	refresh CloExento
	setvaldone CloProvin
	adt_prvta_pcia_cod = CloProvin
	adt_prvta_tasa_cod = CloCatCodIB
	call post:CDSProvCatTas_load
	SETVALDONE adt_prvta_pcia_cod
	/*Si existe sólo una tasa de Ingresos Brutos, la seteo como default - Gabriel Falciola*/
	call CDSUnicaTasaIngBr(adt_prvta_pcia_cod,CloCatCodIB,adt_prvta_vinc_ivaIB,IvaCod)
	if (retval == 0)
		IngBCod = adt_TasCatIB_cattas
		refresh IngBCod
		NEXTFIELD CloCoefPartProv
	else
		NEXTFIELD IngBCod
	end if
END SUB

/*******/
