#include "postfval.h"

SUB post:FVALservcaja_relac
	IF (post:adt_servcaja_caja == "CAJA PROPIA" && post:adt_servcaja_relac.numchars == 0)
		CALL siaf:FVALcampo_requerido
		if (retval < 0)
			return -2
		end if
	END IF
END SUB

