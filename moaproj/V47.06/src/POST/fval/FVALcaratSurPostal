#include "postfval.h"

SUB post:FVALcaratSurPostal
    if (post:CaratulaSaca.numchars < 20) then
           let common:csr_string = "Error en la Caratula."
           errmsg common:errmsg_list,FLD:common:csr_string
           return (-2)
    end if
    nextfield ServTrans
END SUB

