#include "postfval.h"

SUB post:FVALimp_retiro
	
	IF (importe_origen <= 0)
		MSGBOX "INGRESE EL IMPORTE QUE DESEA RETIRAR",0x00,"Atención"
		RETURN -2
	END IF

END SUB

