/****o* Pendientes de agrupación/OFB:post:FVALcp
* NOMBRE
*   OFB:post:FVALcp
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postfval.h"

SUB post:FVALcp
    IF (post:adt_opc_disccp == "T")
        IF (cod_postal.numchars == 0) THEN
           let common:csr_string = "Debe ingresar el Código Postal."
           errmsg common:errmsg_list,FLD:common:csr_string
           return (-2)
        ENDIF
    if (siaf:codigo_siaf != 99070) /* No es Devol Pieza */
        /* Verifica que todos los cpos estèn correct. cargados */

        CALL FVALcteOp
        IF (retval < 0)
            return (retval)
        END IF
        call FVALapertura
        IF (retval < 0)
            return (retval)
        END IF
        call FVALclase
        IF (retval < 0)
            return (retval)
        END IF
	  if (siaf:codigo_siaf != 99080) /* en la transferencia no esta el peso */
	       call FVALpeso
      	 IF (retval < 0)
            	return (retval)
	       END IF
	  end if
        if (post:adt_corteop_ctrl == "S") THEN

            /* Carga la tabla DetTmp segun lo cargado anteriormente */

            call CDScargaDetTmp

            if (retval < 0) then
                let common:csr_string = "No existen piezas pendientes con las caract. para completar el Contenedor."
                errmsg common:errmsg_list,FLD:common:csr_string
                return (-2)
            end if
        end if

        /* Habilita el resto de los cpos y protege los actuales */

        CALL MISCprotLLenChdr
        CALL MISCunprotLlenC
    end if
        INIT 33,0
    END IF
END SUB

/*******/
