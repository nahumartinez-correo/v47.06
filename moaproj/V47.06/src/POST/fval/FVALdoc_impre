/****o* Pendientes de agrupación/OFB:post:FVALdoc_impre
* NOMBRE
*   OFB:post:FVALdoc_impre
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "postfval.h"

SUB post:FVALdoc_impre
LOCAL NRO_FACTURA like adt_nro_fact
local auxTic like revTicket
   auxTic = revTicket
   clearfield revTicket
   If ( testflag(tlr:tran_flags,correction) )
      Return
   Endif
   //Valido que el campo tenga algo
   CALL siaf:FVALcampo_requerido
   IF (RETVAL < 0) THEN
	   revTicket = auxTic
       RETURN (-2)
   END IF

   // Solo Puede pasar por aca cuando es una Factura
   Select (IvaCod)
      //Factura A
      Case FACTURAA
      Case FACTURAA2
      Case REGIMSIMP
         //Valido que el Primer Digito sea un 1 (A).
         If ( substr(CurrentField,0,1) != 1  )
            MsgBox "El Primer Digito del Numero de la Factura no es Correcto",OFB_OK,"ERROR"
       		revTicket = auxTic
            Return -2
         Endif
         Break
      //Factura B
      Case TICKFACTB
      Case TICKFACTB2
      Case TICKFACTB3
      Case TRIBNOIDENT
      //Case REGIMSIMP
         If ( substr(CurrentField,0,1) != 2 )
            MsgBox "El Primer Digito del Numero de la Factura no es Correcto",OFB_OK,"ERROR"
            revTicket = auxTic
            Return -2
         Endif
         Break
      Case else  // [PAW] ---> Ver bien!!!
         If ( substr(CurrentField,0,1) != 2 )
            MsgBox "El Primer Digito del Numero de la Factura no es Correcto",OFB_OK,"ERROR"
            revTicket = auxTic
            Return -2
         Endif
         Break
   End Select
   // Valido que sea la misma casa donde estoy operando
   If ( substr(CurrentField,1,4) != common:br_CEC )
      MsgBox "El Centro de Emision de Comprobante de la Sucursal difiere con el de la Factura",OFB_OK,"ERROR"
      revTicket = auxTic
      Return -2
   Endif
   If ( CurrentField.Numchars < 13)
      MsgBox "El Nro. de Factura esta incompleto",OFB_OK,"ERROR"
      revTicket = auxTic
      Return -2
   End If
   If ( CurrentField.Numchars < 14)
      MsgBox "El Nro. de Digito Verificador no fue Ingresado",OFB_OK,"ERROR"
      revTicket = auxTic
      Return -2
   End If
   // Valido Modulo el Digito verificador
   String_Nume50  = DSC:post:peso_impresion
   Call post:MISCcalc_digito_ver(String_Nume50)
   If ((Retval < 0) || (Retval != Substr(Currentfield,14,1))) Then
      ERRMSG LST:common:errmsg_list,siaf:ERRdigito_invalido
      revTicket = auxTic
      RETURN (-2)
   End If
   adt_tipo_formulario   = substr(Currentfield,0,1)
   Call post:CDSprimera_Fact_Ctrl(adt_tipo_formulario)
   NRO_FACTURA = substr(Currentfield,1,12)
   // PJM 26-08-98 porque habia problemas con la resta de los campos numericos
   nro_factura_aux  = adt_nro_fact

   post:nro_factura = NRO_FACTURA
   post:nro_factura = nro_factura - nro_factura_aux
   If ( post:nro_factura <= 0.0  )
      MsgBox "El Numero de Factura tiene que ser Mayor a la Ultima Factura de Dia Anterior",OFB_OK,"ERROR"
      revTicket = auxTic
      Return -2
   Endif
   // PJM 6-12-98 se le agrego un limite para que no pueda ser muy grande por loops.
   // que pasaban en el tesoro.

; [PAW] --> Se saca este control por pedido del correo.
;   Call post:CDSultima_Fact_Ctrl(adt_tipo_formulario,common:br_CEC).
;   nro_factura_aux  = adt_nro_fact.
;   post:nro_factura = NRO_FACTURA.
;   post:nro_factura = nro_factura - nro_factura_aux.
;   If ( post:nro_factura > 100 ).
;      MsgBox "Solo puede haber una diferencia de 100 numeros entre el ultimo nro de factura y el ingresado",OFB_OK,"ERROR".
;      revTicket = auxTic.
;      Return -2.
;   Endif.
   revTicket = auxTic
END SUB

/*******/
