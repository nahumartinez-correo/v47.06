/****o* Pendientes de agrupación/OFB:tes:PBborrar_pase
* NOMBRE
*   OFB:tes:PBborrar_pase
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tespb.h"

SUB tes:PBborrar_pase
	IF (login_dest.numchars == 0 || post:importe_origen == 0) then             
		errmsg lst:common:errmsg_list,"Falta ingresar algún campo"
	    drv:button = drv:last_button
        call drv:but_up_enable
	    return (-1)
    end if    
    
    drv:button = drv:last_button
    call drv:but_down_disable

    if (drv:dt_app == 4) then
        let adt_cheqaux_tesoro = "S"
    else
        let adt_cheqaux_tesoro = "N"
    end if    
    let adt_cheqaux_op = op:op_operator_id
	adt_cheqaux_moneda = 0
	adt_cheqaux_suc = op:op_branch_id
    adt_cheqaux_banco = 0
    adt_cheqaux_nro = 0
   
    call CDScheques_del    
    Call MISClimpia_pase

	drv:button = drv:last_button
    call drv:but_up_enable
END SUB

/*******/
