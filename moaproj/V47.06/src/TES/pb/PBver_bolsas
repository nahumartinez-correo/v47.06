/****o* Pendientes de agrupación/OFB:tes:PBver_bolsas
* NOMBRE
*   OFB:tes:PBver_bolsas
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tespb.h"

/* Estado de las Bolsas 
"X" = Bolsa con diefrencia
"N" = Bolsa no sale
"S" = Bolsa sale
" " = Bolsa no declarada */
SUB tes:PBver_bolsas
    drv:button = drv:last_button	
    CALL drv:but_down_disable
   /*
    let adt_cheqaux_suc = op:op_branch_id
    let adt_cheqaux_moneda = siaf:moneda
    let adt_cheqaux_tesoro = op:op_tesoro
    let adt_cheqaux_tesoro = "N"
    let adt_cheqaux_op = op:op_operator_id
   */
    CALL CDSbolsas_sel
    IF (RETVAL < 0) THEN
        drv:button = 2
        call drv:but_disable
        drv:button = 3
        call drv:but_disable
        drv:button = 4
        call drv:but_enable
        drv:button = drv:last_button
        CALL drv:but_up_enable
        return (0)
    END IF
    ;MOVE adt_depbuzon_importe to siaf:importe
    /*
    if (tes:adt_depbuzon_tipo == "E") then
        protfield siaf:importe2
    else 
        unprotfield siaf:importe2
    end if
    move adt_depbuzon_bolsa to  bolsa
    refresh bolsa
    clearfield siaf:importe2
    refresh siaf:importe2
    if (tes:adt_depbuzon_tipo == "E") then
         ;Si el depósito realizado fue en especies fuede haber varias
        drv:button = 2
        call drv:but_disable
        drv:button = 3
        call drv:but_disable
        drv:button = 4
        call drv:but_disable
        call MISCdeclara_subesp

    end if
    */
    call MISCabre_bolsa    
    /* clearfield siaf:moneda 
    refresh siaf:moneda */
    drv:button = 2
    call drv:but_enable
    drv:button = 3
    call drv:but_enable
    drv:button = 4
    call drv:but_enable
    drv:button = drv:last_button
    CALL drv:but_up_enable
END SUB

/*******/
