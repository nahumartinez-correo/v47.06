#include "tespb.h"

SUB tes:PBmp_ent3
	IF (testflag(tes:acepta_ent,item_3))
		MOVE "S" TO acepta_ent_mp[3]
	ELSE
		MOVE "N" TO acepta_ent_mp[3]
	END IF
END SUB

