/****o* Pendientes de agrupación/OFB:tes:PBsubespecie_vta
* NOMBRE
*   OFB:tes:PBsubespecie_vta
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tespb.h"

SUB tes:PBsubespecie_vta

    drv:button = drv:last_button	
    CALL drv:but_down_disable   
    call drv:all_but_disable
    
    let adt_espv_cod = espv_cod
    let adt_espv_subcod = adt_evaux_subcod
  	call tes:CDS_espv_subcod
    nextfield adt_evaux_subcod
    
    IF (RETVAL == 0) THEN
        fld:adt_evaux_subcod = adt_espv_subcod
        refresh adt_espv_subcod
        refresh adt_espv_valor
        resetvaldone adt_evaux_subcod
        unprotfield adt_evaux_subcod
        ungetkey RT_TAB
    END IF
    call drv:back_but_status
    drv:button = drv:last_button
    CALL drv:but_up_enable
END SUB

/*******/
