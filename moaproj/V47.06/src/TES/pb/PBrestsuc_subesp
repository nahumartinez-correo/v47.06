/****o* Pendientes de agrupación/OFB:tes:PBrestsuc_subesp
* NOMBRE
*   OFB:tes:PBrestsuc_subesp
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tespb.h"

SUB tes:PBrestsuc_subesp
	IF (adt_restesp_esp.NUMCHARS == 0)
		MSGBOX "Primero debe cargar el Código de Especie",0x00,"Error"
		NEXTFIELD adt_restesp_esp
		drv:button = drv:last_button
		CALL drv:but_up_enable
		RETURN (-2)
	END IF
	drv:button = drv:last_button
	CALL drv:but_down_disable
	CLEARFIELD adt_restesp_subesp
	adt_espv_cod = adt_restesp_esp
	CALL CDSsubesp_sel
	IF (RETVAL >= 0) THEN
		LET adt_restesp_subesp = adt_espv_subcod
		REFRESH adt_restesp_subesp
	END IF
	drv:button = drv:last_button
	CALL drv:but_up_enable
END SUB

/*******/
