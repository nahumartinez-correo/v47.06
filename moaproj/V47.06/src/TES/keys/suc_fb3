/****o* Pendientes de agrupación/OFB:tes:suc_fb3
* NOMBRE
*   OFB:tes:suc_fb3
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "teskeys.h"

SUB tes:suc_fb3
/*******
    if ((testflag(common:rt_flags,tran_active)) \
	&& (!testflag(tlr:state_flags,list)) \
	&& (tlr:cctl_payget_start != 0)) then
		errmsg common:errmsg_list,DSC:tlr:ERRmenu_key_dsbld
		return
    end if
*********/
    if (testflag(tlr:state_flags,tran_active)) then
		errmsg common:errmsg_list,DSC:tlr:ERRmenu_key_dsbld
		return
    end if
    drv:last_button = SFB3
    CALL tlr:check_enabled
    IF (RETVAL != 0) THEN
      RETURN 
    END IF 
    drv:button = SFB3
    CALL drv:but_down_disable
    let drv:INDrun_funct = LST:tes:TRAN_suc_activ
    call drv:do_mode
END SUB 

/*******/
