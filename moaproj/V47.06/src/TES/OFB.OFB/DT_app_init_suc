/****o* Pendientes de agrupación/OFB:tes:DT_app_init_suc
* NOMBRE
*   OFB:tes:DT_app_init_suc
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesOFB.OFB.h"

	SUB tes:DT_app_init_suc
	move "sucursal"  to lib:home_product
	if ( testflag(tlr:state_flags,tran_menu))
		return
	END IF
	call lib:STAThide_na
	call lib:STAThide_tlr
	call KEYScustom_suc
	cutscreen drv:desktop_functions,nocursor
	cutscreen drv:desktop_toolbar,nocursor
	cutscreen drv:desktop_sb,nocursor
	joinscreen tes:SCRNsuc_functions,NOCURSOR

        LET drv:button = BUT_PBD  
        CALL  drv:but_down_disable
END SUB

/*******/
