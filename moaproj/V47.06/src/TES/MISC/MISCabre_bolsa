/****o* Pendientes de agrupación/OFB:tes:MISCabre_bolsa
* NOMBRE
*   OFB:tes:MISCabre_bolsa
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesMISC.h"

SUB tes:MISCabre_bolsa

    ;MOVE adt_depbuzon_importe to siaf:importe
    if (tes:adt_depbuzon_tipo == "E") then
            protfield siaf:importe2
    else
        unprotfield siaf:importe2
    end if
    call MISCdatos_bolsa
    move adt_depbuzon_bolsa to  bolsa
    refresh bolsa
    clearfield siaf:importe2
    refresh siaf:importe2
    if (tes:adt_depbuzon_tipo == "E") then
        /* Si el depósito realizado fue en especies puede haber varias */
        /*
        drv:button = 2
        call drv:but_disable
        drv:button = 3
        call drv:but_disable
        drv:button = 4
        call drv:but_disable
        */
        call MISCdeclara_subesp
        if (retval < 0)
                return (-1)
        end if
    end if
END SUB

/*******/
