SUB tes:MISCdisplay_chq2
    /* Muestra la ultima transacci¢n seleccionada o agregada a la tabla auxiliar AuxCheq*/

    fprint cheques_ingresados,"Banco: %-3e Cuenta: %-20e  Nro.Cheq.: %-8e Importe: %-15e Acred:%-3.3e", \
