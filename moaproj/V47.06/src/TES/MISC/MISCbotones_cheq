#include "tesMISC.h"

SUB tes:MISCbotones_cheq
    drv:button = 3
    call drv:but_enable
    drv:button = 4
    call drv:but_enable
    drv:button = 5
    call drv:but_enable
    drv:button = drv:last_button
    CALL drv:but_up_enable
END SUB


