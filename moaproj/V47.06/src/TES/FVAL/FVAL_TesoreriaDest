/****o* Pendientes de agrupación/OFB:tes:FVAL_TesoreriaDest
* NOMBRE
*   OFB:tes:FVAL_TesoreriaDest
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesFVAL.h"

SUB tes:FVAL_TesoreriaDest
IF (!(testflag(tlr:tran_flags,correction))) THEN
    IF (admin:adt_br_branch.NUMCHARS == 0)
        CALL siaf:FVALcampo_requerido
        IF (RETVAL < 0)
            RETURN -2
        END IF
    ELSE
        CALL post:FVAL_br_mas(admin:adt_br_branch)
        IF (RETVAL < 0)
            MSGBOX common:csr_string,0X00,"ERROR"
            CLEARFIELD admin:adt_br_branch
            NEXTFIELD admin:adt_br_branch
            RETURN -2
        ELSE
            FPRINT common:csr_string, "%s", admin:adt_br_branch
		    CALL csr:PickOp PICKOP_SEARCH DSC:admin:PickSucursales common:csr_string //Buscar en el picklist
		    idx = RETVAL
		    IF (idx < 0)
		        MSGBOX "Ingrese una tesorería destino válida!",0X00,"ERROR"
                CLEARFIELD admin:adt_br_branch
                NEXTFIELD admin:adt_br_branch
                RETURN -2
            END IF
        END IF
        SELECT (siaf:codigo_siaf)
        	CASE 8700
        	CASE 8730
        	CASE 9200
        	CASE 9220
        	CASE 9250
        	CASE 9270
        	CASE 9290
        	CASE 9310
            	drv:button = 7
            	CALL drv:but_disable
            	protfield admin:adt_br_branch        	
        		BREAK
        END SELECT
    END IF
END IF
END SUB

/*******/
