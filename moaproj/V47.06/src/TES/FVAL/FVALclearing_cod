/****o* Pendientes de agrupación/OFB:tes:FVALclearing_cod
* NOMBRE
*   OFB:tes:FVALclearing_cod
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesFVAL.h"

SUB tes:FVALclearing_cod
    IF (TESTFLAG(admin:mode_flags,imprimir)) THEN     
        SETVALDONE adt_clearing_cod                                    
        RETURN (0)                                    
    END IF  
    CALL siaf:FVALcampo_no_cero
    IF (RETVAL < 0) THEN
        RETURN (-2)
    END IF
        
    if ((adt_clearing_cod % 24) != 0 ) /* No es múltiplo de 24...*/            
	 msgbox "Debe ser multiplo de 24",0x30,"ERROR"
	 CLEARFIELD adt_clearing_cod
	 RETURN (-2)
    else	
	CALL CDSclearing_load
        IF (RETVAL == 0) THEN
            let adt_clearing_cod = adt_clearing_cod
            REFRESH adt_clearing_cod
            drv:button = 25
            CALL drv:but_disable
            drv:button = 26
            CALL drv:but_enable
            drv:button = 27
            CALL drv:but_disable
            drv:button = 28
            CALL drv:but_disable
        ELSE
            adt_clearing_cod = adt_clearing_cod
            REFRESH adt_clearing_cod
            drv:button = 25
            CALL drv:but_disable
            drv:button = 26
            CALL drv:but_disable
            drv:button = 27
            CALL drv:but_enable
            drv:button = 28
            CALL drv:but_disable
        END IF
    END IF
    setvaldone adt_clearing_cod
END SUB

/*******/
