/****o* Pendientes de agrupación/OFB:tes:FVALsuc_orig
* NOMBRE
*   OFB:tes:FVALsuc_orig
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesFVAL.h"

SUB tes:FVALsuc_orig
    IF (!TESTFLAG(tlr:tran_flags,correction)) THEN
       IF (sucursal.NUMCHARS == 0)
          CALL PBsuc_orig
          IF (RETVAL != 0)
	     RETURN (-2)
          ENDIF
       ELSE
          admin:adt_br_branch = sucursal
          CALL admin:CDSadt_br_load
          
          IF (retval != 0) 
   	     CALL PBsuc_orig
	     IF (retval != 0)
	        RETURN (-2)
  	     ENDIF 
          ELSE
             /*la sucursal seleccionada no puede ser la 
             misma que sucursal activa*/
             if (admin:adt_br_tipo != "SA" && admin:adt_br_tipo != "SN" && \
                 admin:adt_br_tipo != "SF" && admin:adt_br_tipo != "NG" && \
                 admin:adt_br_tipo != "SB" && admin:adt_br_tipo != "SE") then
                 //cgw:se agrego que se pueda seleccionar sucursales del tipo SA,SN,SF,NG,SB,SE.
                 errmsg common:errmsg_list, "El tipo de sucursal no corresponde con la operacion."
                 return (-2)
             end if   
             if (sucursal == common:sucursal_activa)
                errmsg common:errmsg_list, "Debe Seleccionar Otra Sucursal"
                return (-2)
             else
	        ;SETVALDONE sucursal
                ;PROTFIELD sucursal
                ;drv:button = 1
                ;CALL drv:but_disable       	     
	        RETURN (0)
	     end if
          ENDIF
       ENDIF
    ENDIF       
END SUB

/*******/
