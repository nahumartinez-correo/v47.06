/****o* Pendientes de agrupación/OFB:tes:FVALstes_login_dest
* NOMBRE
*   OFB:tes:FVALstes_login_dest
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesFVAL.h"

SUB tes:FVALstes_login_dest
    if (currentfield.NUMCHARS > 0 && tes:tes_login_dest != op:op_login_id)
        /* Se ingreso un login a mano => verifico que pueda ser tesoro */
        let op:adt_login_id = tes:tes_login_dest
		clearfield op:adt_sup_auth
		clearfield op:adt_tes_auth
        call op:CDSadt_op_load
        /* Debse ser cajero y Supervisor */
        if ((op:adt_tlr_auth != "X" || op:adt_tes_auth != "X") || retval < 0) then
       	    common:csr_string = "(L) OPERADOR DESTINO NO PUEDE MANEJAR HABILITACION"
       	    errmsg common:errmsg_list,fld:common:csr_string
       	    return (-2)
        end if
		let tes:tes_operator_id_dest = op:adt_operator_id
		refresh tes:tes_operator_id_dest
    else
        call PBmuestra_posib_stes
        if (retval <= 0) then
            return (-2)
        end if
    end if
END SUB

/*******/
