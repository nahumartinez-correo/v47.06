/****o* Pendientes de agrupación/OFB:tes:FVALrestcli_subesp
* NOMBRE
*   OFB:tes:FVALrestcli_subesp
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesFVAL.h"

SUB tes:FVALrestcli_subesp
	IF (TESTFLAG(admin:mode_flags,imprimir)) THEN
		SETVALDONE adt_restesp_esp
		SETVALDONE adt_restesp_subesp
		SETVALDONE adt_restesp_cli
		RETURN (0)
	END IF

	CALL siaf:FVALcampo_requerido
	IF (RETVAL < 0) THEN
        RETURN (-2)
    END IF
    
    IF (adt_restesp_esp.NUMCHARS == 0)
		MSGBOX "Primero debe cargar el Código de Especie",0x00,"Error"
		NEXTFIELD adt_restesp_esp
		drv:button = drv:last_button
		CALL drv:but_up_enable			
		RETURN (-2)
	END IF
	
	siaf:last_key = LASTKEY    
	fld:adt_espv_cod = fld:adt_restesp_esp
	fld:adt_espv_subcod = fld:adt_restesp_subesp
	CALL CDSsubesp_load
    IF (RETVAL == 0) THEN
        fld:adt_restesp_subesp = fld:adt_espv_subcod
        refresh adt_restesp_subesp
    ELSE	
		CALL CDSsubesp_sel
        IF (LASTKEY == 0x0190) THEN
           RETURN (-2)
        END IF        	
		fld:adt_restesp_subesp = adt_espv_subcod
		refresh adt_restesp_subesp
	END IF
	//setvaldone adt_restesp_subesp
    drv:button = 1
    call drv:but_enable
END SUB

/*******/
