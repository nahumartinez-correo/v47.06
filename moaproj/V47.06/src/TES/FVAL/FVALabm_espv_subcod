/****o* Pendientes de agrupación/OFB:tes:FVALabm_espv_subcod
* NOMBRE
*   OFB:tes:FVALabm_espv_subcod
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesFVAL.h"

SUB tes:FVALabm_espv_subcod
    IF (TESTFLAG(admin:mode_flags,imprimir)) THEN
        setvaldone adt_espv_cod
        setvaldone adt_espv_codabrev
        setvaldone adt_espv_aliciva
        setvaldone adt_espv_formfiscal
        setvaldone adt_espv_deb_cred
        setvaldone adt_espv_cta_cble1
        setvaldone adt_espv_es_giro
        setvaldone adt_espv_transf
        setvaldone adt_espv_cta_ord
        setvaldone adt_espv_perf_cajero
        setvaldone adt_espv_perf_merch
        setvaldone adt_espv_inv
        //setvaldone adt_espv_up
		//ver si acá se hace el setvaldone de las otras pantallas
        RETURN (0)
    END IF
    
    call csr:trimfield(tes:adt_espv_subcod) //para que no acepte blancos
    CALL siaf:FVALcampo_requerido
    IF (RETVAL < 0) THEN
        RETURN (-2)
    END IF

    //cgw:valida caracteres validos
    CALL tes:MISCchar_espv_subcod
    IF (RETVAL < 0) THEN
        RETURN (-2)
    END IF
    	
    CALL CDSespecie_load
    IF (RETVAL == -2) THEN
        CALL CSR:proc_list(170,LST:ABMespecie_fld2s)
        tes:adt_espv_rsuc = "N"
        tes:adt_espv_rcli = "N"
        tes:adt_espv_rup = "N"
        tes:adt_espv_rprov = "N"
        tes:adt_espv_bonifCM = "N"
        PROTFIELD tes:adt_espv_ie_suc
        PROTFIELD tes:adt_espv_ie_up
        PROTFIELD tes:adt_espv_ie_cli
        PROTFIELD tes:adt_espv_ie_prov
        CALL CSR:proc_list(190,LST:ABMespecie_fld2s)
        CALL post:MISCset_sidebar(0, 0, 1, 0)

        tes:aux_espv_cod = tes:adt_espv_cod
        tes:aux_espv_subcod = tes:adt_espv_subcod

        /* habilito proximo */
        drv:button = 9
        CALL drv:but_enable

    ELSE
        IF (RETVAL == 0) THEN
        	CALL tes:MISCinic_restr_esp

            CALL CSR:proc_list(190,LST:ABMespecie_fld2s)
            CALL post:MISCset_sidebar(1, 1, 0, 0)

            tes:aux_espv_cod = tes:adt_espv_cod
            tes:aux_espv_subcod = tes:adt_espv_subcod
        END IF
    END IF

    call adt_to_bck34
    setvaldone adt_espv_subcod
END SUB

/*******/
