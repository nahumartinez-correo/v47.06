/****o* Especies Valorizadas/OFB:tes:FVAL_Especie_buzon_S
* NOMBRE
*   OFB:tes:FVAL_Especie_buzon_S
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   SCR:tes:SCRNdep_ev_buzon
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesFVAL.h"

SUB tes:FVAL_Especie_buzon_S
    if (espv_cod.numchars == 0 && lastkey != RT_TAB) then
        return 0
    end if
    adt_espv_cod = espv_cod
    call tes:CDSadt_esp_buzon
    if (retval < 0) then
        drv:button = 0	
        CALL drv:but_down_disable   
        call drv:all_but_disable

        let adt_espv_buzon = "S" 	
        CALL CDS_espv_cod_dep(espv_cod)
        IF (RETVAL < 0) THEN
            clearfield tes:espv_cod
            refresh tes:espv_cod
            call drv:back_but_status
            drv:button = 0
            CALL drv:but_up_enable
            return (-2)
        end if

        call drv:back_but_status
        drv:button = 0
        CALL drv:but_up_enable
        
    end if        
    refresh espv_cod
    drv:button = 1
    call drv:but_enable
    clearfield adt_evaux_importe
	clearfield adt_evaux_cantidad
	clearfield adt_evaux_subcod
	refresh adt_evaux_importe
	refresh adt_evaux_cantidad
	refresh adt_evaux_subcod
	drv:button = 3
    call drv:but_enable
	drv:button = 4
    call drv:but_disable
	drv:button = 5
    call drv:but_disable 
    
    unprotfield adt_evaux_subcod
    unprotfield adt_evaux_cantidad
    unprotfield adt_evaux_importe
    unprotfield tmov_login_orig
    setvaldone espv_cod    
END SUB
        
