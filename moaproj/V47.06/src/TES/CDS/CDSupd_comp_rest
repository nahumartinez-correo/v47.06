/****o* Pendientes de agrupación/OFB:tes:CDSupd_comp_rest
* NOMBRE
*   OFB:tes:CDSupd_comp_rest
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesCDS.h"

SUB tes:CDSupd_comp_rest(mone,bco,cheqcta,nro,cod_int,nro_tar,stat,ident,subid)

    call post:MISCcargo_valor(mone,bco,cheqcta,nro,cod_int,nro_tar)

    teso_cheq_estado = stat
    teso_cheq_id     = ident
    teso_cheq_sub_id = subid
    teso_cheq_termi_win = termi_win
        
    Cds select from dsc:TesoCheq_tbl becomes dsc:TesoCheq_ctx where \
    tes:teso_cheq_suc    == op:op_branch_id      && \
    tes:teso_cheq_moneda == tes:teso_cheq_moneda && \
    tes:teso_cheq_nro    == tes:teso_cheq_nro    && \
    tes:teso_cheq_cuenta == tes:teso_cheq_cuenta && \
    tes:teso_cheq_banco  == tes:teso_cheq_banco && \
    tes:teso_cheq_termi_win == tes:teso_cheq_termi_win
        
	CDS FINDFIRST DSC:tes:TesoCheq_ctx LOCK

	lib:err_retval = CDSRETVAL
	IF (lib:err_retval == 0)
	   CDS CLEARBUFFER DSC:TesoCheq_ctx
	   CDS APPEND DSC:TesoCheq_ctx fld:teso_cheq_estado fld:teso_cheq_id fld:teso_cheq_sub_id
	   CDS UPDATE DSC:TesoCheq_ctx LOCK
	   CDS UNLOCK DSC:TesoCheq_ctx
	   CDS ENDSELECT DSC:TesoCheq_ctx
	ELSE
	   CDS ENDSELECT DSC:TesoCheq_ctx
	   return (-2)
	END IF
END SUB

/*******/
