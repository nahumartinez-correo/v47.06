#include "tesCDS.h"

SUB tes:CDSTES99512_append
    
    call post:CDSTRAN99512_append
    
    CDS APPEND DSC:post:CONPresuDeta \
	tes:adt_espv_aliciva as post:item_iva \
	array post:item_opc_iva  
	
	call CDSTRAN8500_append
    	
END SUB

