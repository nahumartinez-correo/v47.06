/****o* Tesorería/OFB:tes:PRES_8700
* NOMBRE
*   OFB:tes:PRES_8700
* DESCRIPCION
*   PreDisplay de la transaccion de Salida de Efectivo de Tesoro a Tesoreria Provincial (8700)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   LST:tes:TRAN8700
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesPRES.h"

SUB tes:PRES_8700
    IF (!testflag(tlr:tran_flags,correction)) THEN
        if (common:br_modTP != "S") then
            move "No posee habilitación para Modo Tesoreria Regional" to drv:error_msg
	    call OFB:drv:error_msg
	    return (-1)
        end if
    END IF
    setflag tes:exit_prec,do_prec
    siaf:nombre_transac.HIGHLITE = 0
    siaf:codigo_siaf = 8700
    siaf:restr_otra_casa = "N"
    siaf:linea_validacion = "S"
    siaf:exhibe_pantalla = "S"
    siaf:imprime_comprobante = "D"
    siaf:afecta_efectivo = "-"
    tes:es_tesoro = "S"
    tes:viaja_nro_tesoro = "S"
    
    common:cargaPicklist = "S"
    admin:adt_br_branch = ""     
    
    let siaf:INDhost_snd_fv = fld:drv:INDspc_rtn[6]
    call siaf:PRESfecha_hora
    if (retval < 0) then
       return (-1)  /* Sale de la transaccion dentro de un pres */
    endif
    IF (!testflag(tlr:tran_flags,correction)) THEN
       post:tipo_rest = GLOBAL_MONETARIO       
       call tes:PREStes_gral       
    end if
    RETURN (RETVAL)
END SUB

/*******/
