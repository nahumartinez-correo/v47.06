/****o* Pendientes de agrupación/OFB:tes:PRES_7650
* NOMBRE
*   OFB:tes:PRES_7650
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesPRES.h"

SUB tes:PRES_7650
    IF (testflag(tlr:tran_flags,correction)) THEN
        if (siaf:codigo_siaf == 7656) then
            move "(L) Se Debe Reversar por el Inicio" to drv:error_msg
            call OFB:drv:error_msg
            return (-1)
        end if
    END IF

    IF (!testflag(tlr:tran_flags,correction)) THEN
        if (common:br_modCB != "S") then
            move "No posee habilitación para Modo Cuentas Bancarias" to drv:error_msg
	    call OFB:drv:error_msg
	    return (-1)
    end if
       CALL tes:MISCclrfld_comunes
    END IF
    IF ((op:op_sup_auth != "X") && (op:op_tesoro != "S")) THEN
        ERRMSG LST:common:errmsg_list, \
        "No tiene el perfil necesario"
        RETURN (-1)
    END IF
       CALL tes:MISCdia_laborable(common:system_date)
    IF (RETVAL < 0) THEN
    	ERRMSG LST:common:errmsg_list, \
        "Hoy no es un día laborable para la sucursal."
        RETURN (-1)
    ENDIF
    setflag tes:exit_prec,do_prec
    siaf:nombre_transac.HIGHLITE = 0
    siaf:codigo_siaf             = 7650
    siaf:restr_otra_casa         = "N"
    siaf:linea_validacion        = "S"
    siaf:exhibe_pantalla         = "S"
    siaf:imprime_comprobante     = "D"
    siaf:afecta_efectivo         = "-"
    tes:es_tesoro                = "S"
    tes:viaja_nro_tesoro         = "N"
    let siaf:INDhost_snd_fv = fld:drv:INDspc_rtn[6]

    call siaf:PRESfecha_hora
    if (retval < 0) then
       return (-1)  /* Sale de la transaccion dentro de una pres */
    endif
    let tes:lugar_cuenta = 1
    RETURN (RETVAL)
END SUB

/*******/
