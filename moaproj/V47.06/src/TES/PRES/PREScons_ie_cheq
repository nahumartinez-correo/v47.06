#include "tesPRES.h"

SUB tes:PREScons_ie_cheq
    siaf:nombre_transac.HIGHLITE = 0
    siaf:nombre_transac = "CONSULTA DE PASES CHEQUES"
    tlr:tran_mnemonic = "LCHEQIE"
    drv:descrip = "LCHEQIE"
    siaf:codigo_siaf = 0
    siaf:tiene_reverso = "N"
    siaf:posible_off = "S"
    siaf:restr_otra_casa = "N"
    siaf:linea_validacion = "N"
    siaf:exhibe_pantalla = "N"
    siaf:imprime_comprobante = "N"
    let siaf:linea_pantalla = "    CONSULTA DE PASES  DE CHEQUES"
	if (drv:dt_app != 4) then
		let drv:INDscreen[drv:screen_active] = SCR:siaf:SCRNencabe_alto
	end if
      
    siaf:INDhost_snd_fv = fld:drv:INDspc_rtn[6]
    CALL siaf:PRESfecha_hora
    return (retval)
END SUB

