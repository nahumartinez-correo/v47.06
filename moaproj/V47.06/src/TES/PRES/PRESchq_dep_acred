/****o* Pendientes de agrupación/OFB:tes:PRESchq_dep_acred
* NOMBRE
*   OFB:tes:PRESchq_dep_acred
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesPRES.h"

SUB tes:PRESchq_dep_acred

    if (common:br_modCB != "S") then
        move "No posee habilitación para Modo Cuentas Bancarias" to drv:error_msg
	call OFB:drv:error_msg
	return (-1)
    end if

    IF (!testflag(tlr:tran_flags,correction)) THEN
       CALL tes:MISCclrfld_comunes
    END IF
    protfield common:sucursal_activa
    siaf:codigo_siaf = 0
    siaf:tiene_reverso = "N"
    siaf:posible_off = "S"
    siaf:restr_otra_casa = "N"
    siaf:linea_validacion = "N"
    siaf:exhibe_pantalla = "N"
    siaf:imprime_comprobante = "N"
    lugar_cuenta = 1
    INIT 39,0
    siaf:modo = "SUCURSAL"
    siaf:codigo_siaf = 0
    siaf:nombre_transac = "CONSULTA DE CHEQUES DEPOSITADOS POR ACRED."
    siaf:linea_pantalla = "CONSULTA DE CHEQUES DEPOSITADOS POR ACRED."
END SUB


