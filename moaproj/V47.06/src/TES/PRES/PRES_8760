/****o* Pendientes de agrupación/OFB:tes:PRES_8760
* NOMBRE
*   OFB:tes:PRES_8760
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesPRES.h"

SUB tes:PRES_8760 
    setflag tes:exit_prec,do_prec
    siaf:nombre_transac.HIGHLITE = 0
    siaf:codigo_siaf = 8760
    siaf:restr_otra_casa = "N"
    siaf:linea_validacion = "S"
    siaf:exhibe_pantalla = "S"
    siaf:imprime_comprobante = "D"
    siaf:afecta_efectivo = "-"
    tes:es_tesoro = "S"
    tes:viaja_nro_tesoro = "S"
    let siaf:INDhost_snd_fv = fld:drv:INDspc_rtn[6]
    call siaf:PRESfecha_hora
    if (retval < 0) then
       return (-1)  /* Sale de la transaccion dentro de un pres */
    endif
    if (!testflag(tlr:tran_flags,correction)) then
       CALL ABMbuzon_clear
       post:tipo_rest = GLOBAL_NO_MONETARIO
       call tes:PREStes_gral
    end if
    return (retval)
END SUB

