/****o* Pendientes de agrupación/OFB:tes:JNLentrada
* NOMBRE
*   OFB:tes:JNLentrada
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "tesJNL.h"

SUB tes:JNLentrada
    if (!testflag(common:rt_flags,reentry_active) && \
        !testflag(tlr:tran_flags,correction))
           FPRINT tes:referencia "%06.6d%04.4d",op:op_operator_id,tlr:jnl_sequence
    endif

    cds append dsc:tlr:CONjnl_write \
        FLD:tes:tmov_login_orig     \
        FLD:tes:den_texto[0]        \
        FLD:tes:den_texto[1]        \
        FLD:tes:den_texto[2]        \
        FLD:tes:den_texto[3]        \
        FLD:tes:den_texto[4]        \
        FLD:tes:den_texto[5]        \
        FLD:tes:den_texto[6]        \
        FLD:tes:den_texto[7]        \
        FLD:tes:den_texto[8]        \
        FLD:tes:den_texto[9]        \
        FLD:tes:den_texto[10]       \
        FLD:tes:den_texto[11]       \
        FLD:tes:den_texto[12]       \
        FLD:tes:den_texto[13]
    cds append dsc:tlr:CONjnl_write \
        FLD:tes:cantidad[0]         \
        FLD:tes:cantidad[1]         \
        FLD:tes:cantidad[2]         \
        FLD:tes:cantidad[3]         \
        FLD:tes:cantidad[4]         \
        FLD:tes:cantidad[5]         \
        FLD:tes:cantidad[6]         \
        FLD:tes:cantidad[7]         \
        FLD:tes:cantidad[8]         \
        FLD:tes:cantidad[9]         \
        FLD:tes:cantidad[10]        \
        FLD:tes:cantidad[11]        \
        FLD:tes:cantidad[12]        \
        FLD:tes:cantidad[13]        \
        FLD:tes:importes[0]         \
        FLD:tes:importes[1]         \
        FLD:tes:importes[2]         \
        FLD:tes:importes[3]         \
        FLD:tes:importes[4]         \
        FLD:tes:importes[5]         \
        FLD:tes:importes[6]         \
        FLD:tes:importes[7]         \
        FLD:tes:importes[8]         \
        FLD:tes:importes[9]         \
        FLD:tes:importes[10]        \
        FLD:tes:importes[11]        \
        FLD:tes:importes[12]        \
        FLD:tes:importes[13]        \
        FLD:tmov_hora_orig          \
        FLD:tmov_sequence_orig      \
        FLD:tmov_login_dest         \
        FLD:tmov_op_id_orig         \
        FLD:siaf:importe            \
        FLD:tes:es_tesoro           \
        FLD:tes:viaja_nro_tesoro    \
        FLD:tes:referencia          \
        FLD:tes:tmov_marca_ing      \
        FLD:tes:es_reenvio
		call post:MISCcampos_reporte(0,1,siaf:importe,NO_TARIFA)
		CALL csr:proc_list(F_APPEND,DSC:tlr:CONjnl_write,LST:post:LISTreporte,0)

END SUB

