/****o* Pendientes de agrupación/OFB:giros:JNLDesctoReemb
* NOMBRE
*   OFB:giros:JNLDesctoReemb
* DESCRIPCION
*   Se encarga de añadir los campos del journal
* ENTRADAS
* SALIDAS
* IMPACTOS
* REFERENCIADO POR
*   giros:TRANDesctoReemb
* DOCUMENTACION
* SOURCE
*/
#include "girosjnl.h"

SUB giros:JNLDesctoReemb
local ind like siaf:i
local x like siaf:i
    Cds Append DSC:tlr:CONjnl_write  	\
    siaf:importe    					\
    post:NroCompPago 					\
    post:NroCompPago2					\
	post:ImporteConIva					\
	post:ImporteIva						\
	post:ImporteSinIva					\
	post:cod_agrupacion
    
    cds append DSC:tlr:CONjnl_write  	\
   	Serie_fld \
	NroGiro_fld  \
	digito_fld
	x = 0
	while (x < 10)
	    CDS APPEND DSC:tlr:CONjnl_write \
	    FLD:post:track[x]
	    x = x + 1
   	end while
   	x = 0
   	;PS9824
   	if (post:track[1].numchars == 0) then
   	    post:importe_crem[x] = siaf:importe
   	end if
	while (x < 10)
	    CDS APPEND DSC:tlr:CONjnl_write \
	    FLD:post:importe_crem[x]
	    x = x + 1
   	end while
   	
    // PJM 10-08-1999 agregado para que no quede diferente el journal de una transaccion del journal que se hace en la anulacion
    Call post:JNLcamp_nota_credito
    call post:MISCcampos_reporte(0,1,post:ImporteSinIva,NO_TARIFA)
    CALL csr:proc_list(F_APPEND,DSC:tlr:CONjnl_write,LST:post:LISTreporte,0)	    
    
END SUB

/*******/
