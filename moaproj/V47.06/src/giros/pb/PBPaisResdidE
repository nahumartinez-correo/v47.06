/****o* Pendientes de agrupación/OFB:giros:PBPaisResdidE
* NOMBRE
*   OFB:giros:PBPaisResdidE
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girospb.h"

/* comienzo de W.U */
SUB giros:PBPaisResdidE
    CALL MISCpaisResidE
    lib:err_retval = RETVAL
   
	IF (CodPaisResidEmis == "AR")
		UNPROTFIELD CodProvEmis_fld
		ENABLECONTROL drv:PBcb0
		PROTFIELD ProvSucEmis_fld
	ELSE
		CLEARFIELD CodProvEmis_fld
		REFRESH CodProvEmis_fld	
		PROTFIELD CodProvEmis_fld
		DISABLECONTROL drv:PBcb0
		UNPROTFIELD ProvSucEmis_fld		
	END IF
	    
    drv:button = drv:last_button
    CALL drv:but_up_enable
    RETURN (lib:err_retval)
END SUB

/*******/
