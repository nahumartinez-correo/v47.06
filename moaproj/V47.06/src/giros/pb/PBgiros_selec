/****o* Pendientes de agrupación/OFB:giros:PBgiros_selec
* NOMBRE
*   OFB:giros:PBgiros_selec
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girospb.h"

SUB giros:PBgiros_selec()

   /********************************************************************************************************/
   /*                                                                                                      */
   /* [PAW - 05/02/99]      Esta rutina se encarga de visualizar el Combo y setear los botones.            */
   /*                        -) Suma el Iva que se descontó si había alguno seleccionado previamente.      */
   /*                        -) Si la selección es exitosa, resta el Iva del registro seleccionado.        */
   /*                                                                                                      */
   /* Salidas:                                                                                             */
   /* - retval:   Devuelve (0) en caso de selección NO exitosa.                 						   */
   /*                                                                                                      */
   /********************************************************************************************************/

    drv:button = drv:last_button
    CALL drv:but_down_disable
	CALL CDSgiros_sel
    IF (RETVAL < 0) THEN
        drv:button = 2
        call drv:but_enable
        drv:button = 3
        call drv:but_enable
        drv:button = 4
        call drv:but_disable
        drv:button = 5
        call drv:but_disable
        drv:button = drv:last_button
        CALL drv:but_up_enable
        return (0)
    END IF

    call ABMgiros_refresh
    CALL MISCdisplay_giros
	call giros:FVALimporte_giroChq
    drv:button = 2
    call drv:but_enable
    drv:button = 3
    call drv:but_disable
    drv:button = 4
    call drv:but_enable
    drv:button = 5
    call drv:but_enable
    drv:button = drv:last_button
    CALL drv:but_up_enable
END SUB

/*******/
