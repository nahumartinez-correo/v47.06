/****o* Pendientes de agrupación/OFB:giros:PBclocuit
* NOMBRE
*   OFB:giros:PBclocuit
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girospb.h"

SUB giros:PBclocuit()


   /********************************************************************************************************/
   /*                                                                                                      */
   /* [PAW - 03/03/99]   Esta rutina se encarga de Visualizar el combo de números de documentos y luego    */
   /*                    actualiza los campos relacionados.                                                */
   /*                                                                                                      */
   /********************************************************************************************************/

    drv:button = drv:last_button
    CALL drv:but_down_disable

    CALL post:CDSadt_clocuit_sel
    IF (RETVAL == 0) THEN
       post:adt_TipoDoc_tipo = post:CloTipDoc
    else
    	drv:button = drv:last_button
    	call post:CDSadt_clocuit_sel
    	CALL drv:but_up_enable
       	RETURN (retval)
    END IF
	refresh post:CloCuit
	refresh post:adt_TipoDoc_tipo
	refresh post:adt_TipoDoc_desc
	refresh post:CliRaz
	call giros:FVALcliRaz

	if (post:CloCuit.NUMCHARS == 0)
		RESETVALDONE post:CloCuit
		return (-1)
	else
		SETVALDONE post:CloCuit
	endif

    drv:button = drv:last_button
    CALL drv:but_up_enable

	call MISCconcatenoDir

	UNPROTFIELD post:adt_TipoDoc_tipo
END SUB

/*******/
