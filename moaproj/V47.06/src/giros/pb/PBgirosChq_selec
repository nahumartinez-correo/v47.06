/****o* Pendientes de agrupación/OFB:giros:PBgirosChq_selec
* NOMBRE
*   OFB:giros:PBgirosChq_selec
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girospb.h"

/* Inicio  de Emision de GP/GT con medio de pago ch/ */
SUB giros:PBgirosChq_selec()

   /********************************************************************************************************/
   /*                                                                                                      */
   /* [PAW - 06/02/99]      Esta rutina se encarga de visualizar el Combo y setear los botones.            */
   /*                                                                                                      */
   /* Salidas:                                                                                             */
   /* - retval:   Devuelve (0) en caso de selección NO exitosa.                 						   */
   /*                                                                                                      */
   /********************************************************************************************************/

    drv:button = drv:last_button
    CALL drv:but_down_disable
	CALL CDSgirosChq_sel
    IF (RETVAL == -1) THEN  /*Salio por picklist vacio*/
    	drv:button = 8
    	call drv:but_enable
    	drv:button = 9
    	call drv:but_enable
    	drv:button = 10
    	call drv:but_disable
    	drv:button = 11
    	call drv:but_disable
        drv:button = drv:last_button
        CALL drv:but_up_enable
        return (0)
    END IF
    IF (RETVAL == -2) THEN  /*Salio por ESC*/
    	drv:button = 8
    	call drv:but_disable
    	drv:button = 9
    	call drv:but_disable
    	drv:button = 10
    	call drv:but_disable
    	drv:button = 11
    	call drv:but_disable
        drv:button = drv:last_button
        CALL drv:but_up_enable
        return (0)
    END IF
    

    call ABMgirosChq_refresh
    CALL MISCdisplay_girosChq

    drv:button = 8
    call drv:but_disable
    drv:button = 9
    call drv:but_disable
    drv:button = 10
    call drv:but_enable
    drv:button = 11
    call drv:but_enable
    drv:button = drv:last_button
    CALL drv:but_up_enable
END SUB

/*******/
