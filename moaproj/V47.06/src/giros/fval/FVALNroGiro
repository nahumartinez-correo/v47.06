/****o* Pendientes de agrupación/OFB:giros:FVALNroGiro
* NOMBRE
*   OFB:giros:FVALNroGiro
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girosfval.h"

SUB giros:FVALNroGiro
    IF ((NroGiro_fld.NUMCHARS == 0 || NroGiro_fld == 0) && (Serie_fld.NUMCHARS == 0)) THEN
      MSGBOX "INGRESE SERIE Y NRO DE GIRO",0x00,"ERROR"
      NEXTFIELD Serie_fld 
      RETURN -2
    ELSE 
      IF ((NroGiro_fld.NUMCHARS != 0 && NroGiro_fld != 0) && (Serie_fld.NUMCHARS == 0)) THEN
		MSGBOX "INGRESE SERIE",0x00,"ERROR"
		NEXTFIELD Serie_fld 
		RETURN -2
      ELSE
	      IF ((NroGiro_fld.NUMCHARS == 0 || NroGiro_fld == 0) && (Serie_fld.NUMCHARS != 0)) THEN
	        MSGBOX "INGRESE NRO DE GIRO ",0x00,"ERROR"
	        NEXTFIELD NroGiro_fld
	        RETURN -2
	      ELSE
			SETVALDONE NroGiro_fld
	      END IF 
      END IF 
    END IF 
END SUB 

/*******/
