/****o* Pendientes de agrupación/OFB:giros:FVALCuitCuilCdi_WU
* NOMBRE
*   OFB:giros:FVALCuitCuilCdi_WU
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girosfval.h"

SUB giros:FVALCuitCuilCdi_WU
	
	IF (CodNacionEmis_inter == "AR")
		IF (CuitCuilCdiEmis_WI.numchars == 0)
			CALL siaf:FVALcampo_requerido
			IF (RETVAL < 0)
				RETURN -2
			END IF
		END IF
	END IF
	
	IF (NroCuitEmis_fld.numchars > 0 && CuitCuilCdiEmis_WI.numchars == 0)
		MSGBOX "Si ingresa  el Nro. de Documento Tributario debe ingresar el Tipo",0x00,"Atencionn"
		return -2
	END IF

	IF (CuitCuilCdiEmis_WI.numchars > 0)	
	    PICKVERIFY CURRENTFIELD,PICCuitCuilCdi
    	IF (RETVAL < 0) THEN
    		ERRMSG LST:common:errmsg_list,post:ERRopcion_invalida
	    	RETURN (-2)
    	END IF
	    IF (RETVAL >= 0) then
    	    MOVE substr(common:csr_string,0,4) TO CuitCuilCdiEmis_WI
        	REFRESH CuitCuilCdiEmis_WI 
	    END IF
	    SELECT (CuitCuilCdiEmis_WI)
	    	CASE "CUIT"
	    		TipoDocTribut_WI = "I"
	    		break
	    	CASE "CUIL"
	    		TipoDocTribut_WI = "J"
	    		break
	    	CASE "CDI"
	    		TipoDocTribut_WI = "K"
	    		break
	    	CASE ELSE 
	    		clearfield TipoDocTribut_WI
	    		break
	    END SELECT
	    //SETVALDONE CuitCuilCdiEmis_WI
	END IF
   	RETURN (0)
 END SUB

/*******/
