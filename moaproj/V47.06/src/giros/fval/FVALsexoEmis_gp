/****o* Pendientes de agrupación/OFB:giros:FVALsexoEmis_gp
* NOMBRE
*   OFB:giros:FVALsexoEmis_gp
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girosfval.h"

SUB giros:FVALsexoEmis_gp
	clearfield common:csr_string
    IF (TESTFLAG(admin:mode_flags,imprimir)) THEN
        SETVALDONE CURRENTFIELD
        RETURN (0)
    ELSE
    	drv:button = drv:last_button
    	CALL drv:but_down_disable
        siaf:last_key = LASTKEY
        PICKVERIFY CURRENTFIELD,PICsexo_GP,update
        IF (LASTKEY == 0x018B) THEN
            drv:button = drv:last_button
    		CALL drv:but_up_enable
          	LASTKEY = siaf:last_key
        END IF
        IF (RETVAL < 0) THEN
          	ERRMSG LST:common:errmsg_list,post:ERRopcion_invalida
          	drv:button = drv:last_button
    		CALL drv:but_up_enable
          	RETURN (-2)
        END IF
        REFRESH CURRENTFIELD
        DescSexoEmis_gp = SUBSTR(common:csr_string,4,15)
        Refresh DescSexoEmis_gp
   		drv:button = drv:last_button
    	CALL drv:but_up_enable        
        RETURN (0)
    END IF
END SUB

/*******/
