/****o* Pendientes de agrupación/OFB:giros:RCVE70383
* NOMBRE
*   OFB:giros:RCVE70383
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girosrcve.h"

SUB giros:RCVE70383

	call  RCVEhdr_giros
	if (hcomm:tran_status == HOST_ACCEPTED ) then 
		call  RCVEgral_giros
	   	if (retval < 0) THEN
       		return (-2)
	   	end if
		//
		cons_nro_ctrol = ClaveMTC_comm
		//
	   	clie_pin_fld = clie_pin_comm
	   	clie_pin_text_fld = clie_pin_text_comm
	   	//
		if (consulta ==  0 ) then
			fprint giros:mensaje_transaccion "TRANSACCION  ACEPTADA ( MTCN: %s )",cons_nro_ctrol
			if (!testflag(tlr:controla_marca,cancel))
	        	IF (TESTFLAG(common:rt_flags,batch_mode) && (batch:batch_options == "REC_CAIDAS_VUELO")) THEN
					FPRINT common:csr_string_171,"%s\n . Hora: %e ",mensaje_transaccion,common:time_of_day
					CALL batch:MISCgeneraLog(common:csr_string_171,"AMBOS","N")
				ELSE 
					MSGBOX mensaje_transaccion,OFB_OK,"Atencion"
				END IF	        				
			end if
		else //consulta = 1
			//actualizo el new_mtn en la PresuDet
			new_mtcn_WI = new_mtcn_WI_comm
			CALL CDSappend_newMTCN
			
		end if
    else
   		return (-2)
	end if  
END SUB

