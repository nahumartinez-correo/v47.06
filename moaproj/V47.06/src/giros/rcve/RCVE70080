#include "girosrcve.h"

Sub giros:RCVE70080
   Call RCVEhdr_giros 
   If (hcomm:tran_status == HOST_ACCEPTED) 
      CALL RCVEgral_giros
      IF (RETVAL < 0) THEN
        RETURN (-2)
      END IF 
    END IF 
End Sub 

