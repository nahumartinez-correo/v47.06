#include "girosrcve.h"

SUB giros:RCVE70220
    CALL RCVEhdr_giros
	IF (hcomm:tran_status == HOST_ACCEPTED ) 
		Call RCVEgral_giros 
		IF (RETVAL < 0) THEN
	        RETURN (-2)
      END IF 
    END IF 
END SUB 

