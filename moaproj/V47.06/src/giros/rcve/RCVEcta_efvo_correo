/****o* Pendientes de agrupación/OFB:giros:RCVEcta_efvo_correo
* NOMBRE
*   OFB:giros:RCVEcta_efvo_correo
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girosrcve.h"


SUB giros:RCVEcta_efvo_correo
	CALL RCVEhdr_giros
	IF (hcomm:tran_status == HOST_ACCEPTED) THEN
		CALL RCVEgral_giros
		IF (RETVAL < 0) THEN
			RETURN (-2)
		ELSEIF (limite_emis_comm == "1") then
			IF (TESTFLAG(common:rt_flags,batch_mode) && (batch:batch_options == "REC_CAIDAS_VUELO")) THEN
				FPRINT common:csr_string_171,"Se está por superar el monto máximo de Emisión permitido a la Sucursal\n . Hora: %e ",common:time_of_day
				CALL batch:MISCgeneraLog(common:csr_string_171,"INFO","N")
			ELSE 
				msgbox "SE ESTA POR SUPERAR\r\nEL MONTO MAXIMO DE EMISION\r\nPERMITIDO A SU SUCURSAL",OFB_OK,"Aviso"
			END IF
		END IF 
		if ( giros:consulta == 0 )
	        IF (TESTFLAG(common:rt_flags,batch_mode) && (batch:batch_options == "REC_CAIDAS_VUELO")) THEN
				FPRINT common:csr_string_171,"%s\n . Hora: %e ",mensaje_transaccion,common:time_of_day
				CALL batch:MISCgeneraLog(common:csr_string_171,"AMBOS","N")
			END IF	        
		end if
	END IF 
END SUB 

/*******/
