/****o* Pendientes de agrupación/OFB:giros:PCOL70045
* NOMBRE
*   OFB:giros:PCOL70045
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girospcol.h"

SUB giros:PCOL70045

	protfield totalRestante
	protfield totalAcreditado

	if (post:estado_presu == 3)
		totalRestante = totalRestante + ImporteNeto
		refresh totalRestante
	end if

	if (totalAcreditado == 0 && (post:estado_presu == 1 || post:estado_presu == 2))
		POPUP giros:SCRN70324 -,PCOL70324,SVAL70324
		if (LASTKEY == RT_ESC)
			cantidad = 0
			totalAcreditado = 0
	        	ungetkey RT_ESC
    	    		return
    		endif
	endif

	totalAcViejo = totalRestante

	if (totalRestante == 0 && post:estado_presu < 3 )
		msgbox "No le resta crédito de cheques para los giros!", OFB_OK ,"Aviso"
        	ungetkey RT_ESC
   	    return
	end if

	disablecontrol drv:PBcb0 
    	disablecontrol drv:PBcb1 
    	call post:PCOLpresu
    	call giros:MISCImpAbonadoCheq
    	call giros:MISCmed_pag_gir
	if (retval == 0 )
		return -2
	end if

END SUB 

/*******/
