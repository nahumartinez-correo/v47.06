#include "girosmisc.h"

SUB giros:MISC70831_subtotP(cant_recibidos)
    siaf:nota_1 =  \
"--------------------------------------------------------------------------- \
-------------"
    CALL CSR:f_saaddpl(DSC:odbc:pick,siaf:nota_1)
    FPRINT siaf:nota_1,"TOTAL DE CHEQUERAS RECIBIDAS: %d",cant_recibidos
    CALL CSR:f_saaddpl(DSC:odbc:pick,siaf:nota_1)
END SUB 

