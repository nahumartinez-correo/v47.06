/****o* Giros Masivos/OFB:giros:MISClst_CEC_env_gm
* NOMBRE
*   OFB:giros:MISClst_CEC_env_gm
* DESCRIPCION
*   Genera Reporte de CEC enviados para la sesion.
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   RETURN (-1) No existen Giros CEC enviados.
*   RETURN (-2) Problemas con la impresion del listado.
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   giros:MISCverif_rep_env_gm
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "girosmisc.h"

SUB giros:MISClst_CEC_env_gm

    //cgw: Reporte de CEC enviados.
    
    CDS SELECT FROM DSC:TBLSesion_GM BECOMES DSC:CTXSesion_GM WHERE \
    	post:op_operator_id == op:op_operator_id && \
    	giros:nro_sesion_gm == giros:nro_sesion_gm && \
    	giros:estado_gm == "E" && \
    	(giros:tipo_giro_fld == "H")

    CDS FINDFIRST DSC:CTXSesion_GM NOLOCK
	LET lib:err_retval = CDSRETVAL
	IF (lib:err_retval < 0) THEN
        CDS ENDSELECT DSC:CTXSesion_GM
        MSGBOX "NO EXISTEN GIROS CEC ENVIADOS PARA LISTAR",OFB_OK,"Aviso"
        RETURN (-1)
	END IF

    FYIMSG LST:common:fyimsg_list,"GENERANDO REPORTE DE GIROS CEC ENVIADOS"
    ;LMB - PS 9780 - 18.06.2008
    ;CALL tes:MISCopen_file("\\te mp\\lisCECenv.lis",1)
    FPRINT common:tmpDirTemporal, "%s%s", common:dirTemporal, "lisCECenv.lis"
    CALL tes:MISCopen_file(common:tmpDirTemporal, 1)

    /* pongo encabezado */
    siaf:indica_calcu = 0 // inicializa cant. de hojas
	siaf:i = 1
	/* Genero el listados */
	FLD:siaf:INDencab_lis = ofb:giros:MISCenc_CEC_env_gm
    CALL giros:MISCenc_CEC_env_gm
    IF (RETVAL == -2) THEN
    	LET common:csr_string = "(L) Error realizando listado"
    	ERRMSG lst:common:errmsg_list,FLD:common:csr_string
    	
    	;LMB - PS 9780 - 18.06.2008
    	;CALL tes:MISCclose_file("\\te mp\\lisCECenv.lis",1)
    	FPRINT common:tmpDirTemporal, "%s%s", common:dirTemporal, "lisCECenv.lis"
    	CALL tes:MISCclose_file(common:tmpDirTemporal, 1)
    	
    	CDS ENDSELECT DSC:CTXSesion_GM
    	RETURN (-2)
    END IF

    CALL giros:MISCwri_CEC_env_gm // lista detalle CEC enviados en la sesion.
    IF (RETVAL == -2) THEN
    	LET common:csr_string = "(L) Error realizando listado"
    	ERRMSG lst:common:errmsg_list,FLD:common:csr_string
    	
    	;LMB - PS 9780 - 18.06.2008
    	;CALL tes:MISCclose_file("\\te mp\\lisCECenv.lis",1)
    	FPRINT common:tmpDirTemporal, "%s%s", common:dirTemporal, "lisCECenv.lis"
    	CALL tes:MISCclose_file(common:tmpDirTemporal, 1)
    	
    	CDS ENDSELECT DSC:CTXSesion_GM
    	RETURN (-2)
    END IF

    CDS ENDSELECT DSC:CTXSesion_GM
    ;LMB - PS 9780 - 18.06.2008
    ;CALL tes:MISCclose_file("\\te mp\\lisCECenv.lis",1)
    ;CALL siaf:MISCfile_print_Land("\\te mp\\lisCECenv.lis","predeterminada")   
    FPRINT common:tmpDirTemporal, "%s%s", common:dirTemporal, "lisCECenv.lis"
    CALL tes:MISCclose_file(common:tmpDirTemporal, 1)
    CALL siaf:MISCfile_print_Land(common:tmpDirTemporal, "predeterminada")                

END SUB

/*******/
