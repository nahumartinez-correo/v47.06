#include "giroslisttots.h"

list giros:TOTS70050 template:TOTS_LIST_EDIT
	member "totals" "0"
	member post:ImporteConIva "+" siaf:total_usoint_vec[36] OFB:post:MODstandardCB "yes"
	member post:ImporteSinIva "+" siaf:sinimp_usoint_vec[36] OFB:post:MODstandardCB "yes"         
	member "1" "+" siaf:cant_usoint_vec[36] OFB:post:MODstandardCB "yes"
    member post:importe_origen "+" siaf:tefvoent_vec[siaf:moneda] OFB:post:MODcaptura_mp "yes"
	member "1" "+" siaf:cefectivo_vec[siaf:moneda] OFB:post:MODstandard "yes"
	member ImporteNeto "+" siaf:total_usoint_vec[37] OFB:post:MODstandard "yes"
	member "1" "+" siaf:cant_usoint_vec[37] OFB:post:MODstandard "yes"
    /* Como no tiene Impuestos en el total sin impuestos totalizo el importe de la tx */
    member ImporteNeto "+" siaf:sinimp_usoint_vec[37] OFB:post:MODstandard "yes"
	
    member post:importe_origen "+" siaf:tefvoent_vec[siaf:moneda] OFB:post:MODcaptura_mp "yes"
    member siaf:importe "+" siaf:tentbase_vec[siaf:moneda] OFB:post:MODcaptura_mp "yes"       
end list    

