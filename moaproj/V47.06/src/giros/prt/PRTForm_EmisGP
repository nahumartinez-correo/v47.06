  FPRINT common:csr_string "%-5.5s %-s\n", common:sucursal_activa,common:br_bank_name
	PRT PRINT EDIT  common:csr_string
	PRT POSITIONABS *870 *3000 
	PRT STYLE 1
	PRT PRINT "NIS Pagadora:"
	PRT POSITIONABS *870 *3620 
	PRT STYLE 0
