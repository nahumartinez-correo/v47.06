/****o* Pendientes de agrupación/OFB:giros:PRTForm_EmisGP
* NOMBRE
*   OFB:pogirosst:PRTForm_EmisGP
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   NI REEMPLAZO DE CHEQUE GIRO POSTAL
* SOURCE
*/
#include "postprt.h"

SUB giros:PRTForm_EmisGP
	LOCAL fexpiracion_emis_wi LIKE giros:FecNacEmis_inter

// \xE1 => á
//      => é
// \xED => í
// \xF3 => ó
//      => ú

//Dimensiono formulario
	PRT SCALE 100
	PRT ORIENTATION 0
	PRT MARGIN *8419 *5953 0 0 0 0 
	PRT STYLE 0
	PRT TEXTFONT 100 
	PRT TEXTFONTNAME "Book Antiqua"
	PRT TEXTSIZE *100 
	
	//ENCABEZADO
	PRT TEXTSIZE *120 
	PRT STYLE 3
	if (siaf:codigo_siaf == 70030) then
	PRT POSITIONABS *270 *2100
	PRT PRINT "EMISION DE GIRO POSTAL"
	else
		PRT POSITIONABS *270 *1850
		PRT PRINT "EMISION DE GIRO POSTAL OFICIO"
	endif
	PRT POSITIONABS *370 *1850
	PRT TEXTSIZE *100 
	PRT STYLE 0
	PRT PRINT "CORREO OFICIAL DE LA REP.ARGENTINA S.A."
	//Fin encabezado
	
	//Recuadro Primera Secciòn 
	PRT PENSIZE *9 *9 
	PRT PENPAT 0
	PRT FRAMERECT *394 *180 *700 *5679
	
//Primera Seccion	- NRO GIRO 
	PRT TEXTSIZE *120 
	PRT STYLE 3
	PRT POSITIONABS *570 *270 
	PRT PRINT "SERIE,NRO Y DÍGITO DEL GIRO:"
	PRT STYLE 0
	PRT POSITIONABS *570 *2100 
	FPRINT common:csr_string, "%s-%s-%s",giros:Serie_fld, giros:NroGiro_fld, giros:digito_fld
	PRT PRINT EDIT  common:csr_string
	
	
	//Recuadro Segunda Secciòn - NIS EMISORA/PAGADORA - OPERADOR - TERMINAL - FECHA - HORA
	PRT PENSIZE *9 *9 
	PRT PENPAT 0
	PRT FRAMERECT *770 *180 *1070 *5679 
	
//Segunda Secciòn - NIS EMISORA/PAGADORA - OPERADOR - TERMINAL - FECHA - HORA
	PRT TEXTSIZE *100
	PRT POSITIONABS *870 *270 
	PRT STYLE 1
	PRT PRINT "NIS Emisora:"
	PRT POSITIONABS *870 *870 
