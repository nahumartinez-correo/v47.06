	PRT PRINT "Código de Operador:"
	PRT POSITIONABS *1000 *1170
	PRT STYLE 0
	PRT PRINT op:op_operator_id
	PRT POSITIONABS *1000 *1500
	PRT STYLE 1
	PRT PRINT "ID Terminal:"
  PRT POSITIONABS *1000 *2100
  PRT STYLE 0
