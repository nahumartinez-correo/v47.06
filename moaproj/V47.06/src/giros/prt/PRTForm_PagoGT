	PRT POSITIONABS *3470 *270 
	PRT PRINT "Localidad:"
	PRT POSITIONABS *3470 *2400 //Alineada con N° 
	PRT PRINT "Provincia:"
	PRT POSITIONABS *3620 *270 
	PRT PRINT "Tel\éfono:"
	PRT POSITIONABS *3620 *2400 //Alineada con N°
	PRT PRINT "Mail:"
	
	
	PRT POSITIONABS *3770 *270 
	PRT PRINT "Actividad/Ocupaci\xF3n:"
	
	PRT POSITIONABS *3930 *270 
	PRT PRINT "Es PEP:"	
	PRT POSITIONABS *3930 *700 
	PRT PRINT "Detalle PEP:"
	PRT POSITIONABS *4080 *270 
	PRT PRINT "Pa\xEDs de Residencia:"
	PRT POSITIONABS *4080 *2400 
	PRT PRINT "Para\xEDso Fiscal:"
	PRT POSITIONABS *4230 *270 
