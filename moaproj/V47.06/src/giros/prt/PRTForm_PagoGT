	PRT POSITIONABS *2650 *1200 
	PRT PRINT giros:DescNacBenef_gp
	PRT POSITIONABS *2650 *3450 
	
	PRT PRINT EDIT giros:DescEstCivilBenef_gp	
	PRT POSITIONABS *2800 *1200 
	PRT PRINT EDIT giros:DescSexoBenef_gp
	
	PRT POSITIONABS *3180 *570 
	PRT PRINT EDIT giros:CalleBenef_fld
	PRT POSITIONABS *3180 *2550
	PRT PRINT giros:NroCalleBenef_fld
	PRT POSITIONABS *3180 *3050 
	PRT PRINT giros:PisoBenef_fld
	PRT POSITIONABS *3180 *3450 
	PRT PRINT giros:DptoBenef_fld
	
	PRT POSITIONABS *3330 *610 
	PRT PRINT EDIT giros:CodPostBenef_fld 
	PRT POSITIONABS *3480 *800 
	PRT PRINT EDIT giros:LocBenef_WU
	PRT POSITIONABS *3480 *2800 
	PRT PRINT EDIT giros:ProvSucBenef_fld
	PRT POSITIONABS *3630 *700 
	PRT PRINT EDIT giros:TelBenef_fld 
	PRT POSITIONABS *3630 *2800 
	PRT PRINT EDIT giros:email_benef_gp 
	PRT POSITIONABS *3780 *1200 
if (giros:OtrosOcupBenef_fld.numchars > 0) then
			PRT PRINT EDIT giros:OtrosOcupBenef_fld 
	else
		  PRT PRINT EDIT giros:OcupacionBenef_fld 
	endif
 PRT POSITIONABS *3930 *600 
	PRT PRINT EDIT giros:CliPepBenef_gp
	PRT POSITIONABS *3930 *1200 
		PRT PRINT EDIT giros:CategoriaPEP_GP
	PRT POSITIONABS *4080 *1200 
	PRT PRINT EDIT giros:PaisResidBenef
	PRT POSITIONABS *4080 *3000
	PRT PRINT EDIT giros:Pfiscal_benef_gp
	PRT POSITIONABS *4230 *1870 
	PRT PRINT EDIT LocProvEstEmiBene_gp
	PRT POSITIONABS *4380 *1400 
	if (MotivoEnvio_GP.numchars != 0) then
		PRT PRINT MotivoEnvio_GP
	else 
		PRT PRINT OtrosMotEnvBenef_gp
	endif
	
		

	//Recuadro Quinta Seccion - Datos del Emisor
	PRT PENSIZE *9 *9 
	PRT PENPAT 0
	PRT FRAMERECT *4550 *180 *5100 *5679 
	
	//Seccion Quinta -	Datos del Emisor
	PRT STYLE 1
	PRT POSITIONABS *4650 *270 
	PRT PRINT "DATOS DEL EMISOR:"
	PRT POSITIONABS *4680 *270 
	PRT PRINT "______________________"

	
	PRT POSITIONABS *4830 *270 
	PRT PRINT "Nombre/Razón Social:"
	PRT POSITIONABS *4830 *2400 
	PRT PRINT "Apellido:"
	PRT POSITIONABS *4980 *270 
	PRT PRINT "Sucursal Emisora:"
	PRT STYLE 0
	PRT POSITIONABS *4830 *1200 
	if (TipoPersona_EmisGP.numchars == 0) then // version anterior
		if (giros:NombreEmi_fld == ".") then
			PRT PRINT EDIT giros:RazonSocEmis_fld
			PRT POSITIONABS *4830 *2900 
		else
			PRT PRINT EDIT giros:NombreEmi_fld
			PRT POSITIONABS *4830 *2900 
			PRT PRINT EDIT giros:ApellidoEmis_fld 
		endif
	else
	if (TipoPersona_EmisGP == "PH")
		PRT PRINT EDIT giros:NombreEmi_fld
		PRT POSITIONABS *4830 *2900 
		PRT PRINT EDIT giros:ApellidoEmis_fld
	else
		PRT PRINT EDIT giros:RazonSocEmis_fld
	endif
	endif
	//Sucursal
	PRT POSITIONABS *4980 *1200 
	CALL giros:CDSbuscaSuc(giros:CodSucEmis_fld)
  FPRINT common:csr_string "%-5.5s %-s\n",giros:CodSucEmis_fld,admin:adt_br_bank_name
	PRT PRINT EDIT common:csr_string

	/*Recuadro Quinta Seccion - Datos del Emisor
	PRT PENSIZE *9 *9 
	PRT PENPAT 0
	PRT FRAMERECT *5050 *180 *5200 *5679 
	PRT STYLE 1
	PRT POSITIONABS *5100 *270
	PRT PRINT  "RECIBO: $"
	PRT STYLE 0
	PRT POSITIONABS *5100 *1200
	PRT PRINT EDIT giros:ImporteNeto*/
	PRT TEXTFONTNAME "Courier New"
//Leyenda GT   
PRT POSITIONABS *5200 *270 
PRT PRINT "COMO BENEFICIARIO DECLARO BAJO JURAMENTO QUE LOS DATOS REGISTRADOS POR MI EN EL PRESENTE"
PRT POSITIONABS *5300 *270 
