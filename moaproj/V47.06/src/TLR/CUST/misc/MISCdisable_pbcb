/****o* Pendientes de agrupación/OFB:tlr:MISCdisable_pbcb
* NOMBRE
*   OFB:tlr:MISCdisable_pbcb
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "custmisc.h"

    sub tlr:MISCdisable_pbcb
	let drv:button = CB0
	CALL  drv:but_disable
	let drv:button = CB1
	CALL  drv:but_disable
	let drv:button = CB2
	CALL  drv:but_disable
	let drv:button = CB3
	CALL  drv:but_disable
	let drv:button = CB4
	CALL  drv:but_disable
	let drv:button = CB5
	CALL  drv:but_disable
	let drv:button = CB6
	CALL  drv:but_disable
	let drv:button = CB7
	CALL  drv:but_disable
	let drv:button = CB8
	CALL  drv:but_disable
	let drv:button = CB9
	CALL  drv:but_disable
	let drv:button = CB10
	CALL  drv:but_disable
	let drv:button = CB11
	CALL  drv:but_disable
	let drv:button = CB12
	CALL  drv:but_disable
	let drv:button = CB13
	CALL  drv:but_disable
	let drv:button = CB14
	CALL  drv:but_disable
	let drv:button = CB15
	CALL  drv:but_disable
	let drv:button = CB16
	CALL  drv:but_disable
	let drv:button = CB17
	CALL  drv:but_disable
	let drv:button = CB18
	CALL  drv:but_disable
	let drv:button = CB19
	CALL  drv:but_disable
end sub

