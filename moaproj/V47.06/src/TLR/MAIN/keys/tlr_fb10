/****o* Pendientes de agrupación/OFB:tlr:tlr_fb10
* NOMBRE
*   OFB:tlr:tlr_fb10
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "mainkeys.h"

SUB tlr:tlr_fb10

#if CORREO_DEBUG == CORREO_ON
	;if (plataf:current_state == CONFIRMATION) then
	call post:MISClogMenues("tlr:tlr_fb10")
	;endif
#endif

	// FGB 14.02.05: Si estoy cerrando el presupuesto (llamando a la Captura) 
	// no dejo que se presion el boton 
	if (plataf:current_state == CLOSING_BUDGET) then
		return
	endif

	let drv:last_button = TFB10
	CALL  tlr:check_enabled
	if ( retval != 0 )
    		return
	END IF
	let drv:button = TFB10
	CALL  drv:but_down_disable
	CALL  drv:calculator
	let drv:button = TFB10
	CALL  drv:but_up_enable
END SUB

/*******/
