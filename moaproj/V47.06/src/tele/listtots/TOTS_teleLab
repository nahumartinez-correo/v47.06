/****l* Pendientes de agrupación/LST:tele:TOTS_teleLab
* NOMBRE
*   LST:tele:TOTS_teleLab
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "telelisttots.h"

LIST tele:TOTS_teleLab template:TOTS_LIST_EDIT
    MEMBER "totals" "0"
    MEMBER siaf:importe "+" siaf:total_usoint_vec[122] OFB:post:MODstandard "yes"
    MEMBER "1" "+" siaf:cant_usoint_vec[122] OFB:post:MODstandard "yes"
    /* Totaliza en el Medio de Pago en importe origen
    unicamente si captura medio de pago */
    MEMBER post:importe_origen "+" siaf:tefectivo_vec[siaf:moneda] OFB:post:MODcaptura_mp "yes"
    MEMBER "1" "+" siaf:cefectivo_vec[siaf:moneda] OFB:post:MODcaptura_mp "yes"
    MEMBER post:cantidad "+" siaf:piezas_usoint_vec[122] OFB:post:MODstandard "yes"
    MEMBER post:ImporteSinIva "+" siaf:sinimp_usoint_vec[122] OFB:post:MODstandard "yes"
    MEMBER post:cantidad "+" siaf:pminor_usoint_vec[122] OFB:post:MODpiezas_minor "yes"
    MEMBER post:importe_origen "+" siaf:tefvoent_vec[siaf:moneda] OFB:post:MODcaptura_mp "yes"
    MEMBER siaf:importe "+" siaf:tentbase_vec[siaf:moneda] OFB:post:MODcaptura_mp "yes"
        /* Agrego Piezas Cliente Medio*/
    MEMBER post:cantidad "+" siaf:pcm_usoint_vec[122] OFB:post:MODCM_Contado "yes"
    MEMBER post:cantidad "+" siaf:pcmCC_usoint_vec[122] OFB:post:MODCM_CC "yes"
    MEMBER post:cantidad "+" siaf:pcmDf_usoint_vec[122] OFB:post:MODCM_Df "yes"
END LIST

