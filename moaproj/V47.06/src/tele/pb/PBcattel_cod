/****o* Pendientes de agrupación/OFB:tele:PBcattel_cod
* NOMBRE
*   OFB:tele:PBcattel_cod
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "telepb.h"

SUB tele:PBcattel_cod
    drv:button = 3
    CALL drv:but_disable
    drv:button = drv:last_button
    CALL drv:but_down_disable    

    CALL tele:CDScatteleg_cod
    IF (RETVAL == 0) THEN
        CALL tele:ABMcatteleg_refresh
        setvaldone tele:adt_cattel_cod
		Call CDSauxcatel_load
		IF (RETVAL < 0)
			CALL post:MISCset_sidebar(1, 1, 0, 0)
		ELSE
			CALL post:MISCset_sidebar(1, 1, 1, 0)
		END IF
    ELSE
 		IF (LASTKEY == RT_ESC) THEN
			Call CDSauxcatel_load
			IF (RETVAL < 0)
	    		CALL post:MISCset_sidebar(0, 0, 0, 0)
	    	END IF
    	ELSE
		    CALL tele:CDScatteleg_load
		    IF (RETVAL == 0) THEN
		        CALL tele:ABMcatteleg_refresh
				Call CDSauxcatel_load
				IF (RETVAL < 0)
					CALL post:MISCset_sidebar(1, 1, 0, 0)
				ELSE
					CALL post:MISCset_sidebar(1, 1, 1, 0)
				END IF
		    ELSE    	
		    	CALL tele:ABMcatteleg_clear2
		    	CALL CSR:proc_list(190,LST:LISTcatteleg_flds2)
				Call CDSauxcatel_load
				IF (RETVAL < 0)
					CALL post:MISCset_sidebar(0, 0, 1, 0)
				ELSE
					CALL post:MISCset_sidebar(1, 1, 1, 0)
				END IF
		    END IF    
		    setvaldone tele:adt_cattel_cod
		    setvaldone tele:adt_cattel_serv
		    setvaldone tele:adt_cattel_cativa
    	END IF
    END IF
    nextfield adt_cattel_serv
    drv:button = 3
    CALL drv:but_enable
    drv:button = drv:last_button
    CALL drv:but_up_enable 

END SUB

/*******/
