/****o* Pendientes de agrupación/OFB:batch:CDSBuscoMaxDumpRep
* NOMBRE
*   OFB:batch:CDSBuscoMaxDumpRep
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "batchCDS.h"

SUB batch:CDSBuscoMaxDumpRep
/*Obtiene los valores máximos en la DumpRep - Gabriel Falciola*/
	CALL MISCgeneraLog("\nSe obtienen valores máximos de la tabla DumpRep\n","INFO","N")
	
	/*Busco la fecha máxima*/
	cds select from dsc:post:DumpRep_tbl becomes dsc:post:DumpRep_ctx
	cds findfirst dsc:post:DumpRep_ctx nolock
	if (cdsretval != 0 && cdsretval != -801)
		select (cdsretval)
		case -802
			FPRINT common:csr_string,"Busqueda de valores máximos fallida por cancelación\n"
	    	CALL MISCgeneraLog(common:csr_string,"AMBOS","N")
			break
		case -803
			FPRINT common:csr_string,"Busqueda de valores máximos fallida, registros lockeados\n"
	    	CALL MISCgeneraLog(common:csr_string,"AMBOS","N")
			break
		end select			
		cds endselect dsc:post:DumpRep_ctx
		return (-2)
	end if
	clearfield post:adt_dump_fecha
	clearfield tlr:jnl_hora_ult_act
	if (cdsretval == 0) then
		cds aggregate dsc:post:DumpRep_ctx max post:adt_dump_fecha
		if (cdsretval == 0) then
			cds extract dsc:post:DumpRep_ctx post:adt_dump_fecha
			cds endselect dsc:post:DumpRep_ctx
		else 
			FPRINT common:csr_string,"Busqueda de valores máximos fallida\n"
	    	CALL MISCgeneraLog(common:csr_string,"AMBOS","N")
			cds endselect dsc:post:DumpRep_ctx
			return (-2)
		end if			
	end if
	if (post:adt_dump_fecha.numchars > 0) then
		FPRINT common:csr_string,"Fecha Máxima: %s\n", post:adt_dump_fecha
	else
		FPRINT common:csr_string,"Fecha Máxima: Ninguna\n"
   	end if
	CALL MISCgeneraLog(common:csr_string,"INFO","N")
	
	if (post:adt_dump_fecha.numchars > 0) then
		/*Busco la hora máxima*/
		cds select from dsc:post:DumpRep_tbl becomes dsc:post:DumpRep_ctx where \
		post:adt_dump_fecha == post:adt_dump_fecha 
		cds findfirst dsc:post:DumpRep_ctx nolock
		if (cdsretval != 0)
			select (cdsretval)
			case -802
				FPRINT common:csr_string,"Busqueda de valores máximos fallida por cancelación\n"
	    		CALL MISCgeneraLog(common:csr_string,"AMBOS","N")
				break
			case -803
				FPRINT common:csr_string,"Busqueda de valores máximos fallida, registros lockeados\n"
	    		CALL MISCgeneraLog(common:csr_string,"AMBOS","N")
				break
			end select			
			cds endselect dsc:post:DumpRep_ctx
			return (-2)
		end if
		if (cdsretval == 0) then
			cds aggregate dsc:post:DumpRep_ctx max tlr:jnl_hora_ult_act
			if (cdsretval == 0) then
				cds extract dsc:post:DumpRep_ctx tlr:jnl_hora_ult_act
				cds endselect dsc:post:DumpRep_ctx
			else 
				FPRINT common:csr_string,"Busqueda de valores máximos fallida\n"
	    		CALL MISCgeneraLog(common:csr_string,"AMBOS","N")
				cds endselect dsc:post:DumpRep_ctx
				return (-2)
			end if			
		end if
		FPRINT common:csr_string,"Hora Máxima: %s\n", tlr:jnl_hora_ult_act
   		CALL MISCgeneraLog(common:csr_string,"INFO","N")
   	end if
END SUB

/*******/
