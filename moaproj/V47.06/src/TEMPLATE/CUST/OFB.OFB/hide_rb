/****o* Pendientes de agrupación/OFB:template:hide_rb
* NOMBRE
*   OFB:template:hide_rb
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "custOFB.OFB.h"

	SUB template:hide_rb

				/* Set high_idx to the highest field array index number
	and this rtn will turn off the radio buttons not
	being used based on max_members
	*/

	let memctr = max_members + 1
	DO
		if (memctr > high_idx)
			break
		END IF
		SELECT (memctr)
			case 1
				hidecontrol TDrb1
				break
			case 2
				hidecontrol TDrb2
				break
			case 3
				hidecontrol TDrb3
				break
			case 4
				hidecontrol TDrb4
				break
			case 5
				hidecontrol TDrb5
				break
			case 6
				hidecontrol TDrb6
				break
			case 7
				hidecontrol TDrb7
				break
			case 8
				hidecontrol TDrb8
				break
			case 9
				hidecontrol TDrb9
				break
			case 10
				hidecontrol TDrb10
				break
			case 11
				hidecontrol TDrb11
				break
			case 12
				hidecontrol TDrb12
				break
			case 13
				hidecontrol TDrb13
				break
			case 14
				hidecontrol TDrb14
				break
			case 15
				hidecontrol TDrb15
				break
			case 16
				hidecontrol TDrb16
				break
			case 17
				hidecontrol TDrb17
				break
			case 18
				hidecontrol TDrb18
				break
			case 19
				hidecontrol TDrb19
				break
			case 20
				hidecontrol TDrb20
				break
			case 21
				hidecontrol TDrb21
				break
			case 22
				hidecontrol TDrb22
				break
			case 23
				hidecontrol TDrb23
				break
			case 24
				hidecontrol TDrb24
				break
			case 25
				hidecontrol TDrb25
				break
			case 26
				hidecontrol TDrb26
				break
			case 27
				hidecontrol TDrb27
				break
			case 28
				hidecontrol TDrb28
				break
			case 29
				hidecontrol TDrb29
				break
			case 30
				hidecontrol TDrb30
				break
		END SELECT
		let memctr = memctr + 1
	LOOP
	END SUB

/*******/
