/****o* Pendientes de agrupación/OFB:template:PBbut_auto_make10
* NOMBRE
*   OFB:template:PBbut_auto_make10
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "DESKTOPKEYS_BUT.OFB.h"

	SUB template:PBbut_auto_make10
	if (currentfield.numchars == 0)
		move "0" to currentfield
	end if
	IF (ms_screen[10] != "0")
		CALL  CSR:parsename ms_screen[10] ms_lstobj ms_lstappl ms_lstname
	ELSE
		BEEP
		RETURN
	END IF
	MOVE "LST" TO ms_lstobj
	IF (ms_dis_data_coll[10] != "M" || \
		ms_dis_data_coll[10] != "P")
		let ms_cnt = BUTTON_LIST + 10
		CALL  csr:listcsr GETMEMBER ms_cnt
		CALL  csr:listcsr GETELEMENT 1 ms_template_obj ms_template_appl ms_template_name wkfld
	ELSE
		MOVE "MENU" TO wkfld
	END IF
	CALL  OFB:auto_make_name

	FPRINT ms_button_list[10],"%s:%s:%s",ms_lstobj,ms_lstappl,ms_lstname
	REFRESH ms_button_list[10]
	NEXTFIELD ms_button_list[10]
	UNGETKEY RT_TAB
	UNGETKEY RT_TAB
	END SUB

/*******/
