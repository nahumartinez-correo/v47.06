	REFRESH ms_dis_req[5]
	REFRESH ms_dis_req[6]
	REFRESH ms_dis_req[7]
	REFRESH ms_dis_req[8]
	REFRESH ms_dis_req[9]
	REFRESH ms_dis_req[10]
	REFRESH ms_dis_req[11]
	REFRESH ms_dis_req[12]
	REFRESH ms_dis_title_flag[0]
	REFRESH ms_dis_title_flag[1]
	REFRESH ms_dis_title_flag[2]
	REFRESH ms_dis_title_flag[3]
	REFRESH ms_dis_title_flag[4]
	REFRESH ms_dis_title_flag[5]
	REFRESH ms_dis_title_flag[6]
	REFRESH ms_dis_title_flag[7]
	REFRESH ms_dis_title_flag[8]
	REFRESH ms_dis_title_flag[9]
	REFRESH ms_dis_title_flag[10]
	REFRESH ms_dis_title_flag[11]
	REFRESH ms_dis_title_flag[12]
