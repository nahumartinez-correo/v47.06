	REFRESH ms_dis_bar[1]
	REFRESH ms_dis_bar[2]
	REFRESH ms_dis_bar[3]
	REFRESH ms_dis_bar[4]
	REFRESH ms_dis_bar[5]
	REFRESH ms_dis_bar[6]
	REFRESH ms_dis_bar[7]
	REFRESH ms_dis_bar[8]
	REFRESH ms_dis_bar[9]
	REFRESH ms_dis_bar[10]
	REFRESH ms_dis_bar[11]
	REFRESH ms_dis_bar[12]
	REFRESH ms_dis_but[0]
	REFRESH ms_dis_but[1]
	REFRESH ms_dis_but[2]
	REFRESH ms_dis_but[3]
	REFRESH ms_dis_but[4]
	REFRESH ms_dis_but[5]
	REFRESH ms_dis_but[6]
	REFRESH ms_dis_but[7]
	REFRESH ms_dis_but[8]
	REFRESH ms_dis_but[9]
	REFRESH ms_dis_but[10]
	REFRESH ms_dis_but[11]
	REFRESH ms_dis_but[12]
	END SUB

