	REFRESH ms_dis_nextscr[11]
	REFRESH ms_dis_nextscr[12]
	REFRESH ms_dis_data_coll[0]
	REFRESH ms_dis_data_coll[1]
	REFRESH ms_dis_data_coll[2]
	REFRESH ms_dis_data_coll[3]
	REFRESH ms_dis_data_coll[4]
	REFRESH ms_dis_data_coll[5]
	REFRESH ms_dis_data_coll[6]
	REFRESH ms_dis_data_coll[7]
	REFRESH ms_dis_data_coll[8]
	REFRESH ms_dis_data_coll[9]
	REFRESH ms_dis_data_coll[10]
	REFRESH ms_dis_data_coll[11]
	REFRESH ms_dis_data_coll[12]
	REFRESH ms_dis_req[0]
	REFRESH ms_dis_req[1]
	REFRESH ms_dis_req[2]
	REFRESH ms_dis_req[3]
