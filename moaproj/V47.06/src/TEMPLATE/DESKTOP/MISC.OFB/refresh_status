/****o* Pendientes de agrupación/OFB:template:refresh_status
* NOMBRE
*   OFB:template:refresh_status
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "DESKTOPMISC.OFB.h"

	SUB template:refresh_status
	IF (TESTFLAG(ms_auto_side_bar,scr0) == 0)
		MOVE " " TO ms_dis_sidebar[0]
	ELSE
		MOVE "X" TO ms_dis_sidebar[0]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr1) == 0)
		MOVE " " TO ms_dis_sidebar[1]
	ELSE
		MOVE "X" TO ms_dis_sidebar[1]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr2) == 0)
		MOVE " " TO ms_dis_sidebar[2]
	ELSE
		MOVE "X" TO ms_dis_sidebar[2]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr3) == 0)
		MOVE " " TO ms_dis_sidebar[3]
	ELSE
		MOVE "X" TO ms_dis_sidebar[3]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr4) == 0)
		MOVE " " TO ms_dis_sidebar[4]
	ELSE
		MOVE "X" TO ms_dis_sidebar[4]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr5) == 0)
		MOVE " " TO ms_dis_sidebar[5]
	ELSE
		MOVE "X" TO ms_dis_sidebar[5]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr6) == 0)
		MOVE " " TO ms_dis_sidebar[6]
	ELSE
		MOVE "X" TO ms_dis_sidebar[6]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr7) == 0)
		MOVE " " TO ms_dis_sidebar[7]
	ELSE
		MOVE "X" TO ms_dis_sidebar[7]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr8) == 0)
		MOVE " " TO ms_dis_sidebar[8]
	ELSE
		MOVE "X" TO ms_dis_sidebar[8]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr9) == 0)
		MOVE " " TO ms_dis_sidebar[9]
	ELSE
		MOVE "X" TO ms_dis_sidebar[9]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr10) == 0)
		MOVE " " TO ms_dis_sidebar[10]
	ELSE
		MOVE "X" TO ms_dis_sidebar[10]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr11) == 0)
		MOVE " " TO ms_dis_sidebar[11]
	ELSE
		MOVE "X" TO ms_dis_sidebar[11]
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr12) == 0)
		MOVE " " TO ms_dis_sidebar[12]
	ELSE
		MOVE "X" TO ms_dis_sidebar[12]
	END IF

	IF (TESTFLAG(ms_auto_next_scr,scr0) == 0)
		MOVE " " TO ms_dis_nextscr[0]
	ELSE
		MOVE "X" TO ms_dis_nextscr[0]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr1) == 0)
		MOVE " " TO ms_dis_nextscr[1]
	ELSE
		MOVE "X" TO ms_dis_nextscr[1]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr2) == 0)
		MOVE " " TO ms_dis_nextscr[2]
	ELSE
		MOVE "X" TO ms_dis_nextscr[2]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr3) == 0)
		MOVE " " TO ms_dis_nextscr[3]
	ELSE
		MOVE "X" TO ms_dis_nextscr[3]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr4) == 0)
		MOVE " " TO ms_dis_nextscr[4]
	ELSE
		MOVE "X" TO ms_dis_nextscr[4]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr5) == 0)
		MOVE " " TO ms_dis_nextscr[5]
	ELSE
		MOVE "X" TO ms_dis_nextscr[5]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr6) == 0)
		MOVE " " TO ms_dis_nextscr[6]
	ELSE
		MOVE "X" TO ms_dis_nextscr[6]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr7) == 0)
		MOVE " " TO ms_dis_nextscr[7]
	ELSE
		MOVE "X" TO ms_dis_nextscr[7]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr8) == 0)
		MOVE " " TO ms_dis_nextscr[8]
	ELSE
		MOVE "X" TO ms_dis_nextscr[8]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr9) == 0)
		MOVE " " TO ms_dis_nextscr[9]
	ELSE
		MOVE "X" TO ms_dis_nextscr[9]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr10) == 0)
		MOVE " " TO ms_dis_nextscr[10]
	ELSE
		MOVE "X" TO ms_dis_nextscr[10]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr11) == 0)
		MOVE " " TO ms_dis_nextscr[11]
	ELSE
		MOVE "X" TO ms_dis_nextscr[11]
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr12) == 0)
		MOVE " " TO ms_dis_nextscr[12]
	ELSE
		MOVE "X" TO ms_dis_nextscr[12]
	END IF


	IF (TESTFLAG(ms_required,scr0) == 0)
		MOVE " " TO ms_dis_req[0]
	ELSE
		MOVE "X" TO ms_dis_req[0]
	END IF
	IF (TESTFLAG(ms_required,scr1) == 0)
		MOVE " " TO ms_dis_req[1]
	ELSE
		MOVE "X" TO ms_dis_req[1]
	END IF
	IF (TESTFLAG(ms_required,scr2) == 0)
		MOVE " " TO ms_dis_req[2]
	ELSE
		MOVE "X" TO ms_dis_req[2]
	END IF
	IF (TESTFLAG(ms_required,scr3) == 0)
		MOVE " " TO ms_dis_req[3]
	ELSE
		MOVE "X" TO ms_dis_req[3]
	END IF
	IF (TESTFLAG(ms_required,scr4) == 0)
		MOVE " " TO ms_dis_req[4]
	ELSE
		MOVE "X" TO ms_dis_req[4]
	END IF
	IF (TESTFLAG(ms_required,scr5) == 0)
		MOVE " " TO ms_dis_req[5]
	ELSE
		MOVE "X" TO ms_dis_req[5]
	END IF
	IF (TESTFLAG(ms_required,scr6) == 0)
		MOVE " " TO ms_dis_req[6]
	ELSE
		MOVE "X" TO ms_dis_req[6]
	END IF
	IF (TESTFLAG(ms_required,scr7) == 0)
		MOVE " " TO ms_dis_req[7]
	ELSE
		MOVE "X" TO ms_dis_req[7]
	END IF
	IF (TESTFLAG(ms_required,scr8) == 0)
		MOVE " " TO ms_dis_req[8]
	ELSE
		MOVE "X" TO ms_dis_req[8]
	END IF
	IF (TESTFLAG(ms_required,scr9) == 0)
		MOVE " " TO ms_dis_req[9]
	ELSE
		MOVE "X" TO ms_dis_req[9]
	END IF
	IF (TESTFLAG(ms_required,scr10) == 0)
		MOVE " " TO ms_dis_req[10]
	ELSE
		MOVE "X" TO ms_dis_req[10]
	END IF
	IF (TESTFLAG(ms_required,scr11) == 0)
		MOVE " " TO ms_dis_req[11]
	ELSE
		MOVE "X" TO ms_dis_req[11]
	END IF
	IF (TESTFLAG(ms_required,scr12) == 0)
		MOVE " " TO ms_dis_req[12]
	ELSE
		MOVE "X" TO ms_dis_req[12]
	END IF

	LET ms_cnt = 0
	DO
		IF (ms_cnt == 13)
			BREAK
		END IF
		IF (ms_title[ms_cnt].NUMCHARS == 0 || \
		    ms_title[ms_cnt] == "NONE")
		    MOVE " " TO ms_dis_title_flag[ms_cnt]
		ELSE
			MOVE "X" TO ms_dis_title_flag[ms_cnt]
		END IF
		LET ms_cnt = ms_cnt + 1
	LOOP

	LET ms_cnt = 0
	DO
		IF (ms_cnt == 13)
			BREAK
		END IF
		IF (ms_side_bar[ms_cnt] == " " || \
		    ms_side_bar[ms_cnt] == "0")
			CLEARFIELD ms_side_bar[ms_cnt]
		END IF
		IF (ms_side_bar[ms_cnt].NUMCHARS == 0)
		    MOVE " " TO ms_dis_bar[ms_cnt]
		ELSE
			MOVE "X" TO ms_dis_bar[ms_cnt]
		END IF
		LET ms_cnt = ms_cnt + 1
	LOOP

	LET ms_cnt = 0
	DO
		IF (ms_cnt == 13)
			BREAK
		END IF
		IF (ms_button_list[ms_cnt].NUMCHARS == 0 || \
		    ms_button_list[ms_cnt] == "0")
		    MOVE " " TO ms_dis_but[ms_cnt]
		ELSE
			MOVE "X" TO ms_dis_but[ms_cnt]
		END IF
		LET ms_cnt = ms_cnt + 1
	LOOP

	REFRESH ms_screen[0]
	REFRESH ms_screen[1]
	REFRESH ms_screen[2]
	REFRESH ms_screen[3]
	REFRESH ms_screen[4]
	REFRESH ms_screen[5]
	REFRESH ms_screen[6]
	REFRESH ms_screen[7]
	REFRESH ms_screen[8]
	REFRESH ms_screen[9]
	REFRESH ms_screen[10]
	REFRESH ms_screen[11]
	REFRESH ms_screen[12]

	REFRESH ms_dis_sidebar[0]
	REFRESH ms_dis_sidebar[1]
	REFRESH ms_dis_sidebar[2]
	REFRESH ms_dis_sidebar[3]
