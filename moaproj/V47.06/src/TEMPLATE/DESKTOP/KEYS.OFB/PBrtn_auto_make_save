/****o* Pendientes de agrupación/OFB:template:PBrtn_auto_make_save
* NOMBRE
*   OFB:template:PBrtn_auto_make_save
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "DESKTOPKEYS.OFB.h"

	SUB template:PBrtn_auto_make_save
	IF (ms_def_name_rtn.NUMCHARS > 0)
		MOVE "OFB" TO ms_lstobj
		MOVE ms_default_appl TO ms_lstappl
		MOVE ms_def_name_rtn TO ms_lstname
	ELSE
		BEEP
		RETURN
	END IF
	MOVE "OFB" TO ms_lstobj
	CALL  csr:listcsr GETMEMBER SAVE
	CALL  csr:listcsr GETELEMENT 1 ms_template_obj ms_template_appl ms_template_name wkfld
	CALL  OFB:auto_make_name

	FPRINT ms_save_rtn,"%s:%s:%s",ms_lstobj,ms_lstappl,ms_lstname
	REFRESH ms_save_rtn
	NEXTFIELD ms_save_rtn
	UNGETKEY RT_TAB
	UNGETKEY RT_TAB
	END SUB

/*******/
