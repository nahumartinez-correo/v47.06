	ELSE
		MOVE "1" TO SUBSTR(ms_auto_sidebar,2,1)
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr3) == 0)
		MOVE "0" TO SUBSTR(ms_auto_sidebar,3,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_sidebar,3,1)
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr4) == 0)
		MOVE "0" TO SUBSTR(ms_auto_sidebar,4,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_sidebar,4,1)
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr5) == 0)
		MOVE "0" TO SUBSTR(ms_auto_sidebar,5,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_sidebar,5,1)
