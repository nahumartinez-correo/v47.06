	END IF
	IF (TESTFLAG(ms_required,scr7) == 0)
		MOVE "0" TO SUBSTR(ms_req,7,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_req,7,1)
	END IF
	IF (TESTFLAG(ms_required,scr8) == 0)
		MOVE "0" TO SUBSTR(ms_req,8,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_req,8,1)
	END IF
	IF (TESTFLAG(ms_required,scr9) == 0)
		MOVE "0" TO SUBSTR(ms_req,9,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_req,9,1)
	END IF
	IF (TESTFLAG(ms_required,scr10) == 0)
		MOVE "0" TO SUBSTR(ms_req,10,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_req,10,1)
	END IF
	IF (TESTFLAG(ms_required,scr11) == 0)
		MOVE "0" TO SUBSTR(ms_req,11,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_req,11,1)
	END IF
	IF (TESTFLAG(ms_required,scr12) == 0)
		MOVE "0" TO SUBSTR(ms_req,12,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_req,12,1)
	END IF

	LET ms_cnt = 0
	DO
		IF (ms_cnt == 13)
			BREAK
		END IF
		IF (ms_dis_data_coll[ms_cnt] == " ")
			MOVE "0" TO ms_dis_data_coll[ms_cnt]
		END IF
		LET ms_cnt = ms_cnt + 1
	LOOP
	FPRINT ms_data_coll,"%s%s%s%s%s", \
	                     ms_dis_data_coll[0], \
	                     ms_dis_data_coll[1], \
	                     ms_dis_data_coll[2], \
	                     ms_dis_data_coll[3], \
	                     ms_dis_data_coll[4]
	FPRINT ms_data_coll,"%s%s%s%s%s%s", \
						 ms_data_coll, \
	                     ms_dis_data_coll[5], \
	                     ms_dis_data_coll[6], \
	                     ms_dis_data_coll[7], \
	                     ms_dis_data_coll[8], \
	                     ms_dis_data_coll[9]
	FPRINT ms_data_coll,"%s%s%s%s", \
						 ms_data_coll, \
	                     ms_dis_data_coll[10], \
	                     ms_dis_data_coll[11], \
	                     ms_dis_data_coll[12]
	FPRINT ms_flags,"%s%s%s%s%s", \
						ms_auto_sidebar, \
						ms_auto_nextscr, \
						ms_data_coll, \
						ms_req, \
						ms_notecard
	END SUB

