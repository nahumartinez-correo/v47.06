#include "DESKTOPSAVE.OFB.h"

	SUB template:save_flags
	IF (TESTFLAG(ms_auto_side_bar,scr0) == 0)
		MOVE "0" TO SUBSTR(ms_auto_sidebar,0,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_sidebar,0,1)
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr1) == 0)
