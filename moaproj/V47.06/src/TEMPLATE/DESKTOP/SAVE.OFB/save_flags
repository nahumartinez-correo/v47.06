		MOVE "0" TO SUBSTR(ms_auto_nextscr,8,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_nextscr,8,1)
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr9) == 0)
		MOVE "0" TO SUBSTR(ms_auto_nextscr,9,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_nextscr,9,1)
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr9) == 0)
		MOVE "0" TO SUBSTR(ms_auto_nextscr,9,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_nextscr,9,1)
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr9) == 0)
		MOVE "0" TO SUBSTR(ms_auto_nextscr,9,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_nextscr,9,1)
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr9) == 0)
		MOVE "0" TO SUBSTR(ms_auto_nextscr,9,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_nextscr,9,1)
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr10) == 0)
		MOVE "0" TO SUBSTR(ms_auto_nextscr,10,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_nextscr,10,1)
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr11) == 0)
