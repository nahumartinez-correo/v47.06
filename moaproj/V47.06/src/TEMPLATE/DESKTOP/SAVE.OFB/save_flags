	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr8) == 0)
		MOVE "0" TO SUBSTR(ms_auto_sidebar,8,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_sidebar,8,1)
	END IF
	IF (TESTFLAG(ms_auto_side_bar,scr9) == 0)
		MOVE "0" TO SUBSTR(ms_auto_sidebar,9,1)
