	IF (TESTFLAG(ms_auto_next_scr,scr0) == 0)
		MOVE "0" TO SUBSTR(ms_auto_nextscr,0,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_nextscr,0,1)
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr1) == 0)
		MOVE "0" TO SUBSTR(ms_auto_nextscr,1,1)
	ELSE
		MOVE "1" TO SUBSTR(ms_auto_nextscr,1,1)
	END IF
	IF (TESTFLAG(ms_auto_next_scr,scr2) == 0)
		MOVE "0" TO SUBSTR(ms_auto_nextscr,2,1)
