/****o* Pendientes de agrupación/OFB:lib:DOCSmake_list
* NOMBRE
*   OFB:lib:DOCSmake_list
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "docsOFB.OFB.h"

	SUB lib:DOCSmake_list
/* This routine finds the next empty set of descriptor and quantity          */
/* fields on the document and fills them.That way when the Document List is  */
/* printed,there are no blank lines in between if not all the documents were */
/* selected.                                                                 */

	SELECT (lib:doc_lst_idx)
		case 1
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc1
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty1
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 2
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc2
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty2
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 3
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc3
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty3
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 4
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc4
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty4
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 5
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc5
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty5
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 6
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc6
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty6
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 7
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc7
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty7
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 8
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc8
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty8
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 9
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc9
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty9
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 10
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc10
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty10
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 11
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc11
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty11
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
		case 12
			move lib:doc_lst_desc_tmp to lib:doc_lst_desc12
			move lib:doc_lst_qty_tmp to lib:doc_lst_qty12
			let lib:doc_lst_idx = lib:doc_lst_idx + 1
			break
	END SELECT
	END SUB

/*******/
