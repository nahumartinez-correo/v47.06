/****o* Pendientes de agrupación/OFB:drv:win_teso_ovr
* NOMBRE
*   OFB:drv:win_teso_ovr
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "DesktopWin.h"

SUB drv:win_teso_ovr

#if CORREO_DEBUG == CORREO_ON
	;if (plataf:current_state == CONFIRMATION) then
	call post:MISClogMenues("drv:win_teso_ovr")
	;endif
#endif

	if (testflag(tlr:state_flags,tran_active)) then
	    errmsg common:errmsg_list,DSC:tlr:ERRtran_act
	    return
	end if
	let gerente = 1 //cgw:tesoro paralelo a gerente
	let drv:return_to_stack = -1
    PDM GETSTATE tesoro,0,menu_state
    IF (menu_state == MOA_CHECKED) THEN
        RETURN
    END IF
    PDM CHECKITEM tesoro,0,MOA_CHECKED
    PDM CHECKITEM gerente,0,MOA_CHECKED    	
    if (drv:dt_app == 2 || drv:dt_app == 3) then  /* En caja o admin */
	    INDrun_funct = LST:ovr:TRAN_override
    else
	    INDrun_funct = LST:ovr:TRAN_override1
    endif
    CALL do_mode
    PDM CHECKITEM tesoro,0,MOA_UNCHECKED
    PDM CHECKITEM gerente,0,MOA_UNCHECKED
END SUB

/*******/
