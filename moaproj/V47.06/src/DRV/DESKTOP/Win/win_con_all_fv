/****o* Pendientes de agrupación/OFB:drv:win_con_all_fv
* NOMBRE
*   OFB:drv:win_con_all_fv
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "DesktopWin.h"

SUB drv:win_con_all_fv

#if CORREO_DEBUG == CORREO_ON
	;if (plataf:current_state == CONFIRMATION) then
	call post:MISClogMenues("drv:win_con_all_fv")
	;endif
#endif
    if (testflag(tlr:state_flags,tran_active)) then
	        errmsg common:errmsg_list,DSC:tlr:ERRtran_act
	        return
    end if
    PDM GETSTATE contador,6,menu_state
    IF (menu_state == MOA_CHECKED) THEN
      RETURN
    END IF
    PDM CHECKITEM contador,6,MOA_CHECKED
    /* las siguientes lineas se comentan para que si se entra a la aplicacion
       con nivel administrativo NO se pueda entrar a funciones de caja */
    ;SETFLAG admin:sav_siaf_flags,cajero
    ;RESETFLAG admin:sav_siaf_flags,retaguardia
    /* */

    INDrun_funct = LST:hcomm:TRANreentry_all_fv
;    new_app = 2
;    CALL but_down_disable
    CALL do_mode
    IF (dt_app == 2) THEN
      button = 64
      CALL but_up_enable
    END IF
    PDM CHECKITEM contador,6,MOA_UNCHECKED
END SUB

/*******/
