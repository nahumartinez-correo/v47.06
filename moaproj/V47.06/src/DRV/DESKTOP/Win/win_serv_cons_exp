/****o* Clientes Medios/OFB:drv:win_serv_cons_exp
* NOMBRE
*   OFB:drv:win_serv_cons_exp
* DESCRIPCION
*   Esta rutina muestra la opcion de Consulta de Piezas/Contenedores y Sacas pendientes de  expedicion del menú de Servicios
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   PDM @servicios
* DOCUMENTACION
*   26634_CRQ Reporte de piezas pendientes a Expedir
* SOURCE
*/
#include "DesktopWin.h"

SUB drv:win_serv_cons_exp

#if CORREO_DEBUG == ON
	;if (plataf:current_state == CONFIRMATION) then
	 call post:MISClogMenues("drv:win_serv_cons_exp")
	;endif
#endif

    if (testflag(tlr:state_flags,tran_active)) then
	    errmsg common:errmsg_list,DSC:tlr:ERRtran_act
	    return
    end if
    PDM GETSTATE expedicion,2,menu_state
    IF (menu_state == MOA_CHECKED) THEN
		RETURN
    END IF
     //NO SE PONE UNA TILDE PORQUE SE GRISA
    // PDM CHECKITEM expedicion,3,MOA_CHECKED
    //PDM CHECKITEM tesoro,12,MOA_CHECKED
    //PDM CHECKITEM gerente,18,MOA_CHECKED
    CALL do_mode
    new_app = 46
    INDrun_funct = LST:post:TRANserv_cons_exp
    
END SUB

/*******/
