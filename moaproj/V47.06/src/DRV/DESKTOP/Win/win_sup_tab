/****o* Pendientes de agrupación/OFB:drv:win_sup_tab
* NOMBRE
*   OFB:drv:win_sup_tab
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "DesktopWin.h"

SUB drv:win_sup_tab

#if CORREO_DEBUG == CORREO_ON
	;if (plataf:current_state == CONFIRMATION) then
	call post:MISClogMenues("drv:win_sup_tab")
	;endif
#endif

    if (testflag(tlr:state_flags,tran_active)) then
	        errmsg common:errmsg_list,DSC:tlr:ERRtran_act
	        return
    end if
    PDM GETSTATE sup_sistema,0,menu_state
    IF (menu_state == MOA_CHECKED) THEN
      RETURN
    END IF
	/*Para evitar inconvenientes, siempre que ingreso al menú de Tabla limpio el flag
	que indica que estoy en un ABM de Autoimpresión - Gabriel Falciola*/
	resetflag post:autoimpresion_flags,abm_autoimpresion
    PDM CHECKITEM sup_sistema,0,MOA_CHECKED
    PDM CHECKITEM gerente,3,MOA_CHECKED
    CALL do_mode
    new_app = 32
    INDrun_funct = LST:admin:TRANwin_tablas
END SUB

/*******/
