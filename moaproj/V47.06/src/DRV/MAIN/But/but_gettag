		CASE SB7
			LET drv:INDbutton_tag = TAG:PBsb7
		BREAK
		CASE SB8
			LET drv:INDbutton_tag = TAG:PBsb8
		BREAK
		CASE SB9
			LET drv:INDbutton_tag = TAG:PBsb9
		BREAK
		CASE ESC
			LET drv:INDbutton_tag = TAG:PBesc
		BREAK
		CASE UB0
			LET drv:INDbutton_tag = TAG:PBub0
		BREAK
		CASE UB1
			LET drv:INDbutton_tag = TAG:PBub1
		BREAK
		CASE UB2
			LET drv:INDbutton_tag = TAG:PBub2
		BREAK
		CASE UB3
			LET drv:INDbutton_tag = TAG:PBub3
		BREAK
		CASE FB0
			LET drv:INDbutton_tag = TAG:PBfb0
		BREAK
		CASE FB1
			LET drv:INDbutton_tag = TAG:PBfb1
