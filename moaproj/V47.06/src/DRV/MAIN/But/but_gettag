		CASE FB4
			LET drv:INDbutton_tag = TAG:PBfb4
		BREAK
		CASE FB5
			LET drv:INDbutton_tag = TAG:PBfb5
		BREAK
		CASE FB6
			LET drv:INDbutton_tag = TAG:PBfb6
		BREAK
		BREAK
		CASE FB7
			LET drv:INDbutton_tag = TAG:PBfb7
		BREAK
		CASE FB8
			LET drv:INDbutton_tag = TAG:PBfb8
		BREAK
		CASE SPB0
			LET drv:INDbutton_tag = TAG:PBspb0
		BREAK
		CASE SPB1
			LET drv:INDbutton_tag = TAG:PBspb1
		BREAK
		CASE SPB2
			LET drv:INDbutton_tag = TAG:PBspb2
		BREAK
		CASE SPB3
			LET drv:INDbutton_tag = TAG:PBspb3
		BREAK
		CASE SPB4
			LET drv:INDbutton_tag = TAG:PBspb4
		BREAK
		CASE SPB5
			LET drv:INDbutton_tag = TAG:PBspb5
		BREAK
		CASE SPB6
			LET drv:INDbutton_tag = TAG:PBspb6
		BREAK
		CASE SPB7
			LET drv:INDbutton_tag = TAG:PBspb7
		BREAK
		CASE TFB0
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb0
		BREAK
		CASE TFB1
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb1
		BREAK
		CASE TFB2
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb2
		BREAK
		CASE TFB3
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb3
		BREAK
		CASE TFB4
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb4
		BREAK
		CASE TFB5
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb5
		BREAK
		CASE TFB6
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb6
		BREAK
		CASE TFB7
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb7
		BREAK
		CASE TFB8
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb8
		BREAK
		CASE TFB9
			if (drv:dt_app == 2 || drv:dt_app == 3) then // si es caja o mercha
				LET drv:INDbutton_tag = TAG:tlr:tlr_fb8b // tag = tecla p' cambiar el modo de búsqueda
			else
				LET drv:INDbutton_tag = TAG:tlr:tlr_fb9
			endif
		BREAK
		CASE TFB10
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb10
		BREAK
		CASE TFB11
			LET drv:INDbutton_tag = TAG:tlr:tlr_fb11
		BREAK
#if BASE_CFIG_CL == 1
		CASE LFB0
			LET drv:INDbutton_tag = TAG:cl:cl_fb0
		BREAK
		CASE LFB1
			LET drv:INDbutton_tag = TAG:cl:cl_fb1
		BREAK
		CASE LFB2
			LET drv:INDbutton_tag = TAG:cl:cl_fb2
		BREAK
		CASE LFB3
			LET drv:INDbutton_tag = TAG:cl:cl_fb3
		BREAK
		CASE LFB4
			LET drv:INDbutton_tag = TAG:cl:cl_fb4
		BREAK
		CASE LFB5
			LET drv:INDbutton_tag = TAG:cl:cl_fb5
		BREAK
		CASE LFB6
			LET drv:INDbutton_tag = TAG:cl:cl_fb6
		BREAK
		CASE LFB7
			LET drv:INDbutton_tag = TAG:cl:cl_fb7
		BREAK
		CASE LFB8
			LET drv:INDbutton_tag = TAG:cl:cl_fb8
		BREAK
		CASE LFB9
			LET drv:INDbutton_tag = TAG:cl:cl_fb9
		BREAK
		CASE LFB10
			LET drv:INDbutton_tag = TAG:cl:cl_fb10
		BREAK
		CASE LFB11
			LET drv:INDbutton_tag = TAG:cl:cl_fb11
		BREAK
#endif
#if BASE_NA_SIDEBAR == 0
#if BASE_CFIG_NA == 1
		CASE NFB0
			LET drv:INDbutton_tag = TAG:na:na_fb0
		BREAK
		CASE NFB1
			LET drv:INDbutton_tag = TAG:na:na_fb1
		BREAK
		CASE NFB2
			LET drv:INDbutton_tag = TAG:na:na_fb2
		BREAK
		CASE NFB3
			LET drv:INDbutton_tag = TAG:na:na_fb3
		BREAK
		CASE NFB4
			LET drv:INDbutton_tag = TAG:na:na_fb4
		BREAK
		CASE NFB5
			LET drv:INDbutton_tag = TAG:na:na_fb5
		BREAK
		CASE NFB6
			LET drv:INDbutton_tag = TAG:na:na_fb6
		BREAK
		CASE NFB7
			LET drv:INDbutton_tag = TAG:na:na_fb7
		BREAK
		CASE NFB8
			LET drv:INDbutton_tag = TAG:na:na_fb8
		BREAK
#endif
#endif
		/* Para el modulo de TESORO */
		CASE LFB0   
			let drv:INDbutton_tag = TAG:tes:tes_fb0
			break
		CASE LFB1   
			let drv:INDbutton_tag = TAG:tes:tes_fb1
			break
		CASE LFB2   
			let drv:INDbutton_tag = TAG:tes:tes_fb2
			break
		CASE LFB3   
			let drv:INDbutton_tag = TAG:tes:tes_fb3
			break
		CASE LFB4   
			let drv:INDbutton_tag = TAG:tes:tes_fb4
			break
		CASE LFB5   
			let drv:INDbutton_tag = TAG:tes:tes_fb5
			break
		CASE LFB6   
			let drv:INDbutton_tag = TAG:tes:tes_fb6
			break
		CASE LFB7   
			let drv:INDbutton_tag = TAG:tes:tes_fb7
			break
		CASE LFB8   
			let drv:INDbutton_tag = TAG:tes:tes_fb8
			break
		CASE LFB9   
			let drv:INDbutton_tag = TAG:tes:tes_fb9
			break
		CASE LFB10   
			let drv:INDbutton_tag = TAG:tes:tes_fb10
			break
		CASE LFB11  
			let drv:INDbutton_tag = TAG:tes:tes_fb11
			break

                /* MLD 1/12/1998 Para el modulo sucursal */	
                /* El resto queda como los de tesoro  */
		CASE SFB3   
			let drv:INDbutton_tag = TAG:tes:suc_fb3
			break
	END SELECT
	
END SUB

