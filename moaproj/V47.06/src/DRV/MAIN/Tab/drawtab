/****o* Pendientes de agrupación/OFB:drv:drawtab
* NOMBRE
*   OFB:drv:drawtab
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "MainTab.h"

SUB drv:drawtab
	SETFCOLOR BLACK
	DRAW LINE 0,2,2,0,1,1,0      /* 2,2 */
	DRAW LINE 103,0,106,3,1,1,0  /* 118 to 119 */

	SETFCOLOR WHITE
	DRAW LINE 0,4,0,21,2,1,0   /* ok */
	DRAW LINE 4,0,101,0,1,2,0
	/* new */
	DRAW LINE 0,3,3,0,1,1,0
	DRAW LINE 2,3,4,1,1,1,0

	DRAW LINE 1,3,3,1,1,1,0
	DRAW LINE 102,0,102,1,1,1,0
	DRAW LINE 105,19,105,20,1,1,0

	SETFCOLOR DARK_GRAY
	DRAW LINE 102,1,104,3,1,1,0
	DRAW LINE 102,2,105,4,1,1,0
	DRAW LINE 103,2,103,1,1,1,0
	DRAW LINE 104,2,104,21,1,1,0
	DRAW LINE 105,3,105,19,1,1,0

	SETFCOLOR BACKGROUND
	draw line 0,0,1,0,1,1,0
	draw line 0,1,0,1,1,1,0
	draw line 104,0,105,0,1,1,0
	draw line 105,1,105,1,1,1,0
END SUB

/*******/
