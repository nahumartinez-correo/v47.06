/****o* Pendientes de agrupación/OFB:drv:drawtab6_6
* NOMBRE
*   OFB:drv:drawtab6_6
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "MainTab.h"

SUB drv:drawtab6_6

	SETFCOLOR WHITE
	DRAW LINE 0,4,0,21,2,1,0   /* ok */
	DRAW LINE 4,0,92,0,1,2,0
	/* new */
	DRAW LINE 0,3,3,0,1,1,0
	DRAW LINE 2,3,4,1,1,1,0

	DRAW LINE 1,3,3,1,1,1,0
	DRAW LINE 92,0,92,1,1,1,0

	SETFCOLOR DARK_GRAY
	DRAW LINE 94,1,96,3,1,1,0
	DRAW LINE 94,2,97,4,1,1,0
	DRAW LINE 95,2,95,1,1,1,0
	DRAW LINE 96,2,96,21,1,1,0
	DRAW LINE 97,3,97,19,1,1,0
	SETFCOLOR BACKGROUND
	draw line 0,0,2,0,1,1,0
	draw line 0,1,1,1,1,1,0
	draw line 94,0,96,0,1,1,0
	draw line 95,1,96,1,1,1,0

	SETFCOLOR BLACK
	DRAW LINE 0,2,2,0,1,1,0      /* 2,2 */
	DRAW LINE 95,0,98,3,1,1,0  /* 118 to 119 */
END SUB

