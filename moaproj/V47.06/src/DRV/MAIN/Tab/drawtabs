/****o* Pendientes de agrupación/OFB:drv:drawtabs
* NOMBRE
*   OFB:drv:drawtabs
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "MainTab.h"

SUB drv:drawtabs
	SETFCOLOR BLACK
	DRAW LINE 0,2,2,0,1,1,0      /* 2,2 */
	DRAW LINE 113,0,116,3,1,1,0  /* 118 to 119 */
	
	SETFCOLOR WHITE
	DRAW LINE 0,4,0,20,2,1,0   /* ok */
	DRAW LINE 4,0,111,0,1,2,0
	DRAW LINE 1,3,3,1,1,1,0
	/* new */
	DRAW LINE 0,3,3,0,1,1,0
	DRAW LINE 2,3,4,1,1,1,0
	DRAW LINE 112,0,112,1,1,1,0
	DRAW LINE 115,19,115,20,1,1,0

	SETFCOLOR DARK_GRAY
	DRAW LINE 112,1,115,3,1,1,0
	DRAW LINE 112,2,115,4,1,1,0
	DRAW LINE 113,2,113,1,1,1,0
	DRAW LINE 114,2,114,21,1,1,0
	DRAW LINE 115,3,115,19,1,1,0
	
	SETFCOLOR BACKGROUND
	draw line 0,0,1,0,1,1,0
	draw line 0,1,0,1,1,1,0
END SUB

/*******/
