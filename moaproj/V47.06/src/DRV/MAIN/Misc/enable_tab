#include "MainMisc.h"

        SUB drv:enable_tab
SELECT (drv:tab_active)
    CASE 0
        ENABLECONTROL drv:PBnb0
        BREAK

    CASE 1
        ENABLECONTROL drv:PBnb1
        BREAK

    CASE 2
        ENABLECONTROL drv:PBnb2
        BREAK

    CASE 3
        ENABLECONTROL drv:PBnb3
        BREAK

    CASE 4
        ENABLECONTROL drv:PBnb4
        BREAK

    CASE 5
        ENABLECONTROL drv:PBnb5
        BREAK

    CASE 6
        ENABLECONTROL drv:PBnb6
        BREAK

    CASE 7
        ENABLECONTROL drv:PBnb7
        BREAK

    CASE 8
        ENABLECONTROL drv:PBnb8
        BREAK

    CASE 9
        ENABLECONTROL drv:PBnb9
        BREAK

    CASE 10
        ENABLECONTROL drv:PBnb10
        BREAK

    CASE 11
        ENABLECONTROL drv:PBnb11
        BREAK

END SELECT
END SUB

