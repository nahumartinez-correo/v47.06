	ENABLECONTROL drv:TB1	
	ENABLECONTROL drv:TB2
	ENABLECONTROL drv:TB3
	ENABLECONTROL drv:TB4
	ENABLECONTROL drv:TB5
	ENABLECONTROL drv:TB6    
	ENABLECONTROL drv:TB7
	ENABLECONTROL drv:TB8
	ENABLECONTROL drv:TB9
	ENABLECONTROL drv:TB10
	ENABLECONTROL drv:TB11
	ENABLECONTROL drv:TB12 
	ENABLECONTROL drv:TB13
	ENABLECONTROL drv:TB14
	ENABLECONTROL drv:TB15 

END SUB

