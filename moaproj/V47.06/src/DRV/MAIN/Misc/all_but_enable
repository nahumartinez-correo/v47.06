/****o* Pendientes de agrupación/OFB:drv:all_but_enable
* NOMBRE
*   OFB:drv:all_but_enable
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "MainMisc.h"

SUB drv:all_but_enable
/*****************************************************
	Esta rutina habilita todos los PB0..PB21
*****************************************************/
	local l_button like drv:button
	l_button = drv:button
	drv:button = 0
    CALL drv:but_enable
    drv:button = 1
    CALL drv:but_enable
    drv:button = 2
    CALL drv:but_enable
    drv:button = 3
    CALL drv:but_enable
    drv:button = 4
    CALL drv:but_enable
    drv:button = 5
    CALL drv:but_enable
    drv:button = 6
    CALL drv:but_enable
    drv:button = 7
    CALL drv:but_enable
    drv:button = 8
    CALL drv:but_enable
    drv:button = 9
    CALL drv:but_enable
    drv:button = 10
    CALL drv:but_enable
    drv:button = 11
    CALL drv:but_enable
    drv:button = 12
    CALL drv:but_enable
    drv:button = 13
    CALL drv:but_enable
    drv:button = 14
    CALL drv:but_enable
    drv:button = 15
    CALL drv:but_enable
    drv:button = 16
    CALL drv:but_enable
    drv:button = 17
    CALL drv:but_enable
    drv:button = 18
    CALL drv:but_enable
    drv:button = 19
    CALL drv:but_enable
    drv:button = 20
    CALL drv:but_enable
    drv:button = 21
    CALL drv:but_enable
    drv:button = l_button
END SUB

/*******/
