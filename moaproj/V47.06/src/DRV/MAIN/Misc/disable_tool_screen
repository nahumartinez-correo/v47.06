/****o* Pendientes de agrupación/OFB:drv:disable_tool_screen
* NOMBRE
*   OFB:drv:disable_tool_screen
* DESCRIPCION
*   Deshabilita los botones de la pantalla drv:desktop_toolbar
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*		drv:start_tran
*   tlr:PDISjscn
*	  tlr:PDISjscn_nuevo
* DOCUMENTACION
*   PS10699
* SOURCE
*/
#include "MainMisc.h"

SUB drv:disable_tool_screen

	DISABLECONTROL drv:TB1
	DISABLECONTROL drv:TB2
	DISABLECONTROL drv:TB3
	DISABLECONTROL drv:TB4
	DISABLECONTROL drv:TB5
	DISABLECONTROL drv:TB6   	
	DISABLECONTROL drv:TB7
	DISABLECONTROL drv:TB8
	DISABLECONTROL drv:TB9
	DISABLECONTROL drv:TB10
	DISABLECONTROL drv:TB11
	DISABLECONTROL drv:TB12 
	DISABLECONTROL drv:TB13
	DISABLECONTROL drv:TB14
	DISABLECONTROL drv:TB15 

END SUB

