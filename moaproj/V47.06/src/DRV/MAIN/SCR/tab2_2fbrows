#include "MainSCR.h"

SCREEN drv:tab2_2fbrows,216,69,106,22,0,1,14,5,5,5
    CALL drv:drawtab
    SETFCOLOR 0x01 
    SETFONT "Helvetica"
    SETFACE 0x0000 
    SETSIZE 10
    DRAW  MLTEXT 5,3,88,14 0,13 FLD:drv:tabtitle[2]
    SETFONT "Zapf Dingbats"
    SETFCOLOR 0x10 
    DRAW  FIELD 91,13,drv:drv_chk[2]
END SCREEN 

