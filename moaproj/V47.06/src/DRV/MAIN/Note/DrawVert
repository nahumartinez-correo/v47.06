/****o* Pendientes de agrupación/OFB:drv:DrawVert
* NOMBRE
*   OFB:drv:DrawVert
* DESCRIPCION
*   Comportamiento del objeto (obligatorio)
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   Objetos que llaman al objeto documentado (obligatorio)
* DOCUMENTACION
*   Documentación relevante que deberá ser actualizada por la modificación de la rutina
* SOURCE
*/
#include "MainNote.h"

sub drv:DrawVert(Psize)
	if ( Psize == "4" )
		if ( tabtitle[0].numchars > 0  )
			draw line   1 8   1 21 1 1 0
			draw line 108 8 108 21 1 1 0
		end if
		if ( tabtitle[1].numchars > 0 )
			draw line 215 8 215 21 1 1 0
		end if
		if ( tabtitle[2].numchars > 0 )
			draw line 322 8 322 21 1 1 0
		end if
		if ( tabtitle[3].numchars > 0 )
			draw line 429 8 429 21 1 1 0
		end if
	else
		if ( tabtitle[0].numchars > 0  )
			draw line   1 8   1 21 1 1 0
			draw line 100 8 100 21 1 1 0
		end if
		if ( tabtitle[1].numchars > 0 )
			draw line 198 8 198 21 1 1 0
		end if
		if ( tabtitle[2].numchars > 0 )
			draw line 296 8 296 21 1 1 0
		end if
		if ( tabtitle[3].numchars > 0 )
			draw line 394 8 394 21 1 1 0
		end if
		if ( tabtitle[4].numchars > 0 )
			draw line 491 8 491 21 1 1 0
		end if
		if ( tabtitle[5].numchars > 0 )
			draw line 590 8 590 21 1 1 0
		end if
	end if
end sub

/*******/
